  <form:scripts>

button_click:function($event){
	if($params.contactRespons==null){
		Messenger.post({message:&quot;请选择职责岗位！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	if($params.contactName==undefined||$params.contactName.length==0){
		Messenger.post({message:&quot;姓名未填写！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	if($params.contactPhone==undefined||$params.contactPhone.length==0){
		Messenger.post({message:&quot;联系电话未填写！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}	
	if($params.contactQq==undefined||$params.contactQq.length==0){
		Messenger.post({message:&quot;QQ未填写！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}	
	if($params.contactEmail==undefined||$params.contactEmail.length==0){
		Messenger.post({message:&quot;E-mail未填写！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}	
	Modal.instance.close($params);	
},

button1_click:function($event){
	Modal.instance.close();
},

page_onload:function(){
	$view.mainForm.clean();	
}</form:scripts>
