  <form:scripts>

add_click:function($event){
	var selArr = $view.listPayPeriod.getTableData().contents;
	var param = {&quot;idList&quot;:selArr};
	Modal.open(&quot;f/payPeriodAdd&quot;,param,function($params){
			$view.listPayPeriod.refresh();
		});
},

edit_click:function($event){
	var selArr=$view.listPayPeriod.getSelectDatas();
	if ( selArr.length == 0 )
	{
		Messenger.post({type:'error',message:'请勾选记录!'});
		return false;
	}
	if(selArr.length>1)
	{
		Messenger.post({type:'error',message:'请勾选单条记录!'});
		return false;
	}
	
	var ob =$view.listPayPeriod.getSingleSelectData();
	var tb = $view.listPayPeriod.getTableData().contents;
		Modal.open(&quot;f/payPeriodEdit&quot;,{&quot;idList&quot;:ob,&quot;ListTe&quot;:tb},function(){
			$view.listPayPeriod.refresh();
		});
},

active_click:function($event){
	var selArr=$view.listPayPeriod.getSelectDatas();
	if ( selArr.length == 0 )
	{
		Messenger.post({type:'error',message:'请勾选记录!'});
		return false;
	}
/*	if(selArr.length>1)
	{
		Messenger.post({type:'error',message:'请勾选单条记录!'});
		return false;
	}
	if($view.listPayPeriod.getSingleSelectData().active == &quot;禁用&quot;){
		Messenger.post({type:'info',message:'已经禁用，请勿重复操作'});
		return false;
	}
	var ob =$view.listPayPeriod.getSingleSelectData();*/
	
	if(selArr.length!=0){
	var idList=new Array();
	var levelList=new Array();
	var pos=0;
	for(var i=0;i&lt;selArr.length;i++){
		idList[pos]=selArr[i].lineid;
		pos++;
		}
	var param = {&quot;idList&quot;:idList};
		Modal.open(&quot;f/payPeriodProhibit&quot;,param,function($params){
			$view.listPayPeriod.refresh();
		});
	}
},



listPayPeriod_beforeProcessing:function(data,status,xhr){
	for(var i=0;i&lt;data.contents.length;i++){
		if(data.contents[i].active==0){
			data.contents[i].active=&quot;禁用&quot;;
		}
		else if(data.contents[i].active==1){
			data.contents[i].active=&quot;启用&quot;;
		}
	}
},

enable_click:function($event){
	var selArr=$view.listPayPeriod.getSelectDatas();
