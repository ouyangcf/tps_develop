  <form:scripts>

page_onload:function(){
	var myArr=new Array();
/*	for(var i=0;i&lt;$params.lineid.length;i++){
		myArr[i]=Number($params.lineid[i]);
	}*/
	var myids=window.parent.angular.element(&quot;#indexConScope&quot;).scope().lineids;
	$params.lineid=myids;
	var conditions = { groupOp:&quot;AND&quot;, rules:[{field:&quot;checkheadLineid&quot;,op:&quot;in&quot;,data:$params.lineid}]};
	$view.custom_checkdetail.refresh({filters:conditions});	
	$view.custom_substract.refresh({filters:conditions});	
	$view.custom_deduct.refresh({filters:conditions});
	$view.custom_revise.refresh({filters:conditions});
	for(var i=0;i&lt;$model.items.result.length;i++){
		$view.checkitem.addOptions($model.items.result[i]);
	}
	$view.mainForm.clean();
	var myDate=new Date();
	myDate.setMonth(myDate.getMonth()-1);
	$view.checkYM.clean();
	$params.checkYM=undefined;
	$params.checkYM=myDate.getFullYear()+&quot;-&quot;+(myDate.getMonth()+1);	
	//$view.div5.toggle();
},

checkitem_change:function(){
	var pos=functions.getiIndex();
	$view.checktype.setOptions({});
	for(var i=0;i&lt;$model.types.result[pos].contents.length;i++){
		$view.checktype.addOptions($model.types.result[pos].contents[i]);
	}
	$view.checktype.setDefaultValue();	
},
getiIndex:function(){
	var ipos=0;
	var l=$model.itemsid.result.length;
	for(;ipos&lt;l;ipos++){
		if($params.checkitem==$model.itemsid.result[ipos]){
			break;
		}	
	}
	return ipos;
},

button3_click:function($event){
	$view.mainForm.clean();
	var myDate=new Date();
	myDate.setMonth(myDate.getMonth()-1);
	$view.checkYM.clean();
	$params.checkYM=undefined;
	$params.checkYM=myDate.getFullYear()+&quot;-&quot;+(myDate.getMonth()+1);		
},


button2_click:function($event){
	var rules=new Array();
	var pos=0;	
	if($params.checkitem!=undefined){
		rules[pos]={field:&quot;itemLineid&quot;,op:&quot;eq&quot;,data:$params.checkitem};
		pos++;
	}
	if($params.checktype!=undefined){
		rules[pos]={field:&quot;operateLineid&quot;,op:&quot;eq&quot;,data:$params.checktype};
		pos++;
	}
	if($params.checkYM!=undefined){
		//将范围设置为月初第一天以及月末最后一天
		var myFirstDate=$view.checkYM.getDate();
		var mySecondDate=$view.checkYM.getDate();
		mySecondDate.setMonth(mySecondDate.getMonth()+1);
		mySecondDate.setDate(mySecondDate.getDate()-1);
		mySecondDate.setHours(23,59,59);
		rules[pos]={field:&quot;createTime&quot;,op:&quot;ge&quot;,data:functions.time2Str(myFirstDate)};//大于月初第一天以及小于月末最后一天
		pos++;
		rules[pos]={field:&quot;createTime&quot;,op:&quot;le&quot;,data:functions.time2Str(mySecondDate)};//大于月初第一天以及小于月末最后一天
		pos++;
	}
	if($model.entity.shipperId1==null){		
		var conditions = { groupOp:&quot;AND&quot;, rules:rules};	
		if(pos==0){
			conditions=null;
		}
		$view.custom_checkdetail.refresh({filters:conditions});		
		return;
	}
	else{
		$http.post(&quot;ws/s_getByShipperId&quot;,{&quot;ShipperId&quot;:$model.entity.shipperId1})
			.success(function(data){
				if(data.result.length!=0){
					rules[pos]={field:&quot;lineid&quot;,op:&quot;in&quot;,data:data.result};
					pos++;
				}
				//搜不到的时候应该设置一个永远用不到的数字
				else{
					rules[pos]={field:&quot;lineid&quot;,op:&quot;eq&quot;,data:-1};
					pos++;					
				}
				var conditions = { groupOp:&quot;AND&quot;, rules:rules};	
				if(pos==0){
					conditions=null;
				}
				$view.custom_checkdetail.refresh({filters:conditions});	
				return;											
			});		
	}
},
time2Str:function(mytime){
	Date.prototype.format =function(format)
    {
        var o = {
        &quot;M+&quot; : this.getMonth()+1, //month
&quot;d+&quot; : this.getDate(),    //day
&quot;h+&quot; : this.getHours(),   //hour
&quot;m+&quot; : this.getMinutes(), //minute
&quot;s+&quot; : this.getSeconds(), //second
&quot;q+&quot; : Math.floor((this.getMonth()+3)/3),  //quarter
&quot;S&quot; : this.getMilliseconds() //millisecond
        }
        if(/(y+)/.test(format)) format=format.replace(RegExp.$1,
        (this.getFullYear()+&quot;&quot;).substr(4- RegExp.$1.length));
        for(var k in o)if(new RegExp(&quot;(&quot;+ k +&quot;)&quot;).test(format))
        format = format.replace(RegExp.$1,
        RegExp.$1.length==1? o[k] :
        (&quot;00&quot;+ o[k]).substr((&quot;&quot;+ o[k]).length));
        return format;
    }
	var mystrtime=mytime.format('yyyy-MM-dd hh:mm:ss');
	return mystrtime;
},

custom_checkdetail_beforeProcessing:function(data,status,xhr){
	for(var i=0;i&lt;data.contents.length;i++){
		if(data.contents[i].argueStatus1==0){
			data.contents[i].argueStatus1=&quot;未申诉&quot;;
		}
		else if(data.contents[i].argueStatus1==1){
			data.contents[i].argueStatus1=&quot;已申诉&quot;;
		}
		else{
			data.contents[i].argueStatus1=&quot;过期未申诉&quot;;
		}
	}	
	if(data.contents.length==0){
		Messenger.post({message:&quot;当前条件没有找到相应数据!&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});	
	}
	else{
		Messenger.post({message:&quot;当前条件共找到&quot;+data.contents.length+&quot;条相应数据!&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});	
	}	
},

custom_revise_beforeProcessing:function(data,status,xhr){
	for(var i=0;i&lt;data.contents.length;i++){
		if(data.contents[i].argueStatus==0){
			data.contents[i].argueStatus=&quot;未申诉&quot;;
		}
		else if(data.contents[i].argueStatus==1){
			data.contents[i].argueStatus=&quot;申诉未处理&quot;;
		}
		else if(data.contents[i].argueStatus==2){
			data.contents[i].argueStatus=&quot;申诉成功&quot;;
		}
		else if(data.contents[i].argueStatus==3){
			data.contents[i].argueStatus=&quot;申诉不成功&quot;;
		}
		else if(data.contents[i].argueStatus==4){
			data.contents[i].argueStatus=&quot;过期未申诉&quot;;
		}
		if(data.contents[i].reviseStatus==0){
			data.contents[i].reviseStatus=&quot;未整改&quot;;
		}
		else if(data.contents[i].reviseStatus==1){
			data.contents[i].reviseStatus=&quot;整改中&quot;;
		}
		else if(data.contents[i].reviseStatus==2){
			data.contents[i].reviseStatus=&quot;已整改&quot;;
		}
		else if(data.contents[i].reviseStatus==3){
			data.contents[i].reviseStatus=&quot;整改成功&quot;;
		}
		else if(data.contents[i].reviseStatus==4){
			data.contents[i].reviseStatus=&quot;整改不成功&quot;;
		}	
		else{
			data.contents[i].reviseStatus=&quot;过期未整改&quot;;
		}	
	}	
},

button_click:function($event){
	$view.div5.toggle();
}</form:scripts>
