  <form:scripts>search_click:function($event){&#xD;
		$view.div2.toggle();&#xD;
	&#xD;
},&#xD;
&#xD;
page_onload:function(){&#xD;
	$view.mainForm.clean();&#xD;
	functions.addmyoption();	&#xD;
	//&#xD;
	var myDate1=new Date();&#xD;
	var myDate2=new Date();&#xD;
	myDate1.setDate(1);&#xD;
	myDate1.setHours(0,0,0);&#xD;
&#xD;
	myDate2.setMonth(myDate1.getMonth()+1);&#xD;
	myDate2.setDate(0);&#xD;
	myDate2.setHours(23,59,59);&#xD;
	var rules=new Array();&#xD;
	var pos=0;	&#xD;
	rules[pos]={field:&quot;createTime&quot;,op:&quot;ge&quot;,data:functions.time2Str(myDate1)};//大于月初第一天以及小于月末最后一天&#xD;
	pos++;&#xD;
	rules[pos]={field:&quot;createTime&quot;,op:&quot;le&quot;,data:functions.time2Str(myDate2)};//大于月初第一天以及小于月末最后一天&#xD;
	pos++;&#xD;
	/*rules[pos]={field:&quot;active&quot;,op:&quot;ne&quot;,data:0};//禁用状态为可用&#xD;
	pos++;*/&#xD;
	var conditions = { groupOp:&quot;AND&quot;, rules:rules};	&#xD;
	if(pos==0){&#xD;
		conditions=null;&#xD;
	}&#xD;
	$view.list.refresh({filters:conditions});	&#xD;
	//&#xD;
},&#xD;
addmyoption:function(){&#xD;
    var firstDate=new Date();&#xD;
	firstDate.setDate(1);&#xD;
	var endDate=new Date(firstDate);&#xD;
	endDate.setMonth(firstDate.getMonth()+1);&#xD;
	endDate.setDate(0);&#xD;
	var aDate=new Date(firstDate).toString(&quot;yyyy-mm-dd&quot;);&#xD;
	var bDate=new Date(endDate).toString(&quot;yyyy-mm-dd&quot;);&#xD;
	$params.createTime=aDate;&#xD;
	$params.createTime2=bDate;&#xD;
	},&#xD;
levelChange_click:function($event){&#xD;
	var selectData=$view.list.getSelectDatas();&#xD;
	if(selectData.length==0||selectData[0]==null)&#xD;
	{&#xD;
		Messenger.post({type:'error',message:'没有选中行'});&#xD;
		return false;&#xD;
	}&#xD;
	var idList=new Array();&#xD;
	var pos=0;&#xD;
	for(var i=0;i&lt;selectData.length;i++){&#xD;
		idList[pos]=selectData[i].lineid;&#xD;
		pos++;&#xD;
		}&#xD;
Modal.open(&quot;f/levelChangeWindow&quot;,{&quot;idList&quot;:idList},function($params){&#xD;
	$view.list.removeParam({},true);&#xD;
});	&#xD;
},&#xD;
submit_click:function($event){&#xD;
	var ob=$view.list.getSelectDatas();&#xD;
		if(ob.length==0)&#xD;
			{&#xD;
			Messenger.post({type:'error',message:'没有选中行'});&#xD;
				return false;&#xD;
			}&#xD;
			&#xD;
		var idList=new Array();&#xD;
		var pos=0;&#xD;
		for(var i=0;i&lt;ob.length;i++)&#xD;
			{ idList[pos]=ob[i].lineid;&#xD;
				pos++;&#xD;
&#xD;
				if(ob[i].approveStatus!=&quot;待提交&quot;)&#xD;
				{Messenger.post({message:&quot;请选择待提交的数据&quot;,type:&quot;error&quot;,hideAfter:5,showCloseButton:true});&#xD;
				return;}&#xD;
			}&#xD;
		Modal.openConfirm({message:&quot;确认提交？&quot;},function(){&#xD;
			Messenger.post({type:'info',message:'正在提交'});&#xD;
			$http.post(&quot;ws/s_setApproveStatus&quot;,{&quot;lineid&quot;:idList})&#xD;
