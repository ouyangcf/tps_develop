  <form:scripts>

button_click:function($event){
	//当初审通过时候
	if($model.entity.auditFlag==3){
		if($model.entity.beginDate==undefined){
		Messenger.post({message:&quot;请填写生效日期！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
		}
		if($model.entity.endDate==undefined){
		Messenger.post({message:&quot;请填写失效日期！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
		}
		if($model.entity.beginDate>$model.entity.endDate){
		Messenger.post({message:&quot;生效日期不能小于失效日期！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
		}
		if($model.entity.auditTool==undefined||$model.entity.auditTool.length==0){
		Messenger.post({message:&quot;请填写沟通方式！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
		}
		if($model.entity.auditMemo==undefined||$model.entity.auditMemo.length==0){
		Messenger.post({message:&quot;请填写审核意见！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
		}
		var strData = $view.beginDate.getValue();
		$model.entity.beginDate.beginDate = strData+&quot;0:0:0&quot;;
		var sttData = $view.endDate.getValue();
		$model.entity.endDate.endDate = sttData+&quot;0:0:0&quot;;						
	}
	//当初审不通过的时候
	if($model.entity.auditFlag==4){
		if($model.entity.auditTool==undefined||$model.entity.auditTool.length==0){
		Messenger.post({message:&quot;请填写沟通方式！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
		}
		if($model.entity.auditMemo==undefined||$model.entity.auditMemo.length==0){
		Messenger.post({message:&quot;请填写审核意见！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
		}			
	}
	Messenger.post({message:&quot;正在提交&quot;,type:&quot;info&quot;,hideAfter:2,showCloseButton:true});
	$model.entity.lineid=$params.lineId.lineId;
	$http.post(&quot;ws/s_updateAuditCompany&quot;,{&quot;data&quot;:$model.entity})
		.success(function(data){
		
	       // Modal.instance.close();
	         Messenger.post({type:'success',message:'数据保存成功'});	
	        
	        if($model.entity.auditFlag==3){ 
	        	AlauditMemo = &quot;您好！您的申请分供方已通过初步认证，合作愉快！&quot;;
		        $http.post(&quot;ws/s_sendAuditMessage&quot;,{&quot;content&quot;:AlauditMemo,&quot;mobilePhone&quot;:$params.lineId.mobileno})
		        .success(function(data){
		        Messenger.post({type:'success',message:'消息推送成功'});
		        })
	        }
	        if($model.entity.auditFlag==4){ 
	        	AllauditMemo = &quot;您好！您的申请分供方因&quot;+ $model.entity.auditMemo +&quot;未通过认证，暂不能进行合作，感谢您的支持！&quot;;
		        $http.post(&quot;ws/s_sendAuditMessage&quot;,{&quot;content&quot;:AllauditMemo,&quot;mobilePhone&quot;:$params.lineId.mobileno})
		        .success(function(data){
		        Messenger.post({type:'success',message:'消息推送成功'});
		        })
	        }
	         
	        Modal.instance.close();				
		})	    
},

auditFlag_clickEvent:function(value){
	if(value==4){
		$view.dataDiv1.hide();
	}
	if(value==3){
		$view.dataDiv1.show();
	}
},

button1_click:function($event){
	Modal.instance.close();
},

page_onload:function(){
	var shippperid=window.parent.angular.element(&quot;#indexConScope&quot;).scope().shippperid;
	$params.lineId = shippperid;
	$params.companyName=$params.lineId.shipperName;
	//$params.cityId=$params.lineId.cityname;
	$model.entity.cityId=$params.lineId.cityname;
	/*$model.entity.cityId=$params.lineId.cityid;
	alert($model.entity.cityId);*/
	$params.legalUser=$params.lineId.legalUser;
	$params.mainContract=$params.lineId.maincontract;
	$params.taxNo=$params.lineId.taxno;
	$params.capital=$params.lineId.capital;
	//$params.assets=$params.lineId.assets;
	$params.contractName=$params.lineId.contractname;
	$params.mobileno=$params.lineId.mobileno;
	$params.contract=$params.lineId.contract;
	$params.carNumber=$params.lineId.carNumber;
	$params.roadLicence=$params.lineId.roadLicence;
	$params.idNumber=$params.lineId.idNumber;
	$view.dataDiv1.hide();
	$model.entity.auditFlag=4;
	var conditions = { groupOp:&quot;AND&quot;, rules:[{field:&quot;joinId&quot;,op:&quot;eq&quot;,data:$params.lineId.joinid},{field:&quot;joinFlag&quot;,op:&quot;eq&quot;,data:1}]};
	$view.cutom_interline.refresh({filters:conditions});
	var conditions2 = { groupOp:&quot;AND&quot;, rules:[{field:&quot;customerId&quot;,op:&quot;eq&quot;,data:$params.lineId.lineid},{field:&quot;flag&quot;,op:&quot;eq&quot;,data:1}]};
	$view.accountlist.refresh({filters:conditions2});
	$view.linkmanliste.refresh({filters:conditions2});
/*	for(var i=0;i&lt;$model.getAuditTools.result.length;i++){
		$view.auditTool1.addOptions($model.getAuditTools.result[i]);
	}*/
},

button2_click:function($event){
	Modal.open(&quot;f/getCompanyFile&quot;,{&quot;info&quot;:$params.lineId.joinid},function(){
	});  		
}</form:scripts>
