  <form:scripts>
page_onload:function(){
	var shippperid=window.parent.angular.element(&quot;#indexConScope&quot;).scope().shippperid;
	$params.shipper_lineid=shippperid;
	$http.post(&quot;ws/s_getinfo&quot;,{&quot;shippperid&quot;:$params.shipper_lineid})
		.success(function(data){ 
		if(data.result!=undefined)
		{
			$params.code=data.result.code;
			$params.shipperName=data.result.name;
			$params.businessType=data.result.businessType;
			$params.taxNo=data.result.taxNo;
			$params.province=data.result.province;
			$params.city=data.result.city;
			$params.address=data.result.regAddress;
			$params.capital=data.result.capital; 
			$params.carNumber=data.result.carNumber;
			var rules=new Array();
			 rules[0]={field:&quot;shipperLineid&quot;,op:&quot;eq&quot;,data:$params.shipper_lineid};
			var filters={ groupOp:&quot;AND&quot;,rules:rules};
            $view.checkDocumentList.refresh({filters:filters});
            
            var trans_rules=new Array();
            trans_rules[0]={field:&quot;lineid&quot;,op:&quot;eq&quot;,data:$params.shipperName};
            var filters={ groupOp:&quot;AND&quot;, rules:trans_rules};
            $view.transportList.refresh({filters:filters}); 
        } 
       
		});

		var link_rules=new Array();
		 link_rules[0]={field:&quot;lineid&quot;,op:&quot;eq&quot;,data:$params.shipper_lineid+&quot;&quot;};
		var filters={ groupOp:&quot;AND&quot;, rules:link_rules};
		$view.linkmanlist.refresh({filters:filters});
		
	var account_rules=new Array();
		 account_rules[0]={field:&quot;lineid&quot;,op:&quot;eq&quot;,data:$params.shipper_lineid+&quot;&quot;};
		var filters={ groupOp:&quot;AND&quot;, rules:account_rules};
		$view.accountlist.refresh({filters:filters});
		
		$http.post(&quot;ws/s_selectInterLine&quot;,{&quot;shipperid&quot;:$params.shipper_lineid})
		.success(function(data){
		var conditions = { groupOp:&quot;AND&quot;, rules:[{field:&quot;joinId&quot;,op:&quot;eq&quot;,data:data.result[1]},{field:&quot;joinFlag&quot;,op:&quot;eq&quot;,data:data.result[0]}]};
		$view.cutom_interline.refresh({filters:conditions});
	});
	
		var conditions = { groupOp:&quot;AND&quot;, rules:[{field:&quot;approveStatus&quot;,op:&quot;eq&quot;,data:3},{field:&quot;shipperLineid&quot;,op:&quot;eq&quot;,data:$params.shipper_lineid+&quot;&quot;}]};
		$view.list.refresh({filters:conditions});
},
checkDocumentList_beforeProcessing:function(data,status,xhr){
	var l=data.contents.length;	
		for(var i=0;i&lt;l;i++){   
		  var obj=data.contents[i].flag;
		  switch(obj){
		  	case &quot;0&quot;:obj=&quot;未生成&quot;;
		 	break;
			case &quot;1&quot;:obj=&quot;已生成&quot;	;
			break;
		  }
		  data.contents[i].flag=obj;
	     }
}</form:scripts>
