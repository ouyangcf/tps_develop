button2_click:function($event){
	var rules=new Array();&#xD;
	var pos=0;	&#xD;
	if($model.entity.shipperId!=null){&#xD;
		rules[pos]={field:&quot;shipperLineid&quot;,op:&quot;eq&quot;,data:$model.entity.shipperId};&#xD;
		pos++;&#xD;
	}	&#xD;
	if($params.checkYM!=undefined){&#xD;
		//将范围设置为月初第一天以及月末最后一天&#xD;
		var myFirstDate=$($view.checkYM.getDom()).val();&#xD;
		var mySecondDate=$($view.checkYM.getDom()).val();&#xD;
		var firstDate=new Date();&#xD;
		var secondDate=new Date();&#xD;
		firstDate.setFullYear(myFirstDate.substring(0,4)*1);&#xD;
		firstDate.setMonth(myFirstDate.substring(5,myFirstDate.length)*1-1);&#xD;
		firstDate.setDate(1);&#xD;
		firstDate.setHours(0,0,0);&#xD;
		secondDate.setFullYear(myFirstDate.substring(0,4)*1);&#xD;
		secondDate.setMonth(myFirstDate.substring(5,myFirstDate.length)*1);&#xD;
		secondDate.setDate(firstDate.getDate()-1);&#xD;
		secondDate.setHours(23,59,59);&#xD;
		rules[pos]={field:&quot;createTime&quot;,op:&quot;ge&quot;,data:functions.time2Str(firstDate)};//大于月初第一天以及小于月末最后一天&#xD;
		pos++;&#xD;
		rules[pos]={field:&quot;createTime&quot;,op:&quot;le&quot;,data:functions.time2Str(secondDate)};//大于月初第一天以及小于月末最后一天&#xD;
		pos++;&#xD;
	}&#xD;
	var conditions = { groupOp:&quot;AND&quot;, rules:rules};	&#xD;
	if(pos==0){&#xD;
		conditions=null;&#xD;
	}&#xD;
	$view.custom_checkHead.refresh({filters:conditions});		
},&#xD;
time2Str:function(mytime){&#xD;
	Date.prototype.format =function(format)&#xD;
    {&#xD;
        var o = {&#xD;
        &quot;M+&quot; : this.getMonth()+1, //month&#xD;
&quot;d+&quot; : this.getDate(),    //day&#xD;
&quot;h+&quot; : this.getHours(),   //hour&#xD;
&quot;m+&quot; : this.getMinutes(), //minute&#xD;
&quot;s+&quot; : this.getSeconds(), //second&#xD;
&quot;q+&quot; : Math.floor((this.getMonth()+3)/3),  //quarter&#xD;
&quot;S&quot; : this.getMilliseconds() //millisecond&#xD;
        }&#xD;
        if(/(y+)/.test(format)) format=format.replace(RegExp.$1,&#xD;
        (this.getFullYear()+&quot;&quot;).substr(4- RegExp.$1.length));&#xD;
        for(var k in o)if(new RegExp(&quot;(&quot;+ k +&quot;)&quot;).test(format))&#xD;
        format = format.replace(RegExp.$1,&#xD;
        RegExp.$1.length==1? o[k] :&#xD;
        (&quot;00&quot;+ o[k]).substr((&quot;&quot;+ o[k]).length));&#xD;
        return format;&#xD;
    }&#xD;
	var mystrtime=mytime.format('yyyy-MM-dd hh:mm:ss');&#xD;
	return mystrtime;&#xD;
},

