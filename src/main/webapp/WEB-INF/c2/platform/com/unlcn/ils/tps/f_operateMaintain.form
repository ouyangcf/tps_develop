  <form:scripts>newTabop:function(url,title){&#xD;
	var a = $(&quot;&lt;a>&quot;);&#xD;
	a.attr('style','display:none');	&#xD;
	a.text(&quot;test&quot;);&#xD;
	a.attr('href','#/template/iframe.jsp?src='+url+'&amp;title='+title);&#xD;
	var w=window.parent.document.getElementsByTagName(&quot;iframe&quot;);&#xD;
	var $w=$(w);&#xD;
	//$('body').append(a);&#xD;
	$w.parent().append(a);&#xD;
	//a.trigger(&quot;click&quot;);&#xD;
	var mo2g = $('&lt;span> &lt;/span>');&#xD;
	//给A标签中的文字添加一个能被jQuery捕获的元素&#xD;
	a.append(mo2g);&#xD;
	//模拟点击A标签中的文字&#xD;
	mo2g.click();	&#xD;
},&#xD;
showmessage:function(url)&#xD;
{&#xD;
	var url=encodeURI(url);&#xD;
	TabOperator.openTab(&quot;template/iframe.jsp?src=&quot;+url);&#xD;
},&#xD;
refresh:function(){&#xD;
	//window.location.reload();&#xD;
	$view.custom_checkrule.refresh();&#xD;
},&#xD;
page_onload:function(){&#xD;
	$(&quot;#cancelbtn&quot;).hide();&#xD;
	$(&quot;#savebtn&quot;).hide();&#xD;
	var rules=new Array();&#xD;
	var pos=0;	&#xD;
	rules[pos]={field:&quot;active&quot;,op:&quot;eq&quot;,data:0};&#xD;
	pos++;	&#xD;
	var conditions = { groupOp:&quot;AND&quot;, rules:rules};	&#xD;
	if(pos==0){&#xD;
		conditions=null;&#xD;
	}&#xD;
	$view.custom_checkrule.refresh({filters:conditions});	&#xD;
},&#xD;
edit:function(){&#xD;
	var selArr=$view.custom_checkrule.getSelectDatas();&#xD;
	if(selArr.length==0){&#xD;
		Messenger.post({message:&quot;请勾选记录!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	&#xD;
		return;&#xD;
	}&#xD;
	if(selArr.length>1){&#xD;
		Messenger.post({message:&quot;请勾选单条记录!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	&#xD;
		return;&#xD;
	}&#xD;
	Modal.open(&quot;f/editOperate_dlg&quot;,{&quot;info&quot;:selArr[0]},function($params){&#xD;
		$view.custom_checkrule.refresh();&#xD;
	});		&#xD;
	return;&#xD;
	$params.rowid=undefined;&#xD;
	$params.rowid=$view.custom_checkrule.getSingleSelectRowId();	&#xD;
	$view.custom_checkrule.editRow($params.rowid,{});&#xD;
	$(&quot;#cancelbtn&quot;).show();&#xD;
	$(&quot;#savebtn&quot;).show();&#xD;
	$(&quot;#editbtn&quot;).hide();&#xD;
},&#xD;
canceledit:function(){&#xD;
	$view.custom_checkrule.restoreRow($params.rowid,{});&#xD;
	$(&quot;#cancelbtn&quot;).hide();&#xD;
	$(&quot;#savebtn&quot;).hide();&#xD;
	$(&quot;#editbtn&quot;).show();&#xD;
},&#xD;
save:function(){&#xD;
	var stroperateName=&quot;#&quot;+$params.rowid+&quot;_operateName&quot;;&#xD;
	var strsubvalue=&quot;#&quot;+$params.rowid+&quot;_subvalue&quot;;&#xD;
	var strsubmoney=&quot;#&quot;+$params.rowid+&quot;_submoney&quot;;&#xD;
	var noperateName=$(stroperateName).val();&#xD;
	var nsubvalue=$(strsubvalue).val();&#xD;
	var nsubmoney=$(strsubmoney).val();&#xD;
	var re0=/^[+-]?([0-9]*\.?[0-9]+|[0-9]+\.?[0-9]*)([eE][+-]?[0-9]+)?$/;&#xD;
	var re1=/^(0|[1-9]\d*)$/;&#xD;
	if(noperateName==undefined||noperateName.length==0){&#xD;
		Messenger.post({message:&quot;请填写考核类型名称!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	&#xD;
		return;&#xD;
	}	&#xD;
	var result=re1.test(nsubvalue);&#xD;
		if(!result){&#xD;
		Messenger.post({message:&quot;每起扣分仅可填写整数!!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	&#xD;
		return;		&#xD;
	}&#xD;
	result=re0.test(nsubmoney);&#xD;
		if(!result){&#xD;
		Messenger.post({message:&quot;考核金额仅可填写整数或小数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	&#xD;
		return;		&#xD;
	}		&#xD;
	$view.custom_checkrule.saveRow($params.rowid,{});	&#xD;
	var info1=$view.custom_checkrule.getRowData($params.rowid);&#xD;
	$model.e_checkrule.lineid=info1.lineid;&#xD;
	$model.e_checkrule.operateName=info1.operateName;&#xD;
	$model.e_checkrule.subvalue=info1.subvalue;&#xD;
	$model.e_checkrule.submoney=info1.submoney;&#xD;
	$model.e_checkrule.itemLineid=info1.itemLineid;&#xD;
	$model.e_checkrule.itemName=info1.itemName;&#xD;
	if(info1.startDate!=undefined){&#xD;
		var str0 = info1.startDate;   &#xD;
		var arr0 = str0.split(&quot;-&quot;);   &#xD;
		var date0 = new Date(arr0[0],parseInt(arr0[1])-1,arr0[2]);	&#xD;
		$model.e_checkrule.startDate=date0.getTime();&#xD;
	}&#xD;
	if(info1.endDate!=undefined){&#xD;
		var str3 = info1.endDate;   &#xD;
		var arr3 = str3.split(&quot;-&quot;);   &#xD;
		var date3 = new Date(arr3[0],parseInt(arr3[1])-1,arr3[2]);	&#xD;
		$model.e_checkrule.endDate=date3.getTime();&#xD;
	}	&#xD;
&#xD;
	Messenger.post({message:&quot;正在提交,请稍后&quot;,type:&quot;info&quot;,hideAfter:2,showCloseButton:true});	&#xD;
/*    $model.e_checkrule.$save(function(){&#xD;
        Messenger.post({type:'success',message:'修改成功'});&#xD;
        setTimeout(&quot;window.location.reload()&quot;,2000);&#xD;
    });*/&#xD;
    $http.post(&quot;ws/s_possessCheckrule&quot;,{&quot;data&quot;:$model.e_checkrule})&#xD;
    	.success(function(data){&#xD;
        Messenger.post({type:'success',message:'修改成功'});&#xD;
        setTimeout(&quot;window.location.reload()&quot;,2000);    		&#xD;
    	});&#xD;
	$(&quot;#cancelbtn&quot;).hide();&#xD;
	$(&quot;#savebtn&quot;).hide();&#xD;
	$(&quot;#editbtn&quot;).show();	&#xD;
},&#xD;
