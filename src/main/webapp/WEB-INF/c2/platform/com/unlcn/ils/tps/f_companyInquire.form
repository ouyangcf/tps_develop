btn_clean_click:function($event){
	$view.mainForm.clean();
},

btn_select_click:function($event){
	if (!$view.mainForm.check())
	{
		Messenger.error('有错误或未填写的信息');
		return;
	}
	Messenger.post({message:&quot;正在查询请稍后&quot;,type:&quot;info&quot;,hideAfter:2,showCloseButton:true});
	var rules=new Array();
	rules[0]={field:&quot;mobileno&quot;,op:&quot;eq&quot;,data:$params.mobileno};
	rules[1]={field:&quot;companyName&quot;,op:&quot;eq&quot;,data:$params.companyName};
	rules[2]={field:&quot;contractName&quot;,op:&quot;eq&quot;,data:$params.contractName};
	var conditions = { groupOp:&quot;AND&quot;, rules:rules};	
		$view.list.refresh({filters:conditions});
},

list_beforeProcessing:function(data,status,xhr){
	for(var i=0;i&lt;data.contents.length;i++){
		if(data.contents[i].checkFlag==0){
			data.contents[i].checkFlag=&quot;待认证&quot;;
		}
		else if(data.contents[i].checkFlag==3){
			data.contents[i].checkFlag=&quot;认证通过&quot;;
		}
		else if(data.contents[i].checkFlag==4){
			data.contents[i].checkFlag=&quot;认证未通过&quot;;
		}
		//{&quot;A&quot;:&quot;5台以下&quot;,&quot;B&quot;:&quot;5-15台&quot;,&quot;C&quot;:&quot;15-50台&quot;,&quot;D&quot;:&quot;50台以上&quot;}
		if(data.contents[i].assets == &quot;A&quot;){
			data.contents[i].assets=&quot;5台以下&quot;
		}else if(data.contents[i].assets == &quot;B&quot;){
			data.contents[i].assets=&quot;5-15台&quot;
		}else if(data.contents[i].assets == &quot;C&quot;){
			data.contents[i].assets=&quot;15-50台&quot;
		}else{
			data.contents[i].assets=&quot;50台以上&quot;
		}
	}
	if(data.contents.length==0){
		Messenger.post({message:&quot;当前条件没有找到相应数据!&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});	
	}
	else{
		Messenger.post({message:&quot;当前条件共找到&quot;+data.contents.length+&quot;条相应数据!&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});	
	}
	
},

