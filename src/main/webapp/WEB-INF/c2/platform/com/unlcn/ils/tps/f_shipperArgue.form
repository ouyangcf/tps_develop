  <form:scripts>refresh:function(){&#xD;
	//window.location.reload();&#xD;
	$view.custom_checkdetail.refresh();	&#xD;
},

page_onload:function(){&#xD;
	$view.div5.toggle();&#xD;
	for(var i=0;i&lt;$model.items.result.length;i++){&#xD;
		$view.checkitem.addOptions($model.items.result[i]);&#xD;
	}&#xD;
	$view.mainForm.clean();&#xD;
	var myDate=new Date();&#xD;
	myDate.setMonth(myDate.getMonth()-1);&#xD;
	$view.checkYM.clean();&#xD;
	$params.checkYM=undefined;&#xD;
	$params.checkYM=myDate.getFullYear()+&quot;-&quot;+(myDate.getMonth()+1);	&#xD;
	//&#xD;
	var sub0=$params.checkYM.substring(0,4);&#xD;
	var sub1=$params.checkYM.substring(5,$params.checkYM.length);&#xD;
	var myDate1=new Date();&#xD;
	var myDate2=new Date();&#xD;
	myDate1.setFullYear(sub0*1);&#xD;
	myDate1.setMonth(sub1*1-1);&#xD;
	myDate1.setDate(1);&#xD;
	myDate1.setHours(0,0,0);&#xD;
	myDate2.setFullYear(sub0*1);&#xD;
	myDate2.setMonth(sub1*1);&#xD;
	myDate2.setDate(1);&#xD;
	myDate2.setDate(myDate2.getDate()-1);&#xD;
	myDate2.setHours(23,59,59);&#xD;
	$http.post(&quot;ws/s_getCheckDetialByShipperId&quot;,{})&#xD;
		.success(function(data){&#xD;
		var rules=new Array();&#xD;
		var pos=0;	&#xD;
		rules[pos]={field:&quot;createTime&quot;,op:&quot;ge&quot;,data:functions.time2Str(myDate1)};//大于月初第一天以及小于月末最后一天&#xD;
		pos++;&#xD;
		rules[pos]={field:&quot;createTime&quot;,op:&quot;le&quot;,data:functions.time2Str(myDate2)};//大于月初第一天以及小于月末最后一天&#xD;
		pos++;	&#xD;
		if(data.result.length!=0){&#xD;
			rules[pos]={field:&quot;lineid&quot;,op:&quot;in&quot;,data:data.result};//大于月初第一天以及小于月末最后一天&#xD;
			pos++;			&#xD;
		}&#xD;
		else{&#xD;
			rules[pos]={field:&quot;lineid&quot;,op:&quot;eq&quot;,data:&quot;-1&quot;};//大于月初第一天以及小于月末最后一天&#xD;
			pos++;				&#xD;
		}&#xD;
		var conditions = { groupOp:&quot;AND&quot;, rules:rules};	&#xD;
		if(pos==0){&#xD;
			conditions=null;&#xD;
		}&#xD;
		$view.custom_checkdetail.refresh({filters:conditions});			&#xD;
		});&#xD;

},

checkitem_change:function(){&#xD;
	if($params.checkitem==undefined){&#xD;
		$params.checktype=undefined;&#xD;
	}
	var pos=functions.getiIndex();&#xD;
	$view.checktype.setOptions({});&#xD;
	for(var i=0;i&lt;$model.types.result[pos].contents.length;i++){&#xD;
		$view.checktype.addOptions($model.types.result[pos].contents[i]);&#xD;
	}&#xD;
	$view.checktype.setDefaultValue();
},&#xD;
getiIndex:function(){&#xD;
	var ipos=0;&#xD;
	var l=$model.itemsid.result.length;&#xD;
	for(;ipos&lt;l;ipos++){&#xD;
		if($params.checkitem==$model.itemsid.result[ipos]){&#xD;
			break;&#xD;
		}	&#xD;
	}&#xD;
	return ipos;&#xD;
},

custom_checkhead_onSelectRow:function(rowid,status,e){&#xD;
	var info=$view.custom_checkhead.getSingleSelectData();&#xD;
	var conditions = { groupOp:&quot;AND&quot;, rules:[{field:&quot;checkheadLineid&quot;,op:&quot;eq&quot;,data:info.lineid}]};	&#xD;
	$view.custom_checkdetail.refresh({filters:conditions});
},

custom_checkhead_beforeProcessing:function(data,status,xhr){&#xD;
		var date=new Date();&#xD;
		var idList=new Array();&#xD;
		var pos=0;
		for(var i=0;i&lt;data.contents.length;i++){&#xD;
		if(data.contents[i].argueStatus==0){&#xD;
			data.contents[i].argueStatus=&quot;未申诉&quot;;&#xD;
		}&#xD;
		else if(data.contents[i].argueStatus==1){&#xD;
			data.contents[i].argueStatus=&quot;部分申诉&quot;;&#xD;
		}&#xD;
		else{&#xD;
			data.contents[i].argueStatus=&quot;已申诉&quot;;&#xD;
		}&#xD;
		if(data.contents[i].collectStatus==0){&#xD;
			data.contents[i].collectStatus=&quot;未汇总&quot;;&#xD;
		}&#xD;
		else{&#xD;
			data.contents[i].collectStatus=&quot;已汇总&quot;;&#xD;
		}&#xD;
		if(data.contents[i].checkStatus==0){&#xD;
			data.contents[i].checkStatus=&quot;未考核&quot;;&#xD;
		}&#xD;
		else{&#xD;
			data.contents[i].checkStatus=&quot;已考核&quot;;&#xD;
