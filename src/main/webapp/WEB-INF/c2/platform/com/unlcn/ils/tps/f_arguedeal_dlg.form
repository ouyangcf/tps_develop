  <form:scripts>

page_onload:function(){
	var conditions = { groupOp:&quot;AND&quot;, rules:[{field:&quot;lineid&quot;,op:&quot;eq&quot;,data:$params.lineid}]};
	$model.e_agrue.lineid=$params.lineid;
	$view.custom_argue.refresh({filters:conditions});	
	var $test=$($view.selectSimple1.getDom());
	$test.parent().parent().hide();		
	functions.reviseLock();
	functions.substractLock();
},

button_click:function($event){
	var e_revise=null;
	var e_substract=null;
	var flag_revice=false;
	var flag_substract=false;
	if($model.e_agrue.argueStatus==undefined){
		Messenger.post({message:&quot;请选择审核结果&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});			
		return;
	}
	if($model.e_agrue.approveMemo==undefined){
		Messenger.post({message:&quot;审核意见未填写&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});			
		return;
	}
	//当决定生成整改单时
	if($params.addRevise==1){
		if($model.e_revise.reviseDescription==undefined||$model.e_revise.reviseDescription.length==0){
		Messenger.post({message:&quot;整改单问题描述未填写!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
		}
		if($model.e_revise.reviseContent==undefined||$model.e_revise.reviseContent.length==0){
		Messenger.post({message:&quot;整改单整改要求未填写!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
		}
		if($model.e_revise.deadDate==undefined){
			Messenger.post({message:&quot;整改单最晚完成日期未填写&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
			return;
		}
		flag_revice=true;
	}
	//当决定生成减免单时
	if($params.addSubtarct==1){
		if($model.e_substract.subMoney==undefined){
		Messenger.post({message:&quot;减免单减免金额未填写！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
		}
		if($model.e_substract.subReson==undefined||$model.e_substract.subReson.length==0){
		Messenger.post({message:&quot;减免单减免原因未填写!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
		}
		flag_substract=true;
	}
	$model.e_agrue.approveStatus=1;
	Messenger.post({message:&quot;正在提交，请稍后&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});	
	$http.post(&quot;ws/s_arguedeal&quot;,{&quot;e_argue&quot;:$model.e_agrue,&quot;e_revise&quot;:flag_revice?$model.e_revise:null,&quot;e_substract&quot;:flag_substract?$model.e_substract:null})
		.success(function(data){
			if(data.result==true){
				Messenger.post({message:&quot;提交完成&quot;,type:&quot;success&quot;,hideAfter:1,showCloseButton:true});	
				Modal.instance.close();
			}
			else{
				Messenger.post({message:&quot;提交失败&quot;,type:&quot;error&quot;,hideAfter:1,showCloseButton:true});	
				Modal.instance.close();
			}
		})
		.error(function(data){
			Messenger.post({message:&quot;post异常&quot;,type:&quot;error&quot;,hideAfter:1,showCloseButton:true});		
			Modal.instance.close();	
		});	
	
},

approveStatus_change:function(){

	if($model.e_agrue.argueStatus==2){
		var $test=$($view.selectSimple1.getDom());
		$test.parent().parent().show();		
	}
	else{
		var $test=$($view.selectSimple1.getDom());
		$test.parent().parent().hide();				
	}
},

button1_click:function($event){
	$http.post(&quot;ws/s_arguedeal&quot;,{&quot;e_argue&quot;:null,&quot;e_revise&quot;:null,&quot;e_substract&quot;:null})
		.success(function(data){})
		.error(function(data){});
},
reviseLock:function(){
	var $reviseDescription=$($view.reviseDescription.getDom());
	var $reviseContent=$($view.reviseContent.getDom());
	var $deadDate=$($view.deadDate.getDom());
	$model.e_revise.reviseDescription=undefined;
	$model.e_revise.reviseContent=undefined;
	$model.e_revise.deadDate=undefined;
	$reviseDescription.attr(&quot;disabled&quot;,&quot;disabled&quot;);
	$reviseContent.attr(&quot;disabled&quot;,&quot;disabled&quot;);
	$deadDate.attr(&quot;disabled&quot;,&quot;disabled&quot;);
},
substractLock:function(){
	var $subMoney=$($view.subMoney.getDom());
	var $subReson=$($view.subReson.getDom());
	$model.e_substract.subMoney=undefined;
	$model.e_substract.subReson=undefined;
	$subMoney.attr(&quot;disabled&quot;,&quot;disabled&quot;);
	$subReson.attr(&quot;disabled&quot;,&quot;disabled&quot;);
},
reviseunLock:function(){
	var $reviseDescription=$($view.reviseDescription.getDom());
	var $reviseContent=$($view.reviseContent.getDom());
	var $deadDate=$($view.deadDate.getDom());
	$reviseDescription.removeAttr(&quot;disabled&quot;);
	$reviseContent.removeAttr(&quot;disabled&quot;);
	$deadDate.removeAttr(&quot;disabled&quot;);
},
substractunLock:function(){
	var $subMoney=$($view.subMoney.getDom());
	var $subReson=$($view.subReson.getDom());
	$subMoney.removeAttr(&quot;disabled&quot;);
	$subReson.removeAttr(&quot;disabled&quot;);
},

selectSimple_change:function(){
	if($params.addRevise==0){
		functions.reviseLock();
	}
	else{
		functions.reviseunLock();		
	}
},

selectSimple1_change:function(){
	if($params.addSubtarct==0){
		functions.substractLock();	
	}
	else{
		functions.substractunLock();	
	}	
},

subMoney_checkAfter:function(checkResult){
	var $money=$($view.subMoney.getDom());
	var money_val=$money.val();
	var re=/^[+-]?([0-9]*\.?[0-9]+|[0-9]+\.?[0-9]*)([eE][+-]?[0-9]+)?$/;
	var result=  re.test($model.e_substract.subMoney);
	if(money_val.length==0)
	return;
	if($.trim(money_val)==''){
	Messenger.post({message:&quot;考核金额仅可填写整数或浮点数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});		
	return;
	}
	if(!result){
		Messenger.post({message:&quot;考核金额仅可填写整数或浮点数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});			
	}	
},

buttonClose_click:function($event){
	//Modal.instance.close();	
	$view.mainForm.clean();
}</form:scripts>
