button2_click:function($event){
	Messenger.post({message:&quot;正在查询请稍后&quot;,type:&quot;info&quot;,hideAfter:2,showCloseButton:true});
	var rules=new Array();
	var pos=0;
	//司机为空，注册城市为空
	if($model.entity.treedriver==null&amp;&amp;$model.entity.treecity==null){
		if($params.selectSimple!=undefined){
			rules[pos]={field:&quot;auditFlag&quot;,op:&quot;eq&quot;,data:$params.selectSimple};
			pos++;

		}
		rules[pos]={field:&quot;joinFlag&quot;,op:&quot;eq&quot;,data:0};
		pos++;
		var conditions = { groupOp:&quot;AND&quot;, rules:rules};	
		if(pos==0){
			conditions=null;
		}
		$view.custom_audit.refresh({filters:conditions});
		return;			
	}
	//司机不为空，注册城市为空
	if($model.entity.treedriver!=null&amp;&amp;$model.entity.treecity==null){
		rules[pos]={field:&quot;joinFlag&quot;,op:&quot;eq&quot;,data:0};
		pos++;		
		rules[pos]={field:&quot;joinId&quot;,op:&quot;eq&quot;,data:$model.entity.treedriver};
		pos++;	
		if($params.selectSimple!=undefined){
			rules[pos]={field:&quot;auditFlag&quot;,op:&quot;eq&quot;,data:$params.selectSimple};
			pos++;
		}
		var conditions = { groupOp:&quot;AND&quot;, rules:rules};	
		if(pos==0){
			conditions=null;
		}
		$view.custom_audit.refresh({filters:conditions});
		return;					
	}
	//司机为空，注册城市不为空
	if($model.entity.treedriver==null&amp;&amp;$model.entity.treecity!=null){
		$http.post(&quot;ws/s_selectByCityId&quot;,{&quot;cityId&quot;:$model.entity.treecity})
			.success(function(data){
				if(data.result.length!=0){
					rules[pos]={field:&quot;joinId&quot;,op:&quot;in&quot;,data:data.result};
					pos++;	
					rules[pos]={field:&quot;joinFlag&quot;,op:&quot;eq&quot;,data:0};
					pos++;						
				}
				else{
					rules[pos]={field:&quot;joinId&quot;,op:&quot;in&quot;,data:&quot;[-1]&quot;};
					pos++;	
					rules[pos]={field:&quot;joinFlag&quot;,op:&quot;eq&quot;,data:0};
					pos++;								
				}
				if($params.selectSimple!=undefined){
					rules[pos]={field:&quot;auditFlag&quot;,op:&quot;eq&quot;,data:$params.selectSimple};
					pos++;
				}				
				var conditions = { groupOp:&quot;AND&quot;, rules:rules};	
				if(pos==0){
					conditions=null;
				}
				$view.custom_audit.refresh({filters:conditions});
				return;					
			});
	}
	//司机不为空，注册城市不为空
	if($model.entity.treedriver!=null&amp;&amp;$model.entity.treecity!=null){
		$http.post(&quot;ws/s_selectByCityId&quot;,{&quot;cityId&quot;:$model.entity.treecity})
			.success(function(data){
				if(data.result.length!=0){
					rules[pos]={field:&quot;joinId&quot;,op:&quot;in&quot;,data:data.result};
					pos++;	
					rules[pos]={field:&quot;joinFlag&quot;,op:&quot;eq&quot;,data:0};
					pos++;						
				}
				else{
					rules[pos]={field:&quot;joinId&quot;,op:&quot;in&quot;,data:&quot;[-1]&quot;};
					pos++;	
					rules[pos]={field:&quot;joinFlag&quot;,op:&quot;eq&quot;,data:0};
					pos++;								
				}
				rules[pos]={field:&quot;joinId&quot;,op:&quot;eq&quot;,data:$model.entity.treedriver};
				pos++;				
				if($params.selectSimple!=undefined){
					rules[pos]={field:&quot;auditFlag&quot;,op:&quot;eq&quot;,data:$params.selectSimple};
					pos++;
				}				
				var conditions = { groupOp:&quot;AND&quot;, rules:rules};	
				if(pos==0){
					conditions=null;
				}
				$view.custom_audit.refresh({filters:conditions});
				return;				
			});		
	}	
},

