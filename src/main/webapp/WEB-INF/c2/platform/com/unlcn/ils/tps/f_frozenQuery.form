  <form:scripts>
refresh:function(){
	//window.location.reload();
	$view.custom_frozendetail.refresh();	
},

custom_frozendetail_beforeProcessing:function(data,status,xhr){
		for(var i=0;i&lt;data.contents.length;i++){
		if(data.contents[i].FrozenStatus==0){
			data.contents[i].FrozenStatus=&quot;已冻结&quot;;
		}
		else if(data.contents[i].FrozenStatus==1){
			data.contents[i].FrozenStatus=&quot;已解冻&quot;;
		}
		if(data.contents[i].flag==0){
			data.contents[i].flag=&quot;未生成&quot;;
		}
		else if(data.contents[i].flag==1){
			data.contents[i].flag=&quot;已生成&quot;;
		}			
	}
	if(data.contents.length==0){
		Messenger.post({message:&quot;当前条件没有找到相应数据!&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});	
	}
	else{
		Messenger.post({message:&quot;当前条件共找到&quot;+data.contents.length+&quot;条相应数据!&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});	
	}
},

page_onload:function(){
	for(var i=0;i&lt;$model.items.result.length;i++){
		$view.checkitem.addOptions($model.items.result[i]);
	}
	$view.mainForm.clean();
	var myDate=new Date();
	myDate.setMonth(myDate.getMonth()-1);
	$view.checkYM.clean();
	$params.checkYM=undefined;
	$params.checkYM=myDate.getFullYear()+&quot;-&quot;+(myDate.getMonth()+1);		
	//
	var sub0=$params.checkYM.substring(0,4);
	var sub1=$params.checkYM.substring(5,$params.checkYM.length);
	var myDate1=new Date();
	var myDate2=new Date();
	myDate1.setFullYear(sub0*1);
	myDate1.setMonth(sub1*1-1);
	myDate1.setDate(1);
	myDate1.setHours(0,0,0);
	myDate2.setFullYear(sub0*1);
	myDate2.setMonth(sub1*1);
	myDate2.setDate(1);
	myDate2.setDate(myDate2.getDate()-1);
	myDate2.setHours(23,59,59);
	var rules=new Array();
	var pos=0;	
	rules[pos]={field:&quot;createTime&quot;,op:&quot;ge&quot;,data:functions.time2Str(myDate1)};//大于月初第一天以及小于月末最后一天
	pos++;
	rules[pos]={field:&quot;createTime&quot;,op:&quot;le&quot;,data:functions.time2Str(myDate2)};//大于月初第一天以及小于月末最后一天
	pos++;	
	var conditions = { groupOp:&quot;AND&quot;, rules:rules};	
	if(pos==0){
		conditions=null;
	}
	$view.custom_frozendetail.refresh({filters:conditions});	
	//	
	$view.div5.toggle();	
},

checkitem_change:function(){
	if($params.checkitem==undefined){
		$params.checktype=undefined;
		return;
	}
	var pos=functions.getiIndex();
	$view.checktype.setOptions({});
	for(var i=0;i&lt;$model.types.result[pos].contents.length;i++){
		$view.checktype.addOptions($model.types.result[pos].contents[i]);
	}
	$view.checktype.setDefaultValue();	
},
getiIndex:function(){
	var ipos=0;
	var l=$model.itemsid.result.length;
	for(;ipos&lt;l;ipos++){
		if($params.checkitem==$model.itemsid.result[ipos]){
			break;
		}	
	}
	return ipos;
},

button3_click:function($event){
	$view.mainForm.clean();
	var myDate=new Date();
	myDate.setMonth(myDate.getMonth()-1);
	$view.checkYM.clean();
	$params.checkYM=undefined;
	$params.checkYM=myDate.getFullYear()+&quot;-&quot;+(myDate.getMonth()+1);		
},

button2_click:function($event){
	var rules=new Array();
	var pos=0;
	if($params.checkitem!=undefined){
		rules[pos]={field:&quot;itemLineid&quot;,op:&quot;eq&quot;,data:$params.checkitem};
		pos++;
	}
	if($params.checktype!=undefined){
		rules[pos]={field:&quot;operateLineid2&quot;,op:&quot;eq&quot;,data:$params.checktype};
		pos++;
	}
	if($params.checkYM!=undefined){
		//将范围设置为月初第一天以及月末最后一天
		var myFirstDate=$($view.checkYM.getDom()).val();
		var mySecondDate=$($view.checkYM.getDom()).val();
		var firstDate=new Date();
		var secondDate=new Date();
		firstDate.setFullYear(myFirstDate.substring(0,4)*1);
		firstDate.setMonth(myFirstDate.substring(5,myFirstDate.length)*1-1);
		firstDate.setDate(1);
		firstDate.setHours(0,0,0);
		secondDate.setFullYear(myFirstDate.substring(0,4)*1);
		secondDate.setMonth(myFirstDate.substring(5,myFirstDate.length)*1);
		secondDate.setDate(firstDate.getDate()-1);
		secondDate.setHours(23,59,59);
		rules[pos]={field:&quot;createTime&quot;,op:&quot;ge&quot;,data:functions.time2Str(firstDate)};//大于月初第一天以及小于月末最后一天
		pos++;
		rules[pos]={field:&quot;createTime&quot;,op:&quot;le&quot;,data:functions.time2Str(secondDate)};//大于月初第一天以及小于月末最后一天
		pos++;
	}
	if($params.checkTag!=undefined){
		rules[pos]={field:&quot;frozenStatus&quot;,op:&quot;eq&quot;,data:$params.checkTag};
		pos++;
	}	
	if($model.entity.shipperId==null){
		var conditions = { groupOp:&quot;AND&quot;, rules:rules};	
		if(pos==0){
			conditions=null;
		}
		$view.custom_frozendetail.refresh({filters:conditions});
		return;		
	}	
	else{
		$http.post(&quot;ws/s_getCheckHeadByShipperId&quot;,{&quot;ShipperId&quot;:$model.entity.shipperId})
			.success(function(data){
				if(data.result.length!=0){
					rules[pos]={field:&quot;checkheadLineid&quot;,op:&quot;in&quot;,data:data.result};
					pos++;
				}
				//搜不到的时候应该设置一个永远用不到的数字
				else{
					rules[pos]={field:&quot;checkheadLineid&quot;,op:&quot;eq&quot;,data:-1};
					pos++;					
				}
				var conditions = { groupOp:&quot;AND&quot;, rules:rules};	
				if(pos==0){
					conditions=null;
				}
				$view.custom_frozendetail.refresh({filters:conditions});	
				return;											
			});			
	}
},
time2Str:function(mytime){
	Date.prototype.format =function(format)
    {
        var o = {
        &quot;M+&quot; : this.getMonth()+1, //month
&quot;d+&quot; : this.getDate(),    //day
&quot;h+&quot; : this.getHours(),   //hour
&quot;m+&quot; : this.getMinutes(), //minute
&quot;s+&quot; : this.getSeconds(), //second
&quot;q+&quot; : Math.floor((this.getMonth()+3)/3),  //quarter
&quot;S&quot; : this.getMilliseconds() //millisecond
        }
        if(/(y+)/.test(format)) format=format.replace(RegExp.$1,
        (this.getFullYear()+&quot;&quot;).substr(4- RegExp.$1.length));
        for(var k in o)if(new RegExp(&quot;(&quot;+ k +&quot;)&quot;).test(format))
        format = format.replace(RegExp.$1,
        RegExp.$1.length==1? o[k] :
        (&quot;00&quot;+ o[k]).substr((&quot;&quot;+ o[k]).length));
        return format;
    }
	var mystrtime=mytime.format('yyyy-MM-dd hh:mm:ss');
	return mystrtime;
},

button72_click:function($event){
	$view.div5.toggle();
},

button78_click:function($event){
