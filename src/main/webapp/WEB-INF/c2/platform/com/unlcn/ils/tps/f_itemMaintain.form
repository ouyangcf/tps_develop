  <form:scripts>refresh:function(){
	//window.location.reload();
	$view.custom_item.refresh();
},
page_onload:function(){
	$(&quot;#cancelbtn&quot;).hide();
	$(&quot;#savebtn&quot;).hide();
	var rules=new Array();
	var pos=0;	
	rules[pos]={field:&quot;active&quot;,op:&quot;eq&quot;,data:0};
	pos++;	
	var conditions = { groupOp:&quot;AND&quot;, rules:rules};	
	if(pos==0){
		conditions=null;
	}
	$view.custom_item.refresh({filters:conditions});	
	debugger;
},
edit:function(){
	var selArr=$view.custom_item.getSelectDatas();
	if(selArr.length==0){
		Messenger.post({message:&quot;请勾选记录!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	if(selArr.length>1){
		Messenger.post({message:&quot;请勾选单条记录!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	Modal.open(&quot;f/editItem_dlg&quot;,{&quot;info&quot;:selArr[0].lineid,&quot;itemName&quot;:selArr[0].itemName,&quot;flag&quot;:selArr[0].flag},function($params){
		$view.custom_item.refresh();
	});	
	return;	
	$params.rowid=undefined;
	$params.rowid=$view.custom_item.getSingleSelectRowId();	
	$view.custom_item.editRow($params.rowid,{});
	$(&quot;#cancelbtn&quot;).show();
	$(&quot;#savebtn&quot;).show();
	$(&quot;#editbtn&quot;).hide();
},
canceledit:function(){
	$view.custom_item.restoreRow($params.rowid,{});
	$(&quot;#cancelbtn&quot;).hide();
	$(&quot;#savebtn&quot;).hide();
	$(&quot;#editbtn&quot;).show();
},
save:function(){
	
	var stritemName=&quot;#&quot;+$params.rowid+&quot;_itemName&quot;;
	var nitemName=$(stritemName).val();
	if(nitemName==undefined||nitemName==0){
		Messenger.post({message:&quot;考核名称未填写!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	Messenger.post({message:&quot;正在提交,请稍后&quot;,type:&quot;info&quot;,hideAfter:2,showCloseButton:true});
	$view.custom_item.saveRow($params.rowid,{});
	var data=$view.custom_item.getRowData($params.rowid);
	$(&quot;#cancelbtn&quot;).hide();
	$(&quot;#savebtn&quot;).hide();
	$(&quot;#editbtn&quot;).show();	
	$model.e_item.itemName=data.itemName;
	$model.e_item.lineid=data.lineid;
    $model.e_item.$save(function(){
        Messenger.post({type:'success',message:'修改成功'});
        $view.custom_item.refresh();
    });	
},
newItem:function(){
	Modal.open(&quot;f/itemMainTain_dlg&quot;,{},function($params){
		$view.custom_item.refresh();
	});		
},


button10_click:function($event){
	functions.newItem();
},

button11_click:function($event){
	functions.edit();
},

button15_click:function($event){
	functions.refresh();
},

custom_item_beforeProcessing:function(data,status,xhr){
	if(data.contents.length==0){
		Messenger.post({message:&quot;当前条件没有找到相应数据!&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});	
	}
	else{
		Messenger.post({message:&quot;当前条件共找到&quot;+data.contents.length+&quot;条相应数据!&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});	
	}
	var l=data.contents.length;
	for(var i=0;i&lt;l;i++){   
		var obj=data.contents[i].flag;
		switch(obj)
		{
		case 1:obj=&quot;绩效考核&quot;	;
		break;
		case 2:obj=&quot;规范考核&quot;	;
		break;
		}
		data.contents[i].flag=obj;
	}
},

delItem_click:function($event){
	var selArr=$view.custom_item.getSelectDatas();
	if(selArr.length==0){
		Messenger.post({message:&quot;请勾选记录!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	Modal.openConfirm({message:&quot;是否确认删除?&quot;},function($params){
		var arr=new Array();
		for(var i=0;i&lt;selArr.length;i++){
			arr[i]=selArr[i].lineid;
		}	
		Messenger.post({message:&quot;正在提交&quot;,type:&quot;info&quot;,hideAfter:2,showCloseButton:true});	
		$http.post(&quot;ws/s_activeItems&quot;,{&quot;idList&quot;:arr})
			.success(function(data){
				Messenger.post({message:&quot;删除成功&quot;,type:&quot;success&quot;,hideAfter:2,showCloseButton:true});	
				var rules=new Array();
				var pos=0;	
				rules[pos]={field:&quot;active&quot;,op:&quot;eq&quot;,data:0};
				pos++;	
				var conditions = { groupOp:&quot;AND&quot;, rules:rules};	
				if(pos==0){
					conditions=null;
				}
				$view.custom_item.refresh({filters:conditions});	
			});		
	});

}</form:scripts>
