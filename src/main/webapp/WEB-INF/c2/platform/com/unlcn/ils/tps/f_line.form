    <form:datasource xsi:type="form:PagingDataSource" id="listdata" name="列表型数据源" elementType="entity:com.unlcn.ils.tps.e_line" pageSize="10"/>
    <form:datasource xsi:type="form:TreeDataSource" id="treecity" name="城市树型数据源" impl="com.unlcn.ils.tps.services.CityService" type="custom"/>
    <form:datasource xsi:type="form:LogicDataSource" id="listprovince" name="省份数据源" logic="rule:com.unlcn.ils.tps.s_getProvince" sn="s_getAllProvince"/>
    <form:datasource xsi:type="form:EntityDataSource" id="edata" name="单实体数据源" entity="entity:com.unlcn.ils.tps.e_line"/>
  </form:datasources>
  <form:scripts>button_click:function($event){
	
	//城市查询
	var acity=new Array();
	var tcity = $view.treecity.getTree();
	nodes= tcity.getCheckedNodes();
	var pos = 0;
	for(var i=0;i&lt;nodes.length;i++)
	{
		if ( nodes[i].isParent ) continue;//非叶子结点
		var selectnodes = nodes[i].id;
		acity[pos++] = {field:&quot;startCityId&quot;,op:&quot;eq&quot;,data:selectnodes};
	}
	
	var rulecity={groupOp:&quot;OR&quot;,rules:acity};
	if (acity.length&lt;=0) rulecity=null;
	
	var conditions = new Array();
	pos = 0;
	if ( rulecity!=null ) conditions[pos++]=rulecity;
	
	//省份查询
	var ruleprovince = new Array();
	var destprovinceid = $params.destprovinceid;
	var linepos = 0;
	ruleprovince[linepos++]={field:&quot;active&quot;,op:&quot;eq&quot;,data:&quot;1&quot;};
	if (isEmptyValue(destprovinceid)!=&quot;&quot;)
		ruleprovince[linepos++]={field:&quot;destProvinceId&quot;,op:&quot;eq&quot;,data:destprovinceid};	
	var lineno=$params.lineno;
	if (isEmptyValue(lineno)==false)
		ruleprovince[linepos++]={field:&quot;lineno&quot;,op:&quot;cn&quot;,data:&quot;%&quot;+lineno+&quot;%&quot;};
	var morecon = null; 	
	
	var objcon = new Object();	
	objcon.groupOp = &quot;AND&quot;;	
	if  (conditions.length>0)
	  objcon.groups=conditions;	
	if  (ruleprovince.length>0)
		objcon.rules=ruleprovince;
	morecon = JSON.stringify(objcon);
	$view.list.refresh({filters:morecon});
	
},

btnadd_click:function($event){
	var param = {&quot;lineid&quot;:&quot;0&quot;};
	Modal.open(&quot;ef/e_line/add&quot;,param,function(){
    	$view.list.refresh();
    });    
},

btnsearch_click:function($event){
	//$view.list.tableButtonActions.expendEven();
	$view.divsearch.toggle();
},

btnrefresh_click:function($event){
	$view.list.refresh();
},

btndelete_click:function($event){
	var ob=$view.list.getSelectDatas();
	var idList=new Array();
	var pos=0;
	if(ob.length==0)
			{
			Messenger.post({type:'error',message:'没有选中行'});
				return false;
			}
	for(var i=0;i&lt;ob.length;i++)
	{
		idList[pos]=ob[i].lineid;
		pos++;
	}
	Modal.openDelConfirm({message:&quot;确认删除？&quot;},function(){
			$http.post(&quot;ws/s_deleteLine&quot;,{&quot;idList&quot;:idList})
				.success(function(data){
				Messenger.post({message:&quot;删除成功&quot;,type:&quot;success&quot;,hideAfter:2,showCloseButton:true});	
				var rules=new Array();
				var pos=0;	
				rules[pos]={field:&quot;active&quot;,op:&quot;eq&quot;,data:1};
				pos++;	
				var conditions = { groupOp:&quot;AND&quot;, rules:rules};	
				if(pos==0){
					conditions=null;
				}
				$view.list.refresh({filters:conditions});	
				})
				.error(function(data){
					Messenger.post({message:&quot;删除失败&quot;,type:&quot;error&quot;,hideAfter:5,showCloseButton:true});
				});		
	})
	
},


btnreset_click:function($event){
	$view.mainForm.clean();
},

page_onload:function(){
	$view.divsearch.toggle();
}</form:scripts>
</form:Form>
