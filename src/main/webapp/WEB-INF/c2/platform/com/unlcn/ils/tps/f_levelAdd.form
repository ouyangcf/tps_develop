  <form:scripts>

add_click:function($event){
	var data=$view.list.getTableData().contents;
	Modal.open(&quot;f/levelAddWindow&quot;,{&quot;data&quot;:data},function($params){
			$view.list.refresh();
		});
},
delete_click:function($event){ 
	var ob=$view.list.getSelectDatas();
	var idList=new Array();
	var pos=0;
	if(ob.length==0)
			{
			Messenger.post({type:'error',message:'没有选中行'});
				return false;
			}
	for(var i=0;i&lt;ob.length;i++)
	{
		idList[pos]=ob[i].lineid;
		pos++;
	}
	Modal.openConfirm({message:&quot;确认删除？&quot;},function(){
			$http.post(&quot;ws/s_deleteLevel&quot;,{&quot;idList&quot;:idList})
				.success(function(data){
					$view.list.refresh();
					Messenger.post({message:&quot;删除成功&quot;,type:&quot;success&quot;,hideAfter:5,showCloseButton:true});
				})
				.error(function(data){
					Messenger.post({message:&quot;删除失败&quot;,type:&quot;error&quot;,hideAfter:5,showCloseButton:true});
				});		
	})
	
	
},

list_beforeProcessing:function(data,status,xhr){
	
	var l=data.contents.length;
					
		 	 for(var i=0;i&lt;l;i++){   
		 	         var obj=data.contents[i].active;
		 	         
		 	         switch(obj)
		 	         {case 0:obj=&quot;禁用&quot;;
		 	         break;
					 case 1:obj=&quot;可用&quot;	;
					 break;
					 
					 }
					
					 data.contents[i].active=obj;
	         }

},

button2_click:function($event){
	var ob=$view.list.getSelectDatas();
	if(ob.length!=0){
	var idList=new Array();
	var levelList=new Array();
	var pos=0;
	for(var i=0;i&lt;ob.length;i++){
		idList[pos]=ob[i].lineid;
		pos++;
		}
		Modal.open(&quot;f/levelActive&quot;,{&quot;idList&quot;:idList},function($params){
			$view.list.refresh();
		});
	}
	else
	{
		Messenger.post({type:'error',message:'请选择一条数据'});
		return;
	}
},


button3_click:function($event){
	var ob=$view.list.getSelectDatas();
	if(ob.length!=0){
	var idList=new Array();
		var pos=0;
		for(var i=0;i&lt;ob.length;i++){
				idList[pos]=ob[i].lineid;
				pos++;
		}
		Messenger.post({type:'info',message:'正在提交'});
		$http.post(&quot;ws/s_enableLevel&quot;,{&quot;idList&quot;:idList})
		.success(function($data){
			Messenger.post({type:'success',message:'修改成功'});
			$view.list.refresh();
			
		})
		
	}
	else
		 {Messenger.post({type:'error',message:'请选择一条数据'});
		 return;}
},

edit_click:function($event){
	var ob=$view.list.getSelectDatas();
	if(ob.length!=1)
	{
		Messenger.post({type:'error',message:'请选择一条数据'});
		return false;
	}
	var DataList=ob[0].lineid;
	Modal.open(&quot;f/quotationDelete&quot;,{&quot;DataList&quot;:DataList},function($params){
	$view.list.refresh();
		});
}</form:scripts>
