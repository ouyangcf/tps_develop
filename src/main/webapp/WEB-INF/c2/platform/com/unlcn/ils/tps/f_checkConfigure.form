  <form:scripts>refresh:function(){&#xD;
	window.location.reload();&#xD;
},

page_onload:function(){&#xD;
	for(var i=$model.configures.result.length-1;i>=0;i--){&#xD;
		$view.selectSimple.addOptions($model.configures.result[i]);&#xD;
	}	&#xD;
	$view.selectSimple.firstOptionSelected();&#xD;
	$http.post(&quot;ws/s_getNewestConfigureid&quot;,{})&#xD;
		.success(function(data){ &#xD;
			var conditions = { groupOp:&quot;AND&quot;, rules:[{field:&quot;configureLineid&quot;,op:&quot;eq&quot;,data:data.result}]};&#xD;
			$view.custom_configureDetails.refresh({filters:conditions});&#xD;
		})&#xD;
		.error(function(data){});
}&#xD;
,&#xD;
contensedit:function(){&#xD;
if($params.configure==undefined){&#xD;
	Messenger.post({message:&quot;当前没有配置表!&quot;,type:&quot;error&quot;,hideAfter:1,showCloseButton:true});	&#xD;
	return;&#xD;
}&#xD;
var selArr=$view.custom_configureDetails.getRowData();&#xD;
var totalweight=0;&#xD;
if(selArr.length!=0){&#xD;
	for(var i=0;i&lt;selArr.length;i++){&#xD;
		totalweight=totalweight+selArr[i].weight*1;&#xD;
	}	&#xD;
}&#xD;
Modal.open(&quot;f/configureDetail_dlg&quot;,{&quot;configureLineid&quot;:$params.configure,&quot;totalweight&quot;:totalweight},function($params){&#xD;
	$view.custom_configureDetails.refresh();	&#xD;
});			&#xD;
},&#xD;
newConfigureHead:function(){&#xD;
Modal.open(&quot;f/newConfigureHead_dlg&quot;,{},function($params){&#xD;
	functions.refresh();&#xD;
});	&#xD;
},&#xD;
edit:function(){&#xD;
	var selArr=$view.custom_configureDetails.getSelectDatas();&#xD;
	if(selArr.length==0){&#xD;
		Messenger.post({message:&quot;请勾选记录!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	&#xD;
		return;&#xD;
	}&#xD;
	if(selArr.length>1){&#xD;
		Messenger.post({message:&quot;请勾选单条记录!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	&#xD;
		return;&#xD;
	}	&#xD;
	var datas=$view.custom_configureDetails.getRowData();&#xD;
	var totalweight=0;&#xD;
	for(var i=0;i&lt;datas.length;i++){&#xD;
		totalweight=totalweight+datas[i].weight*1;&#xD;
	}&#xD;
	Modal.open(&quot;f/editConfigureDetail&quot;,{&quot;info&quot;:selArr[0],&quot;totalweight&quot;:totalweight-selArr[0].weight},function($params){&#xD;
		$view.custom_configureDetails.refresh();&#xD;
	});		&#xD;
},

custom_configureDetails_beforeProcessing:function(data,status,xhr){
	if(data.contents.length==0){&#xD;
		Messenger.post({message:&quot;当前条件没有找到相应数据!&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});	&#xD;
	}&#xD;
	else{&#xD;
		Messenger.post({message:&quot;当前条件共找到&quot;+data.contents.length+&quot;条相应数据!&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});	&#xD;
	}	
},

selectSimple_change:function(){&#xD;
	var rules=new Array();&#xD;
	var pos=0;&#xD;
	if($params.configure!=undefined){&#xD;
		rules[pos]={field:&quot;configureLineid&quot;,op:&quot;eq&quot;,data:$params.configure};//大于月初第一天以及小于月末最后一天&#xD;
		pos++;		&#xD;
	}&#xD;
	var conditions = { groupOp:&quot;AND&quot;, rules:rules};	&#xD;
	if(pos==0){&#xD;
		conditions=null;&#xD;
	}&#xD;
	$view.custom_configureDetails.refresh({filters:conditions});	
},

button10_click:function($event){
	functions.newConfigureHead();
},

button11_click:function($event){
	functions.contensedit();
},

button12_click:function($event){
	functions.edit();
},

button15_click:function($event){
	functions.refresh();
},

button_click:function($event){
	var rules=new Array();&#xD;
	var pos=0;&#xD;
	if($params.configure!=undefined){&#xD;
		rules[pos]={field:&quot;configureLineid&quot;,op:&quot;eq&quot;,data:$params.configure};//大于月初第一天以及小于月末最后一天&#xD;
		pos++;		&#xD;
	}&#xD;
	var conditions = { groupOp:&quot;AND&quot;, rules:rules};	&#xD;
	if(pos==0){&#xD;
		conditions=null;&#xD;
	}&#xD;
	$view.custom_configureDetails1.refresh({filters:conditions});	
},

delConfigure_click:function($event){&#xD;
	Modal.openConfirm({message:&quot;是否删除?&quot;},function($params){&#xD;
		var selArr=$view.custom_configureDetails.getSelectDatas();&#xD;
		if(selArr.length==0){&#xD;
			Messenger.post({message:&quot;请勾选记录!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	&#xD;
			return;&#xD;
		}&#xD;
		var arr=new Array();&#xD;
		for(var i=0;i&lt;selArr.length;i++){&#xD;
			arr[i]=selArr[i].lineid;&#xD;
		}	&#xD;
		Messenger.post({message:&quot;正在提交&quot;,type:&quot;info&quot;,hideAfter:2,showCloseButton:true});	&#xD;
		$http.post(&quot;ws/s_delConfigures&quot;,{&quot;idList&quot;:arr})&#xD;
			.success(function(data){&#xD;
				Messenger.post({message:&quot;删除成功&quot;,type:&quot;success&quot;,hideAfter:2,showCloseButton:true});	&#xD;
				$view.custom_configureDetails.refresh();	&#xD;
			});	&#xD;
	});

},

test_click:function($event){
	console.info($params.configure);
}</form:scripts>
