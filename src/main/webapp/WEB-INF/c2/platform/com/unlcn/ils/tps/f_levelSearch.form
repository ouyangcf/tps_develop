  <form:scripts>

search_click:function($event){
		$view.div.toggle();
	
},
refresh_click:function($event){
	$view.list.refresh();
	$view.mainForm.clean();
},
reset_click:function($event){
	$view.mainForm.clean();
		
},
select_click:function($event){
	var rules=new Array();
	var pos=0;	
	//情况一，五个都没有

	if($params.startCity==null&amp;&amp;$params.destProvince==undefined&amp;&amp;$params.approveStatus==undefined||$params.approveStatus==0&amp;&amp;$params.createTime==null&amp;&amp;$params.createTime2==null){
		if($params.approveStatus==0){
			Messenger.post({type:'info',message:'正在查询'});
			$http.post(&quot;ws/s_getApproveStatusCondition&quot;,{})
			.success(function(data){
				var arr0=data.result;
				if(arr0.length!=0){
					
						rules[pos]={field:&quot;lineid&quot;,op:&quot;in&quot;,data:arr0};
						pos++;
											
				}
		if($params.shipper_name!=null){
		rules[pos]={field:&quot;shipperLineid&quot;,op:&quot;eq&quot;,data:$params.shipper_name};
		pos++;
		}
		if($params.level_name!=null){
			rules[pos]={field:&quot;levelLineid&quot;,op:&quot;eq&quot;,data:$params.level_name};
			pos++;
		}
		/*rules[pos]={field:&quot;active&quot;,op:&quot;ne&quot;,data:0};//禁用状态为可用
			pos++;	*/
		var filters={ groupOp:&quot;AND&quot;,rules:rules};
		if(pos==0){
			filters=null;        
		}
		$view.list.refresh({filters:filters});	
		Messenger.post({type:'success',message:'查询成功'});
		return;		
			});
		}
		if($params.approveStatus==undefined){
				if($params.shipper_name!=null){
		rules[pos]={field:&quot;shipperLineid&quot;,op:&quot;eq&quot;,data:$params.shipper_name};
		pos++;
		}
		if($params.level_name!=null){
			rules[pos]={field:&quot;levelLineid&quot;,op:&quot;eq&quot;,data:$params.level_name};
			pos++;
		}
		/*rules[pos]={field:&quot;active&quot;,op:&quot;ne&quot;,data:0};//禁用状态为可用
			pos++;	*/
		var filters={ groupOp:&quot;AND&quot;,rules:rules};
		if(pos==0){
			filters=null;        
		}
		$view.list.refresh({filters:filters});	
		Messenger.post({type:'success',message:'查询成功'});
		return;
		
		}
		
		
		
	}
	//情况二，五个任一之一都有
	else{
		if($params.createTime!=null)
			var date=$view.selectData.getValue()+&quot; &quot;+&quot;00:00:00&quot;;
		if($params.createTime2!=null)
			var date2=$view.selectData2.getValue()+&quot; &quot;+&quot;23:59:59&quot;;
		Messenger.post({type:'info',message:'正在查询'});
		$http.post(&quot;ws/s_getCondition&quot;,{&quot;startCityID&quot;:$params.startCity!=null?$params.startCity:null,
		&quot;destProvinceId&quot;:$params.destProvince!=undefined?$params.destProvince:null,
		&quot;approveStatus&quot;:($params.approveStatus!=undefined||$params.approveStatus==0)?$params.approveStatus:null,
		&quot;createTime&quot;:$params.createTime!=null?date:null,
		&quot;createTime2&quot;:$params.createTime2!=null?date2:null})
			.success(function(data){
				var arr0=data.result[0];
				var arr1=data.result[1];
				var arr2=data.result[2];
				var arr3=data.result[3];	
				var arr4=data.result[4];
				var rules=new Array();
				var pos=0;
				if(arr0.length!=0){
					if(arr0[0]!=-1){
						rules[pos]={field:&quot;lineid&quot;,op:&quot;in&quot;,data:arr0};
						pos++;						
					}
					else{
						rules[pos]={field:&quot;lineid&quot;,op:&quot;eq&quot;,data:-1};
						pos++;						
					}
				}
				if(arr1.length!=0){
					if(arr1[0]!=-1){
						rules[pos]={field:&quot;lineid&quot;,op:&quot;in&quot;,data:arr1};
						pos++;						
					}
					else{
						rules[pos]={field:&quot;lineid&quot;,op:&quot;eq&quot;,data:-1};
						pos++;						
					}
				}	
				if(arr2.length!=0){
					if(arr2[0]!=-1){
						rules[pos]={field:&quot;lineid&quot;,op:&quot;in&quot;,data:arr2};
						pos++;						
					}
					else{
						rules[pos]={field:&quot;lineid&quot;,op:&quot;eq&quot;,data:-1};
						pos++;						
					}
				}
				if(arr3.length!=0){
					if(arr3[0]!=-1){
						rules[pos]={field:&quot;lineid&quot;,op:&quot;in&quot;,data:arr3};
						pos++;						
					}
					else{
						rules[pos]={field:&quot;lineid&quot;,op:&quot;eq&quot;,data:-1};
						pos++;						
					}
				}
				if(arr4.length!=0){
					if(arr4[0]!=-1){
						rules[pos]={field:&quot;lineid&quot;,op:&quot;in&quot;,data:arr4};
						pos++;						
					}
					else{
						rules[pos]={field:&quot;lineid&quot;,op:&quot;eq&quot;,data:-1};
						pos++;						
					}
				}																			
				if($params.shipper_name!=null){
					rules[pos]={field:&quot;shipperLineid&quot;,op:&quot;eq&quot;,data:$params.shipper_name};
					pos++;
				}
				if($params.level_name!=null){
					rules[pos]={field:&quot;levelLineid&quot;,op:&quot;eq&quot;,data:$params.level_name};
					pos++;
				}
				/*rules[pos]={field:&quot;active&quot;,op:&quot;ne&quot;,data:0};//禁用状态为可用
					pos++;	*/				
				var filters={ groupOp:&quot;AND&quot;,rules:rules};
				if(pos==0){
					filters=null;     
				}
				$view.list.refresh({filters:filters});	
				Messenger.post({type:'success',message:'查询成功'});			
			});		
		return;
	}
},

list_beforeProcessing:function(data,status,xhr){
	var l=data.contents.length;
	for(var i=0;i&lt;l-1;i++)      
	{	
	   for(var j=0;j&lt;l-1-i;j++) 
	   if((data.contents[j].code*1)&lt;(data.contents[j+1].code*1))
	   {
	      var temp=data.contents[j];
	      data.contents[j]=data.contents[j+1]; 
	      data.contents[j+1]=temp;
	   }
	}
		
},

page_onload:function(){
	
	var pos=0;	
	var rules=new Array();
	/*rules[pos]={field:&quot;active&quot;,op:&quot;ne&quot;,data:0};//禁用状态为可用
	pos++;*/
	var conditions = { groupOp:&quot;AND&quot;, rules:rules};	
	if(pos==0){
		conditions=null;
	}
	$view.list.refresh({filters:conditions});
}</form:scripts>
