  <form:scripts>

page_onload:function(){
		for(var i=0;i&lt;$model.items.result.length;i++){
		$view.itemLineid.addOptions($model.items.result[i]);
	}
},
getiIndex:function(){
	var ipos=0;
	var l=$model.itemsid.result.length;
	for(;ipos&lt;l;ipos++){
		if($model.checkdocument.itemLineid==$model.itemsid.result[ipos]){
			break;
		}	
	}
	return ipos;
},
getjIndex(){
	var ipos=functions.getiIndex();
	var jpos=0;
	var l=$model.types.result[ipos].contents.length;
	for(;jpos&lt;l;jpos++){
		if($model.checkdocument.operateLineid==$model.typesid.result[ipos].contents[jpos]){
			break;
		}	
	}
	return jpos;
},
//一集动态加载
itemLineid_change:function(){
	if($model.checkdocument.itemLineid==undefined){
		$model.checkdocument.operateLineid=undefined;
		$model.checkdocument.checkScoreStandard=undefined;
		$model.checkdocument.checkMoneyStandard=undefined;
		return;
	}
	var pos=functions.getiIndex();
	$view.operateLineid.setOptions({});
	for(var i=0;i&lt;$model.types.result[pos].contents.length;i++){
		$view.operateLineid.addOptions($model.types.result[pos].contents[i]);
	}
	$view.operateLineid.setDefaultValue();
	$view.checkScoreStandard.setOptions({});	
	$view.checkScoreStandard.setDefaultValue();
	$view.checkMoneyStandard.setOptions({});	
	$view.checkMoneyStandard.setDefaultValue();
},
//二级动态加载
operateLineid_change:function(){
	if($model.checkdocument.operateLineid==undefined){
		$model.checkdocument.checkScoreStandard=undefined;
		$model.checkdocument.checkMoneyStandard=undefined;
		return;
	}
	var ipos=functions.getiIndex();
	var jpos=functions.getjIndex();
	$view.checkScoreStandard.setOptions({});
	$view.checkScoreStandard.addOptions($model.standardsubvalue.result[ipos].contents[jpos]);
	$view.checkScoreStandard.firstOptionSelected();	
	$view.checkMoneyStandard.setOptions({});
	$view.checkMoneyStandard.addOptions($model.standardmoney.result[ipos].contents[jpos]);
	$view.checkMoneyStandard.firstOptionSelected();	
},

checkScore_checkAfter:function(checkResult){
	var $subvalue=$($view.checkScore.getDom());
	var subvalue=$subvalue.val();
	var re=/^(0|[1-9]\d*)$/;
	var result=  re.test($model.checkdocument.checkScore);
	if(subvalue.length==0)
	return;
	if($.trim(subvalue)==''){
	Messenger.post({message:&quot;每起扣分仅可填写整数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});		
	return;
	}
	if(!result){
		Messenger.post({message:&quot;每起扣分仅可填写整数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});			
	}
},

checkMoney_checkAfter:function(checkResult){
	$params.perCheckMoney=undefined;
	var $money=$($view.checkMoney.getDom());
	var money_val=$money.val();
	var re=/^[+-]?([0-9]*\.?[0-9]+|[0-9]+\.?[0-9]*)([eE][+-]?[0-9]+)?$/;
	var result=  re.test($model.checkdocument.checkMoney);
	if(money_val.length==0)
	return;
	if($.trim(money_val)==''){
	Messenger.post({message:&quot;考核金额仅可填写整数或小数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});		
	return;
	}
	if(!result){
		Messenger.post({message:&quot;考核金额仅可填写整数或小数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});			
	}
	if($model.checkdocument.checkNumber!=undefined){
		$params.perCheckMoney=$model.checkdocument.checkNumber*$model.checkdocument.checkMoney;
	}
},

checkNumber_checkAfter:function(checkResult){
	$params.perCheckMoney=undefined;	
	var $nums=$($view.checkNumber.getDom());
	var num_val=$nums.val();
	var re=/^(0|[1-9]\d*)$/;
	var result=  re.test($model.checkdocument.checkNumber);
	if(num_val.length==0)
	return;
	if($.trim(num_val)==''){
	Messenger.post({message:&quot;发生数量仅可填写整数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});		
	return;
	}
	if(!result){
		Messenger.post({message:&quot;发生数量仅可填写整数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});			
	}
	if($model.checkdocument.checkMoney!=undefined){
		$params.perCheckMoney=$model.checkdocument.checkNumber*$model.checkdocument.checkMoney;
	}	
},
refresh:function(){
	window.location.reload();
},
saveData:function(){
	if($model.checkdocument.shipperLineid==undefined){
		Messenger.post({message:&quot;请选择分供方！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	if(($model.checkdocument.itemOther==undefined||$model.checkdocument.itemOther.length==0)&amp;&amp;$model.checkdocument.itemLineid==undefined){
		Messenger.post({message:&quot;请选择考核项目！考核项目和其它项目两者必有其一有数据&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	if($model.checkdocument.itemOther!=undefined&amp;&amp;$model.checkdocument.itemLineid!=undefined){
		if($model.checkdocument.itemOther.length!=0){
		Messenger.post({message:&quot;考核项目只能填一个&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;		
		}
	}
	if(($model.checkdocument.operateOther==undefined||$model.checkdocument.operateOther.length==0)&amp;&amp;$model.checkdocument.operateLineid==undefined){
		Messenger.post({message:&quot;请选择考核类型！考核类型和其它类型两者必有其一有数据&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	if($model.checkdocument.operateOther!=undefined&amp;&amp;$model.checkdocument.operateLineid!=undefined){
		if($model.checkdocument.operateOther.length!=0){
		Messenger.post({message:&quot;考核类型只能填一个&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;		
		}
	}
	
	if($model.checkdocument.checkScore==undefined){
		Messenger.post({message:&quot;请填写每起扣分！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	if($model.checkdocument.checkNumber==undefined){
		Messenger.post({message:&quot;请填写发生数量！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	if($model.checkdocument.startDate==undefined){
		Messenger.post({message:&quot;请填写项目发生日期！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	else{
		var strtime=$view.startDate.getValue();
		$model.checkdocument.startDate.startDate=strtime+&quot; 0:0:0&quot;;
	}
	
	if($model.checkdocument.checkMoney==undefined){
		Messenger.post({message:&quot;请填写考核金额！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	
	if($model.checkdocument.checkMemo==undefined||$model.checkdocument.checkMemo.length==0){
		Messenger.post({message:&quot;考核说明未填写!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	if($model.checkdocument.itemOther==undefined||$model.checkdocument.itemOther.length==0){
		$model.checkdocument.itemOther=&quot;&quot;;
	}
	if($model.checkdocument.operateOther==undefined||$model.checkdocument.operateOther.length==0){
		$model.checkdocument.operateOther=&quot;&quot;;
	}
	Messenger.post({message:&quot;正在生成考核记录表，请稍后!&quot;,type:&quot;info&quot;,hideAfter:2,showCloseButton:true});	
	$model.checkdocument.$save(function(){
        Messenger.post({type:'success',message:'新增成功'});
        setTimeout(&quot;window.location.reload()&quot;,2000);
    });
},

