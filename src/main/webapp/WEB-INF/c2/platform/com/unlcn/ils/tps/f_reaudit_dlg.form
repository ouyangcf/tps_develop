  <form:scripts>

button_click:function($event){
	if($model.e_audit.beginDate==undefined){
		Messenger.post({message:&quot;有效开始日期未填写!&quot;,type:&quot;error&quot;,hideAfter:1,showCloseButton:true});
		return;
	}
	if($model.e_audit.endDate==undefined){
		Messenger.post({message:&quot;有效结束日期未填写!&quot;,type:&quot;error&quot;,hideAfter:1,showCloseButton:true});
		return;
	}
	var endTime=$view.endDate.getValue();
	var startTime=$view.beginDate.getValue();
	var compare_reusult=functions.duibi(startTime,endTime);	
	if(compare_reusult==false){
		Messenger.post({message:&quot;开始时间大于结束时间!&quot;,type:&quot;error&quot;,hideAfter:1,showCloseButton:true});
		return;	
	}
	var beigintimes=$($view.beginDate.getDom()).val();
	var bg=$view.beginDate.getValue();
	var cur=new Date();
	var str_cur=functions.time2Str(cur);
	var cmbg=functions.duibi(str_cur,beigintimes);	
	if(cmbg==false){
		Messenger.post({message:&quot;开始时间早于当前时间!&quot;,type:&quot;error&quot;,hideAfter:1,showCloseButton:true});
		return;	
	}	
	var endDate=$($view.endDate.getDom()).val();
	var cur=new Date();
	var str_cur=functions.time2Str(cur);
	var compare_reusult=functions.duibi(str_cur,endDate);	
	if(compare_reusult==false){
		Messenger.post({message:&quot;结束时间早于当前时间!&quot;,type:&quot;error&quot;,hideAfter:1,showCloseButton:true});
		return;	
	}		
	$model.e_audit.lineid=$params.info.lineId;
	Messenger.post({message:&quot;正在提交!&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});
	$http.post(&quot;ws/s_auditAgain&quot;,{&quot;data&quot;:$model.e_audit})
		.success(function(data){
			Messenger.post({message:&quot;提交完成&quot;,type:&quot;success&quot;,hideAfter:1,showCloseButton:true});
			Modal.instance.close();		
	});
},

endDate_onChange:function($event){
	if($model.e_audit.endDate==undefined){
		return;
	}
	var endDate=$($view.endDate.getDom()).val();
	var endTime=$view.endDate.getValue();
	//console.info('length='+$model.e_audit.endDate);
	var cur=new Date();
	var str_cur=functions.time2Str(cur);
	var compare_reusult=functions.duibi(str_cur,endDate);	
	if(compare_reusult==false){
		Messenger.post({message:&quot;结束时间早于当前时间!&quot;,type:&quot;error&quot;,hideAfter:1,showCloseButton:true});
		return;	
	}
	if($model.e_audit.beginDate==undefined){
		return;
	}
	var endTime=$view.endDate.getValue();
	var startTime=$view.beginDate.getValue();
	var compare_reusult1=functions.duibi(startTime,endTime);	
	if(compare_reusult1==false){
		Messenger.post({message:&quot;开始时间大于结束时间!&quot;,type:&quot;error&quot;,hideAfter:1,showCloseButton:true});
		return;	
	}	
},

cancel_click:function($event){
	Modal.instance.close();	
},

duibi:function(a,b){
    var arr = a.split(&quot;-&quot;);
    var starttime = new Date(arr[0], arr[1], arr[2]);
    var starttimes = starttime.getTime();
    var arrs = b.split(&quot;-&quot;);
    var lktime = new Date(arrs[0], arrs[1], arrs[2]);
    var lktimes = lktime.getTime();
    if (starttimes >= lktimes) {
        return false;
    }
    else
        return true;

},
time2Str:function(mytime){
	Date.prototype.format =function(format)
    {
        var o = {
        &quot;M+&quot; : this.getMonth()+1, //month
&quot;d+&quot; : this.getDate(),    //day
&quot;q+&quot; : Math.floor((this.getMonth()+3)/3),  //quarter
&quot;S&quot; : this.getMilliseconds() //millisecond
        }
        if(/(y+)/.test(format)) format=format.replace(RegExp.$1,
        (this.getFullYear()+&quot;&quot;).substr(4- RegExp.$1.length));
        for(var k in o)if(new RegExp(&quot;(&quot;+ k +&quot;)&quot;).test(format))
        format = format.replace(RegExp.$1,
        RegExp.$1.length==1? o[k] :
        (&quot;00&quot;+ o[k]).substr((&quot;&quot;+ o[k]).length));
        return format;
    }
	var mystrtime=mytime.format('yyyy-MM-dd');
	return mystrtime;
},

beginDate_onChange:function($event){
	var endDate=$($view.beginDate.getDom()).val();
	var endTime=$view.beginDate.getValue();
	var cur=new Date();
	var str_cur=functions.time2Str(cur);
	var compare_reusult=functions.duibi(str_cur,endDate);	
	if(compare_reusult==false){
		Messenger.post({message:&quot;开始时间早于当前时间!&quot;,type:&quot;error&quot;,hideAfter:1,showCloseButton:true});
		return;	
	}
	if($model.e_audit.endDate==undefined){
		return;
	}
	var endTime=$view.endDate.getValue();
	var startTime=$view.beginDate.getValue();
	var compare_reusult1=functions.duibi(startTime,endTime);	
	if(compare_reusult1==false){
		Messenger.post({message:&quot;开始时间大于结束时间!&quot;,type:&quot;error&quot;,hideAfter:1,showCloseButton:true});
		return;	
	}		
}</form:scripts>
