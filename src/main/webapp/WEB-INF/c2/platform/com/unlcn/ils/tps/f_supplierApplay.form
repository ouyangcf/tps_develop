  <form:scripts>
//保存按钮，对错误信息和未填写的必填信息进行验证
btn_add_click:function($event){
	//alert($params.fileID)
   if (!$view.mainForm.check())
	{
		Messenger.error('有错误或未填写的信息');
		return;
	}
	if($params.fileID==undefined||$params.fileID==null){
		Messenger.error('请上传身份证照片');
		return;
	}
	var idArr=$view.list.getRowData();
	if(idArr.length==0){
		Messenger.post({message:&quot;意向线路至少添加一条以上！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;		
	}
	var accountRoData = $view.accountlist.getRowData();
	if(accountRoData.length==0){
		Messenger.post({message:&quot;账户信息至少添加一条以上！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;		
	}
	var accountList = new Array();
	for(var i in accountRoData ){
		var id = accountRoData[i].lineid;
		var pathid = id+'path';
		accountRoData[i].accountFilepath = $(&quot;#&quot;+pathid).text();
		accountList[i] = accountRoData[i];
	}
	
	var linkmanRowData = $view.linkmanlist.getRowData();
	if(linkmanRowData.length==0){
		Messenger.post({message:&quot;联系人信息至少添加一条以上！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;		
	}
	var linkmanList = new Array();
	for(var i in linkmanRowData){
		linkmanList[i] = linkmanRowData[i];
	}
	
	//线路意向数据整理
	var startCitys=new Array();
	var endCitys=new Array();
	var prices=new Array();
	var qtys=new Array();
	for(var i=0;i&lt;idArr.length;i++){
		startCitys[i]=idArr[i].startCity;
		endCitys[i]=idArr[i].destProvince;
		prices[i]=idArr[i].price;
		qtys[i]=idArr[i].qty;
	}
	var urlString = $params.fileID.url;
	var nameString = $params.fileID.name;
	Modal.openConfirm({message:&quot;是否确定提交该分供方的认证申请？&quot;},function(){
		Messenger.post({message:&quot;正在提交&quot;,type:&quot;info&quot;,hideAfter:5,showCloseButton:true});	
		$http.post(&quot;ws/s_createDriver&quot;,{&quot;data&quot;:$model.entity,
			&quot;startCity&quot;:startCitys,&quot;endCity&quot;:endCitys,
			&quot;price&quot;:prices,&quot;qty&quot;:qtys,
			linkmanList:linkmanList,accountList:accountList,&quot;urlString&quot;:urlString,&quot;nameString&quot;:nameString})
			.success(function(data){
				Messenger.post({type:'success',message:'新增成功'});	
				
				/*Modal.open(&quot;f/driverAttched_dlg&quot;,{&quot;info&quot;:data.result},function(){
			    }); */ 
			    
				$view.mainForm.clean(); 
				var ids=$view.list.getDataIDs();
				for(var i=0;i&lt;ids.length;i++){
					$view.list.delRowData(ids[i]);
				}
				$view.accountlist.clearGridData(true);	
				$view.linkmanlist.clearGridData(true);				
			});
			
    });
},

btn_clean_click:function($event){
	$view.mainForm.clean();
	var ids=$view.list.getDataIDs();
	for(var i=0;i&lt;ids.length;i++){
		$view.list.delRowData(ids[i]);
	}
	$view.accountlist.clearGridData(true);	
	$view.linkmanlist.clearGridData(true);
},

cityId_beforeClick:function(treeId,treeNode,clickFlag){
	var check = (treeNode &amp;&amp; !treeNode.isParent);  
    if (!check)
    {
    	Messenger.error('不能选择省份');
    }
    return check;  
},

