  <form:scripts>
button6_click:function($event){
	var shipper=$params.shipperLineid;
	var check_configure=$params.check_configure;
	var checkCycle=$params.checkCycle;
	var checkDate=$params.checkDate;
	var checkDate1=$params.checkDate1;
	var checkDate2=$params.checkDate2;
	var checkDate3=$params.checkDate3;
	if($params.check_configure==undefined||$params.check_configure.length==0){
		Messenger.post({message:&quot;考核配置表未填写!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	if($params.checkCycle==undefined||$params.checkCycle==0){
		Messenger.post({message:&quot;考核周期未填写!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	if($params.checkCycle==1&amp;&amp;($params.checkDate==undefined||$params.checkDate.length==0)){
		Messenger.post({message:&quot;考核年月未填写!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	if($params.checkCycle==2&amp;&amp;(($params.checkDate1==undefined||$params.checkDate1.length==0)||($params.checkDate2==undefined||$params.checkDate2.length==0))){
		Messenger.post({message:&quot;考核年份或上下半年未填写!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	if($params.checkCycle==3&amp;&amp;($params.checkDate3==undefined||$params.checkDate3.length==0)){
		Messenger.post({message:&quot;考核年份未填写!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}

	$http.post(&quot;ws/s_createCheckDatas&quot;,{&quot;shipper&quot;:shipper,&quot;check_configure&quot;:check_configure,&quot;checkCycle&quot;:checkCycle,&quot;checkDate&quot;:checkDate,&quot;checkDate1&quot;:checkDate1,
	&quot;checkDate2&quot;:checkDate2,&quot;checkDate3&quot;:checkDate3})
	.success(function(){})
},
radioGroup_clickEvent:function(value){
	if(value==1)
	{
		$view.div3.show();
		$view.div5.hide();
		$view.div6.hide();
	}
	if(value==2){
		$view.div3.hide();
		$view.div5.show();
		$view.div6.show();
	}
	if(value==3){
		$view.div3.hide();
