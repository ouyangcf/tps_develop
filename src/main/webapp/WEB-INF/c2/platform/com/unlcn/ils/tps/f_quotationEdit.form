  <form:scripts>//保存报价单信息&#xD;
btnsave_click:function($event){&#xD;
	//检查数据&#xD;
	var shipperid  = $params.shipperid;&#xD;
	if (angular.isUndefined(shipperid) || &quot;&quot;==shipperid)&#xD;
	{&#xD;
		Messenger.error('请选择的分供方');&#xD;
		$($view.sltshipper.getDom()).focus();&#xD;
		return false;&#xD;
	}&#xD;
	var urgency = $params.urgency;&#xD;
	if (angular.isUndefined(urgency) || &quot;&quot;==urgency)&#xD;
	{&#xD;
		Messenger.error('请选择紧急程序');&#xD;
		$($view.slturgency.getDom()).focus();&#xD;
		return false;&#xD;
	}&#xD;
	var enddate = $params.enddate;&#xD;
	if (angular.isUndefined(enddate) || &quot;&quot;==enddate)&#xD;
	{&#xD;
		Messenger.error('请选择有效日期');&#xD;
		$($view.sltdate.getDom()).focus();&#xD;
		return false;&#xD;
	}&#xD;
	var estimate = $params.estimate;&#xD;
	if (estimate == true)&#xD;
	{&#xD;
		/*Modal.openConfirm({message:&quot;确定是暂估报价吗?&quot;},function()&#xD;
		{},function() &#xD;
		{&#xD;
			return false;&#xD;
		});*/&#xD;
	}&#xD;
	//检查开票信息&#xD;
	var invoice = $params.invoice;&#xD;
	if (angular.isUndefined(invoice))&#xD;
	{&#xD;
		Messenger.error('请选择开票方式');&#xD;
		return false;&#xD;
	}&#xD;
	var payperiod = $params.payperiod;&#xD;
	if (angular.isUndefined(payperiod) || &quot;&quot;==payperiod)&#xD;
	{&#xD;
		Messenger.error('请选择支付帐期');&#xD;
		$($view.sltperiod.getDom()).focus();&#xD;
		return false;&#xD;
	}&#xD;
	var transid=$params.transway;&#xD;
	if (angular.isUndefined(transid))&#xD;
	{&#xD;
		Messenger.error('请选择运输方式');&#xD;
		$($view.slttransway.getDom()).focus();&#xD;
		return false;&#xD;
	}&#xD;
	&#xD;
	//检查报价数据&#xD;
	var result = functions.checkmaindata();&#xD;
	if (!result) return false;&#xD;
	&#xD;
	&#xD;
	//检查支付方式&#xD;
	if (!functions.checkpayment()) return false;&#xD;
	&#xD;
	//检查其它费用&#xD;
	if (!functions.checkothercharge()) return false;&#xD;
	&#xD;
	functions.savedata();	&#xD;
}&#xD;
,&#xD;
//保存数据&#xD;
savedata:function()&#xD;
{&#xD;
	var data = $params.maindata;//$($view.ldetail.getDom()).jqGrid('getRowData');&#xD;
	var paydata = $($view.lpayment.getDom()).jqGrid('getRowData');&#xD;
	var chargedata = $($view.lcharge.getDom()).jqGrid('getRowData');&#xD;
	var postdata = {};&#xD;
	postdata.lineid = $params.quotationid;&#xD;
	postdata.shipperid = $params.shipperid;&#xD;
