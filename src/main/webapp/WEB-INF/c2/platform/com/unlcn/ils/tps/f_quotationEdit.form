button_click:function($event){&#xD;
	//计算单价报价&#xD;
	var adata = $($view.ldetail.getDom()).jqGrid('getRowData');&#xD;
	if ( adata.length == 0 ) &#xD;
	{&#xD;
		Messenger.error('请输入报价信息');&#xD;
		return false;&#xD;
	}&#xD;
	for(var i=0; i&lt;adata.length; i++)  &#xD;
	{  &#xD;
		var data = adata[i];&#xD;
		var selectid = data.id;&#xD;
		var quotationPriceid = selectid + &quot;_quotationPrice&quot;;&#xD;
		var price = data.price;&#xD;
		var kilometer = data.kilometer;&#xD;
		var value = $params.formula;&#xD;
		debugger;&#xD;
		value = value.toLowerCase().replace(&quot;p&quot;,price);&#xD;
		value = value.toLowerCase().replace(&quot;k&quot;,kilometer);&#xD;
		var price = 0;&#xD;
		try&#xD;
		{&#xD;
			var price = eval(value);&#xD;
		}catch(e)&#xD;
		{&#xD;
			Messenger.error('错误的计算公式');			&#xD;
			$($view.txtformula.getDom()).focus();&#xD;
			break;&#xD;
		}&#xD;
		if ( price == null ) &#xD;
		{&#xD;
			Messenger.error('错误的计算公式');			&#xD;
			$($view.txtformula.getDom()).focus();&#xD;
			break;&#xD;
		}&#xD;
		&#xD;
		$(&quot;#&quot;+quotationPriceid).val(parseFloat(price).toFixed(3));&#xD;
		&#xD;
	}&#xD;
},&#xD;
&#xD;
button1_click:function($event){&#xD;
	//计算总价&#xD;
	//计算单价报价&#xD;
	var adata = $($view.ldetail.getDom()).jqGrid('getRowData');&#xD;
	if ( adata.length == 0 ) &#xD;
	{&#xD;
		Messenger.error('请输入报价信息');&#xD;
		return false;&#xD;
	}&#xD;
	var totalqty = 0;&#xD;
	for(var i=0; i&lt;adata.length; i++)  &#xD;
	{  //得到所有的数量&#xD;
		var data = adata[i];&#xD;
		var qty = data.qty;&#xD;
		totalqty += parseFloat(qty);&#xD;
	}&#xD;
	var value = $params.qutotiontotal;&#xD;
	if (angular.isUndefined(value))&#xD;
	{&#xD;
		Messenger.error('请输入正确的总价格');&#xD;
		return false;&#xD;
	}&#xD;
	var oneprice = parseFloat(value)/parseFloat(totalqty);&#xD;
	for(var i=0; i&lt;adata.length; i++)  &#xD;
	{  &#xD;
		var data = adata[i];&#xD;
		var selectid = data.id;&#xD;
		var qty = data.qty;&#xD;
		var price = parseFloat(qty) * parseFloat(oneprice);&#xD;
		&#xD;
		var quotationPriceid = selectid + &quot;_quotationPrice&quot;;&#xD;
		$(&quot;#&quot;+quotationPriceid).val(parseFloat(price).toFixed(3));&#xD;
		&#xD;
	}&#xD;
},&#xD;
&#xD;
&#xD;
quotationPrice_customFormatter:function(cellvalue,options,rowObject){&#xD;
