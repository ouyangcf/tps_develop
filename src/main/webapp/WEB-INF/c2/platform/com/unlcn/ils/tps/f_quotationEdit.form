	&#xD;
	&#xD;
}&#xD;
,&#xD;
pricechange:function(rowobject)&#xD;
{&#xD;
	var rowid = rowobject.id;&#xD;
	var quotationPriceid = rowid + &quot;_quotationPrice&quot;;&#xD;
	var value = $(&quot;#&quot;+quotationPriceid).val();&#xD;
	rowobject.quotationPrice = value;&#xD;
	var overflow = parseFloat(parseFloat(rowobject.quotationPrice-rowobject.price)/parseFloat(rowobject.price)).toFixed(2);&#xD;
	rowobject.overflow = parseFloat(overflow)*100;&#xD;
		&#xD;
	var overflowid = rowid + &quot;_overflow&quot;;&#xD;
	$(&quot;#&quot;+overflowid).text(rowobject.overflow+&quot;%&quot;);&#xD;
},&#xD;
