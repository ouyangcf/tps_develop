refresh:function(){
	//window.location.reload();
	$view.custom_revise.refresh();		
},
custom_revise_beforeProcessing:function(data,status,xhr){
	for(var i=0;i&lt;data.contents.length;i++){
		if(data.contents[i].argueStatus==0){
			data.contents[i].argueStatus=&quot;未申诉&quot;;
		}
		else if(data.contents[i].argueStatus==1){
			data.contents[i].argueStatus=&quot;申诉未处理&quot;;
		}
		else if(data.contents[i].argueStatus==2){
			data.contents[i].argueStatus=&quot;申诉成功&quot;;
		}
		else if(data.contents[i].argueStatus==3){
			data.contents[i].argueStatus=&quot;申诉不成功&quot;;
		}
		else if(data.contents[i].argueStatus==4){
			data.contents[i].argueStatus=&quot;过期未申诉&quot;;
		}
		if(data.contents[i].reviseStatus==0){
			data.contents[i].reviseStatus=&quot;未整改&quot;;
		}
		else if(data.contents[i].reviseStatus==1){
			data.contents[i].reviseStatus=&quot;整改中&quot;;
		}
		else if(data.contents[i].reviseStatus==2){
			data.contents[i].reviseStatus=&quot;已整改&quot;;
		}
		else if(data.contents[i].reviseStatus==3){
			data.contents[i].reviseStatus=&quot;整改成功&quot;;
		}
		else if(data.contents[i].reviseStatus==4){
			data.contents[i].reviseStatus=&quot;整改不成功&quot;;
		}	
		else{
			data.contents[i].reviseStatus=&quot;过期未整改&quot;;
		}	
	}
	if(data.contents.length==0){
		Messenger.post({message:&quot;当前条件没有找到相应数据!&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});	
	}
	else{
		Messenger.post({message:&quot;当前条件共找到&quot;+data.contents.length+&quot;条相应数据!&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});	
	}
},
submitRevise:function(){
	var selArr=$view.custom_revise.getSelectDatas();
	if(selArr.length==0){
		Messenger.post({message:&quot;请勾选记录!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}

	
	if(selArr.length>1){
		Messenger.post({message:&quot;请勾选单条记录!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	else{
	if(selArr[0].reviseStatus!=&quot;未整改&quot;){
		Messenger.post({message:&quot;所选记录不处于未整改状态&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
		Modal.open(&quot;f/submitRivese_dlg&quot;,{&quot;info&quot;:selArr[0]},function($params){
			$view.custom_revise.refresh();
		});
	}	
},
finishRevise:function(){
	var selArr=$view.custom_revise.getSelectDatas();
	if(selArr.length==0){
		Messenger.post({message:&quot;请勾选记录!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	if(selArr.length>1){
		Messenger.post({message:&quot;请勾选单条记录!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	else{
	if(selArr[0].reviseStatus!=&quot;整改中&quot;){
		Messenger.post({message:&quot;所选记录不处于未整改状态&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	$model.e_revise.lineid=selArr[0].lineid;
	$model.e_revise.reviseStatus=2;
	Messenger.post({message:&quot;正在提交，请稍后&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});
	$model.e_revise.$save(function(){
		Messenger.post({type:'success',message:'提交成功'});
		$view.custom_revise.refresh();
    });		
	}	
},

page_onload:function(){
	$view.mainForm.clean();
	var myDate=new Date();
	myDate.setMonth(myDate.getMonth()-1);
	$view.checkYM.clean();
	$params.checkYM=undefined;
	$params.checkYM=myDate.getFullYear()+&quot;-&quot;+(myDate.getMonth()+1);		
	$view.div6.toggle();
		var myFirstDate=$($view.checkYM.getDom()).val();
		var mySecondDate=$($view.checkYM.getDom()).val();
		var firstDate=new Date();
		var secondDate=new Date();
		firstDate.setFullYear(myFirstDate.substring(0,4)*1);
		firstDate.setMonth(myFirstDate.substring(5,myFirstDate.length)*1-1);
		firstDate.setDate(1);
		firstDate.setHours(0,0,0);
		secondDate.setFullYear(myFirstDate.substring(0,4)*1);
		secondDate.setMonth(myFirstDate.substring(5,myFirstDate.length)*1);
		secondDate.setDate(firstDate.getDate()-1);
		secondDate.setHours(23,59,59);	
		$http.post(&quot;ws/s_getByTime&quot;,{&quot;startTime&quot;:firstDate.getTime(),&quot;endTime&quot;:secondDate.getTime()})
			.success(function(data){
				if(data.result.length!=0){
					rules[pos]={field:&quot;checkdetailLineid&quot;,op:&quot;in&quot;,data:data.result};
					pos++;
				}
				//搜不到的时候应该设置一个永远用不到的数字
				else{
					rules[pos]={field:&quot;checkdetailLineid&quot;,op:&quot;eq&quot;,data:-1};
					pos++;					
				}
				$http.post(&quot;ws/s_getCheckDetialByShipperId&quot;,{})
					.success(function(data){
						if(data.result.length!=0){
							rules[pos]={field:&quot;checkdetailLineid&quot;,op:&quot;in&quot;,data:data.result};
							pos++;
						}
						//搜不到的时候应该设置一个永远用不到的数字
						else{
							rules[pos]={field:&quot;checkdetailLineid&quot;,op:&quot;eq&quot;,data:-1};
							pos++;					
						}						
						var conditions = { groupOp:&quot;AND&quot;, rules:rules};	
						if(pos==0){
							conditions=null;
						}
						$view.custom_revise.refresh({filters:conditions});	
						return;						
					});
			});			
},
time2Str:function(mytime){
	Date.prototype.format =function(format)
    {
        var o = {
        &quot;M+&quot; : this.getMonth()+1, //month
&quot;d+&quot; : this.getDate(),    //day
&quot;h+&quot; : this.getHours(),   //hour
&quot;m+&quot; : this.getMinutes(), //minute
&quot;s+&quot; : this.getSeconds(), //second
&quot;q+&quot; : Math.floor((this.getMonth()+3)/3),  //quarter
&quot;S&quot; : this.getMilliseconds() //millisecond
        }
        if(/(y+)/.test(format)) format=format.replace(RegExp.$1,
        (this.getFullYear()+&quot;&quot;).substr(4- RegExp.$1.length));
        for(var k in o)if(new RegExp(&quot;(&quot;+ k +&quot;)&quot;).test(format))
        format = format.replace(RegExp.$1,
        RegExp.$1.length==1? o[k] :
        (&quot;00&quot;+ o[k]).substr((&quot;&quot;+ o[k]).length));
        return format;
    }
	var mystrtime=mytime.format('yyyy-MM-dd hh:mm:ss');
	return mystrtime;
},

