},

button7_click:function($event){
	$params.relateFlag=undefined;
	$params.relateFlag1=undefined;
},

button6_click:function($event){
	var rules=new Array();
	var pos=0;	
	if($params.relateFlag!=undefined){
		rules[pos]={field:&quot;realateFlag&quot;,op:&quot;eq&quot;,data:$params.relateFlag};
		pos++;
	}
	if($params.relateFlag1!=undefined){
		rules[pos]={field:&quot;complainTypeRelateFlag&quot;,op:&quot;eq&quot;,data:$params.relateFlag1};
		pos++;
	}	
	var conditions = { groupOp:&quot;AND&quot;, rules:rules};	
	if(pos==0){
		conditions=null;
	}
	$view.custom_complain_charge.refresh({filters:conditions});	
	return;		
},

button39_click:function($event){
	var selArr=$view.custom_complain_charge.getSelectDatas();
	var operateFlag=false;
	if(selArr.length==0){
		Messenger.post({message:&quot;请勾选记录!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	else{
		var idList=new Array();
		var pos=0;
		for(var i=0;i&lt;selArr.length;i++){
			if(selArr[i].Realate_Flag==&quot;未关联&quot;){
				if(selArr[i].type_realateflag==&quot;已关联&quot;){
					idList[pos]=selArr[i].LineID;
					pos++;
				}
			else
				operateFlag=true;
			}
		}
		if(idList.length!=0)
		{
		Messenger.post({message:&quot;正在生成考核单,请稍后&quot;,type:&quot;info&quot;,hideAfter:2,showCloseButton:true});
		$http.post(&quot;ws/s_createCheckDocBycrm&quot;,{&quot;idList&quot;:idList})
			.success(function(data){
				if(operateFlag==true){
					Messenger.post({message:&quot;部分已生成考核单,存在未关联类型，请及时关联&quot;,type:&quot;info&quot;,hideAfter:2,showCloseButton:true});
				}	
				$view.custom_complain_charge.refresh();					
			})
			.error(function(data){
				Messenger.post({message:&quot;post异常&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});					
			});
		}
		else{
			if(operateFlag==true){
				Messenger.post({message:&quot;所选数据中存在未关联的客诉异常标准，请及时关联&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});
			}	
			else		
			Messenger.post({message:&quot;所选记录并无需要生成考核单的&quot;,type:&quot;info&quot;,hideAfter:2,showCloseButton:true});
		}	
	}		
},

