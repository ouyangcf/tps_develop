  <form:scripts>refresh:function(){
	//window.location.reload();
	$view.custom_complain_charge.refresh();	
},

page_onload:function(){
	$view.div1.toggle();
	$view.searchDiv.toggle();
	$view.mainForm.clean();
	var myDate=new Date();
	myDate.setMonth(myDate.getMonth()-1);
	$view.startDate.clean();
	$params.startDate=undefined;
	$params.endDate=undefined;
	$params.startDate=myDate.getFullYear()+&quot;-&quot;+(myDate.getMonth()+1);	
	$params.endDate=myDate.getFullYear()+&quot;-&quot;+(myDate.getMonth()+2);	
},

button2_click:function($event){
/*	var startDate=$view.startDate.getDate();
	var str0=functions.time2str(startDate);
	var endDate=$view.endDate.getDate();
	var str1=functions.time2str(endDate);*/
	var startDate=$($view.startDate.getDom()).val();
	var endDate=$($view.endDate.getDom()).val();
	Messenger.post({message:&quot;导入中&quot;,type:&quot;info&quot;,hideAfter:2,showCloseButton:true});	
	$http.post(&quot;ws/s_updateComplaintCharges&quot;,{&quot;shipperId&quot;:$params.shipperLineid,
	&quot;startTime&quot;:startDate,&quot;endTime&quot;:endDate})
		.success(function(data){
			Messenger.post({message:&quot;导入完成&quot;,type:&quot;success&quot;,hideAfter:2,showCloseButton:true});	
			$view.custom_complain_charge.refresh();
		});
}
,
time2str:function(date){
	if(date.getMonth()+1&lt;10){
		return date.getFullYear()+&quot;-0&quot;+(date.getMonth()+1);
	}
	else{
		return date.getFullYear()+&quot;-&quot;+(date.getMonth()+1);	
	}
},

custom_complain_charge_beforeProcessing:function(data,status,xhr){
	for(var i=0;i&lt;data.contents.length;i++){
		if(data.contents[i].Realate_Flag==0){
			data.contents[i].Realate_Flag=&quot;未关联&quot;;
		}	
		else 
			data.contents[i].Realate_Flag=&quot;已关联&quot;;
		if(data.contents[i].category==1){
			data.contents[i].category=&quot;客诉&quot;;
		}	
		else 
			data.contents[i].category=&quot;异常&quot;;
		if(data.contents[i].type_realateflag==0){
			data.contents[i].type_realateflag=&quot;未关联&quot;;
		}	
		else 
			data.contents[i].type_realateflag=&quot;已关联&quot;;		
	}
	if(data.contents.length==0){
		Messenger.post({message:&quot;当前条件没有找到相应数据!&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});	
	}
	else{
		Messenger.post({message:&quot;当前条件共找到&quot;+data.contents.length+&quot;条相应数据!&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});	
	}		
},

