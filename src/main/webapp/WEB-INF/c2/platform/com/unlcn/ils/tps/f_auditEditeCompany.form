page_onload:function(){
	$model.entity.lineid=$params.lineid.joinid;
	$model.entity.companyName=$params.lineid.shipperName;
	$model.entity.cityId=$params.lineid.cityid;
	$model.entity.legalUser=$params.lineid.legalUser;
	$model.entity.mainContract=$params.lineid.maincontract;
	$model.entity.taxNo=$params.lineid.taxno;
	$model.entity.capital=$params.lineid.capital;
	$model.entity.assets=$params.lineid.assets;
	$model.entity.contractName=$params.lineid.contractname;
	$model.entity.mobileno=$params.lineid.mobileno;
	$model.entity.contract=$params.lineid.contract;
	$model.entity.roadLicence=$params.lineid.roadLicence;
	$model.entity.idNumber=$params.lineid.idNumber;
	$model.entity.carNumber=$params.lineid.carNumber;
	$params.cityId=functions.getCityName($params.lineid.cityid);
	var conditions = { groupOp:&quot;AND&quot;, rules:[{field:&quot;joinId&quot;,op:&quot;eq&quot;,data:$params.lineid.joinid},{field:&quot;joinFlag&quot;,op:&quot;eq&quot;,data:1}]};
	$view.list.refresh({filters:conditions});
},

btn_add_click:function($event){
	if (!$view.mainForm.check())
	{
		Messenger.error('有错误信息或未填写的信息');
		return;
	};
