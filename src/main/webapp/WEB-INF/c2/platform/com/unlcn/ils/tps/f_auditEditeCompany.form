		return;
	}
	/*if(selArr.length>1){
		Messenger.post({message:&quot;请勾选单条记录!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}*/
	
		var idList=new Array();
		var pos=0;
		for(var i=0;i&lt;selArr.length;i++)
		{
			idList[pos]=selArr[i].lineid;
			pos++;
		}
		Messenger.post({message:&quot;正在删除!&quot;,type:&quot;info&quot;,hideAfter:2,showCloseButton:true});
		$http.post(&quot;ws/s_delInterLine&quot;,{&quot;idList&quot;:idList})
			.success(function(data){
				Messenger.post({type:'success',message:'删除成功'});
				var conditions = { groupOp:&quot;AND&quot;, rules:[{field:&quot;joinId&quot;,op:&quot;eq&quot;,data:$model.entity.lineid},{field:&quot;joinFlag&quot;,op:&quot;eq&quot;,data:1}]};
				$view.list.refresh({filters:conditions});				
			})
			.error(function(data){
					Messenger.post({message:&quot;删除失败&quot;,type:&quot;error&quot;,hideAfter:5,showCloseButton:true});
				});	
	
},

