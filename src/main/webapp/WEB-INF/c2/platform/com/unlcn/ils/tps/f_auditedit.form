  <form:scripts>

page_onload:function(){ 
	$model.entity.lineid=$params.lineid.joinid;
	$model.entity.carNumber=$params.lineid.carNumber;
	$model.entity.driverName=$params.lineid.driverName;
	$model.entity.cityId=$params.lineid.cityid;
	$model.entity.licenseno=$params.lineid.licenseno;
	$model.entity.vehicleLicenseno=$params.lineid.vehiclelicenseno;
	$model.entity.cardno=$params.lineid.cardno;
	$model.entity.mobileno=$params.lineid.mobileno;
	$params.cityId=functions.getCityName($params.lineid.cityid);
	var conditions = { groupOp:&quot;AND&quot;, rules:[{field:&quot;joinId&quot;,op:&quot;eq&quot;,data:$params.lineid.joinid},{field:&quot;joinFlag&quot;,op:&quot;eq&quot;,data:0}]};
	$view.list1.refresh({filters:conditions});
	
	/*$model.entity.lineid=$params.lineid.lineid;
	$params.driverName=$params.lineId.driverName;
	$params.cityId=cityName;
	$params.licenseno=$params.lineId.licenseno;
	$params.vehicleLicenseno=$params.lineId.vehiclelicenseno;
	$params.cardno=$params.lineId.cardno;
	$params.mobileno=$params.lineId.mobileno;*/
},
   

btn_add_click:function($event){
	if (!$view.mainForm.check())
	{
		Messenger.error('有错误信息或未填写的信息');
		return;
	};
	Messenger.post({message:&quot;正在提交&quot;,type:&quot;info&quot;,hideAfter:2,showCloseButton:true});
	$http.post(&quot;ws/s_updateDriver&quot;,{&quot;data&quot;:$model.entity})
		.success(function(data){
	        Modal.instance.close();
	        Messenger.post({type:'success',message:'数据保存成功'});	
		});
},



btn_clean_click:function($event){
	$view.mainForm.clean();
},
getCityName:function(id){
	var i=0;
	for(;i&lt;$model.cityIds.result.length;i++){
		if($model.cityIds.result[i]==id)
			break;
	}
	var cityName=$model.cityNames.result[i];
	return cityName;
},
getProvinceName:function(id){
	var i=0;
	for(;i&lt;$model.provinceIds.result.length;i++){
		if($model.provinceIds.result[i]==id)
			break;
	}
	var provinceName=$model.provinceNames.result[i];
	return provinceName;
},

cityId_nodeCheck:function(event,treeId,treeNode){
	
},

cityId_nodeClick:function(event,treeId,treeNode,clickFlag){
	$model.entity.cityId=treeNode.id;
},

cityId_beforeCheck:function(treeId,treeNode){
	
},

button_click:function($event){
	var selArr=$view.list1.getSelectDatas();
	if(selArr.length==0){
		Messenger.post({message:&quot;请选择要删除行!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	/*if(selArr.length>1){
		Messenger.post({message:&quot;请勾选单条记录!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}*/
	
		var idList=new Array();
		var pos=0;
		for(var i=0;i&lt;selArr.length;i++)
		{
			idList[pos]=selArr[i].lineid;
			pos++;
		}
		Messenger.post({message:&quot;正在删除!&quot;,type:&quot;info&quot;,hideAfter:2,showCloseButton:true});
		$http.post(&quot;ws/s_delInterLine&quot;,{&quot;idList&quot;:idList})
			.success(function(data){
				Messenger.post({type:'success',message:'删除成功'});
				var conditions = { groupOp:&quot;AND&quot;, rules:[{field:&quot;joinId&quot;,op:&quot;eq&quot;,data:$model.entity.lineid},{field:&quot;joinFlag&quot;,op:&quot;eq&quot;,data:0}]};
				$view.list1.refresh({filters:conditions});				
			}) 
			.error(function(data){
					Messenger.post({message:&quot;删除失败&quot;,type:&quot;error&quot;,hideAfter:5,showCloseButton:true});
				});	
},

button2_click:function($event){
	Modal.open(&quot;f/interLineDlg&quot;,{},function($params){
				/*setTimeout(&quot;window.location.reload()&quot;,2000);*/
				var cityName=functions.getCityName($params.startcity);
				var provinceName=functions.getProvinceName($params.desprovince);
				var arr=$view.list1.getDataIDs();
				var rowid=0;
				if(arr.length!=0)
				rowid=arr[arr.length-1]*1+1;
				//$view.list1.addRowData(rowid,{start_city:cityName,dest_province:provinceName,
				//price:$params.price,qty:$params.qty});
				
				//下面新增一条数据
				$model.join_interline.startCity=cityName;
				$model.join_interline.destProvince=provinceName;
				$model.join_interline.price=$params.price.trim().length==0?&quot;&quot;:$params.price;
				$model.join_interline.qty=$params.qty.trim().length==0?&quot;&quot;:$params.qty;
				$model.join_interline.joinFlag=0;
				$model.join_interline.joinId=$model.entity.lineid;
				Messenger.post({type:'info',message:'正在提交'});
				$http.post(&quot;ws/s_possessInterLine&quot;,{&quot;data&quot;:$model.join_interline})
					.success(function(data){
					Messenger.post({type:'success',message:'新增成功'});
					var conditions = { groupOp:&quot;AND&quot;, rules:[{field:&quot;joinId&quot;,op:&quot;eq&quot;,data:$model.entity.lineid},{field:&quot;joinFlag&quot;,op:&quot;eq&quot;,data:0}]};
					$view.list1.refresh({filters:conditions});
					});
			});	
},


