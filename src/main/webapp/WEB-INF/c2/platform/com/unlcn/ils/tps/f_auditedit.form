	Modal.open(&quot;f/getDriverFile&quot;,{&quot;info&quot;:$params.lineid.joinid},function(){
	});  
},


dlgDriverUpon_click:function($event){
	Modal.open(&quot;f/driverAttched_dlg&quot;,{&quot;info&quot;:$params.lineid.joinid},function(){
	});  
