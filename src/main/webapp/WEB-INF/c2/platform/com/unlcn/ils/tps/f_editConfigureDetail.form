  <form:scripts>

totalvalue_checkAfter:function(checkResult){
	var $subvalue=$($view.totalvalue.getDom());
	var subvalue=$subvalue.val();
	var re=/^(0|[1-9]\d*)$/;
	var result=  re.test($model.e_configuredetail.totalvalue);
	if(subvalue.length==0)
	return;
	if($.trim(subvalue)==''){
	Messenger.post({message:&quot;总分仅可填写整数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});		
	return;
	}
	if(!result){
		Messenger.post({message:&quot;总分仅可填写整数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});			
	}
},

subvalue_checkAfter:function(checkResult){
	var $subvalue=$($view.subvalue.getDom());
	var subvalue=$subvalue.val();
	var re=/^(0|[1-9]\d*)$/;
	var result=  re.test($model.e_configuredetail.subvalue);
	if(subvalue.length==0)
	return;
	if($.trim(subvalue)==''){
	Messenger.post({message:&quot;每降一个百分点扣分值仅可填写整数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});		
	return;
	}
	if(!result){
		Messenger.post({message:&quot;每降一个百分点扣分值仅可填写整数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});			
	}	
},

lowvalue_checkAfter:function(checkResult){
	var $subvalue=$($view.lowvalue.getDom());
	var subvalue=$subvalue.val();
	var re=/^(0|[1-9]\d*)$/;
	var result=  re.test($model.e_configuredetail.lowvalue);
	if(subvalue.length==0)
	return;
	if($.trim(subvalue)==''){
	Messenger.post({message:&quot;最低得分指标仅可填写整数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});		
	return;
	}
	if(!result){
		Messenger.post({message:&quot;最低得分指标仅可填写整数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});			
	}		
},

checkvalue_checkAfter:function(checkResult){
	var $money=$($view.checkvalue.getDom());
	var money_val=$money.val();
	var re=/^[+-]?([0-9]*\.?[0-9]+|[0-9]+\.?[0-9]*)([eE][+-]?[0-9]+)?$/;
	var result=  re.test($model.e_configuredetail.checkvalue);
	if(money_val.length==0)
	return;
	if($.trim(money_val)==''){
	Messenger.post({message:&quot;考核金额仅可填写整数或小数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});		
	return;
	}
	if(!result){
		Messenger.post({message:&quot;考核金额仅可填写整数或小数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});			
	}	
},

weight_checkAfter:function(checkResult){
	var $subvalue=$($view.weight.getDom());
	var subvalue=$subvalue.val();
	var re=/^(0|[1-9]\d*)$/;
	var result=  re.test($model.e_configuredetail.weight);
	if(subvalue.length==0)
	return;
	if($.trim(subvalue)==''){
	Messenger.post({message:&quot;权重仅可填写整数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});		
	return;
	}
	if(!result){
		Messenger.post({message:&quot;权重仅可填写整数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});			
	}		
},

button_click:function($event){
	if($model.e_configuredetail.itemName==undefined){
		Messenger.post({message:&quot;请选择考核项目！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	/*if($model.e_configuredetail.opearateName==undefined){
		Messenger.post({message:&quot;请选择考核类型！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}*/
	if($model.e_configuredetail.requirements==undefined||$model.e_configuredetail.requirements.length==0){
		Messenger.post({message:&quot;指标要求未填写!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}	
	if($model.e_configuredetail.totalvalue==undefined){
		Messenger.post({message:&quot;总分未填写或格式不正确!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}	
	if($model.e_configuredetail.subvalue==undefined){
		Messenger.post({message:&quot;每降一个百分点扣分值未填写或格式不正确!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}	
	if($model.e_configuredetail.lowvalue==undefined){
		Messenger.post({message:&quot;最低得分指标未填写或格式不正确!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}	
	if($model.e_configuredetail.checkvalue==undefined){
		Messenger.post({message:&quot;考核金额未填写或格式不正确!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}		
	if($model.e_configuredetail.weight==undefined){
		Messenger.post({message:&quot;权重未填写或格式不正确!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	if($model.e_configuredetail.weight>100-$params.totalweight){
		Messenger.post({message:&quot;当前权重已大于剩余权重!&quot;+&quot;剩余权重为:&quot;+(100-$params.totalweight),type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;		
	}
	if($model.e_configuredetail.calDescription==undefined||$model.e_configuredetail.calDescription.length==0){
		Messenger.post({message:&quot;计算方法未填写!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}	
	if($model.e_configuredetail.description==undefined||$model.e_configuredetail.description.length==0){
		Messenger.post({message:&quot;考核依据未填写!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}	
	Messenger.post({message:&quot;正在提交，请稍后&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});		
    $model.e_configuredetail.$save(function(){
			Modal.instance.close();
			Messenger.post({type:'success',message:'修改成功'});
    });		
},

page_onload:function(){
	for(var i=0;i&lt;$model.items.result.length;i++){
		$view.itemName.addOptions($model.items.result[i]);
	}
	for(var i=0;i&lt;$model.methods.result.length;i++){
		$view.calDescription.addOptions($model.methods.result[i]);
	}
	if($params.info.calDescription!=null&amp;&amp;$params.info.calDescription!=undefined)
	$http.post(&quot;ws/s_getMethodId&quot;,{methodName:$params.info.calDescription})
		.success(function(data){
			$model.e_configuredetail.calDescription=data.result;
		})
	$model.e_configuredetail.configureLineid=$params.info.configureLineid;	
	$model.e_configuredetail.lineid=$params.info.lineid;	
	$model.e_configuredetail.itemName=$params.info.itemLineid;
	
	$model.e_configuredetail.requirements=$params.info.requirements;
	$model.e_configuredetail.totalvalue=$params.info.totalvalue;
	$model.e_configuredetail.subvalue=$params.info.subvalue;
	$model.e_configuredetail.lowvalue=$params.info.lowvalue;
	$model.e_configuredetail.checkvalue=$params.info.checkvalue;
	$model.e_configuredetail.weight=$params.info.weight;
	$model.e_configuredetail.calDescription=$params.info.calDescription;
	$model.e_configuredetail.description=$params.info.description;
	if($params.info.startDate!=undefined&amp;&amp;$params.info.startDate.length!=0){
		$model.e_configuredetail.startDate=$params.info.startDate+&quot; 00:00:00&quot;;
	}
	if($params.info.endDate!=undefined&amp;&amp;$params.info.endDate.length!=0){
		$model.e_configuredetail.endDate=$params.info.endDate+&quot; 00:00:00&quot;;
	}	
	var pos=functions.getiIndex();
	$view.opearateName.setOptions({});
	for(var i=0;i&lt;$model.types.result[pos].contents.length;i++){
		$view.opearateName.addOptions($model.types.result[pos].contents[i]);
	}
	$view.opearateName.setDefaultValue();
	var options = $view.opearateName.getOptions();
	for(var j=0;j&lt;options.length;j++){
		if(options[j].key==$params.info.opearateName){
			$model.e_configuredetail.opearateName=options[j].value;
		}
	}
},

itemName_change:function(){
	if($model.e_configuredetail.itemName==undefined){
		$model.e_configuredetail.opearateName=undefined;
		return;
	}
	var pos=functions.getiIndex();
	$view.opearateName.setOptions({});
	for(var i=0;i&lt;$model.types.result[pos].contents.length;i++){
		$view.opearateName.addOptions($model.types.result[pos].contents[i]);
	}
	$view.opearateName.setDefaultValue();	
},

