},
getiIndex:function(){
	var ipos=0;
	var l=$model.itemsid.result.length;
	for(;ipos&lt;l;ipos++){
		if($model.e_configuredetail.itemName==$model.itemsid.result[ipos]){
			break;
		}	
	}
	return ipos;
},

