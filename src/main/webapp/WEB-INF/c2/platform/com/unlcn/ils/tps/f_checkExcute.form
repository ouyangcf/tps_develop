custom_checkHead_beforeProcessing:function(data,status,xhr){
	for(var i=0;i&lt;data.contents.length;i++){&#xD;
		if(data.contents[i].checkStatus==0){&#xD;
			data.contents[i].checkStatus=&quot;未审核&quot;;&#xD;
		}&#xD;
		else if(data.contents[i].checkStatus==1){&#xD;
			data.contents[i].checkStatus=&quot;提交审核未处理&quot;;&#xD;
		}&#xD;
		else if(data.contents[i].checkStatus==2){&#xD;
			data.contents[i].checkStatus=&quot;审核通过&quot;;&#xD;
		}&#xD;
		else if(data.contents[i].checkStatus==3){&#xD;
			data.contents[i].checkStatus=&quot;审核不通过&quot;;&#xD;
		}&#xD;
		if(data.contents[i].collectStatus==0){&#xD;
			data.contents[i].collectStatus=&quot;未汇总&quot;;&#xD;
		}&#xD;
		else if(data.contents[i].collectStatus==1){&#xD;
			data.contents[i].collectStatus=&quot;已汇总&quot;;&#xD;
		}&#xD;
		if(data.contents[i].argueStatus==0){&#xD;
			data.contents[i].argueStatus=&quot;未申诉&quot;;&#xD;
		}&#xD;
		else if(data.contents[i].argueStatus==1){&#xD;
			data.contents[i].argueStatus=&quot;部分申诉&quot;;&#xD;
		}&#xD;
		else{&#xD;
			data.contents[i].argueStatus=&quot;已申诉&quot;;&#xD;
		}&#xD;
	}&#xD;
	if(data.contents.length==0){&#xD;
		Messenger.post({message:&quot;当前条件没有找到相应数据!&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});	&#xD;
	}&#xD;
	else{&#xD;
		Messenger.post({message:&quot;当前条件共找到&quot;+data.contents.length+&quot;条相应数据!&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});	&#xD;
	}	
},&#xD;
collect:function(){&#xD;
	var selArr=$view.custom_checkHead.getSelectDatas();&#xD;
	if(selArr.length==0){&#xD;
		Messenger.post({message:&quot;请勾选记录!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	&#xD;
		return;&#xD;
	}&#xD;
	var idList=new Array();&#xD;
	var pos=0;&#xD;
	for(var i=0;i&lt;selArr.length;i++){&#xD;
		if(selArr[i].checkStatus==&quot;未审核&quot;||selArr[i].checkStatus==&quot;审核不通过&quot;){&#xD;
			idList[pos]=selArr[i].lineid;&#xD;
			pos++;		&#xD;
		}&#xD;
	}&#xD;
	if(idList.length==0){&#xD;
		Messenger.post({message:&quot;未存在符合汇总条件的数据!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});&#xD;
		return;	&#xD;
	}&#xD;
	Messenger.post({message:&quot;正在进行汇总,请稍后&quot;,type:&quot;info&quot;,hideAfter:2,showCloseButton:true});&#xD;
	$http.post(&quot;ws/s_collect&quot;,{&quot;idList&quot;:idList})&#xD;
		.success(function(data){&#xD;
			Messenger.post({type:'success',message:'汇总成功'});&#xD;
			$view.custom_checkHead.refresh();			&#xD;
		})&#xD;
		.error(function(data){});&#xD;
},&#xD;
submitCheck:function(){&#xD;
	var selArr=$view.custom_checkHead.getSelectDatas();&#xD;
	if(selArr.length==0){&#xD;
		Messenger.post({message:&quot;请勾选记录!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	&#xD;
		return;&#xD;
	}&#xD;
	var idList=new Array();&#xD;
	var pos=0;&#xD;
	for(var i=0;i&lt;selArr.length;i++){&#xD;
		if((selArr[i].collectStatus==&quot;已汇总&quot;&amp;&amp;selArr[i].checkStatus==&quot;未审核&quot;)||(selArr[i].collectStatus==&quot;已汇总&quot;&amp;&amp;selArr[i].checkStatus==&quot;审核不通过&quot;)){&#xD;
			idList[pos]=selArr[i].lineid;&#xD;
			pos++;		&#xD;
		}&#xD;
	}&#xD;
	if(idList.length==0){&#xD;
		Messenger.post({message:&quot;未存在符合提交条件的数据!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});&#xD;
		return;	&#xD;
	}	&#xD;
	Messenger.post({message:&quot;正在提交,请稍后&quot;,type:&quot;info&quot;,hideAfter:2,showCloseButton:true});&#xD;
	$http.post(&quot;ws/s_submitCheck&quot;,{&quot;idList&quot;:idList})&#xD;
		.success(function(data){&#xD;
			Messenger.post({type:'success',message:'提交成功'});&#xD;
			$view.custom_checkHead.refresh();			&#xD;
		})&#xD;
		.error(function(data){});&#xD;
},

