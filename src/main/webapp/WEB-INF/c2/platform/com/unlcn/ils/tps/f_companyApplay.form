  <form:scripts>

btn_add_click:function($event){
	if (!$view.mainForm.check())
	{
		Messenger.error('有错误或未填写的信息');
		return;
	}
	if($params.roadLicenceFile==undefined||$params.roadLicenceFile==null){
		Messenger.error('请上传道路运输经营许可证照片');
		return;
	}
	if($params.idNumberFile==undefined||$params.idNumberFile==null){
		Messenger.error('请上传营业执照照片');
		return;
	}
	var idArr=$view.list.getRowData();
	if(idArr.length==0){
		Messenger.post({message:&quot;意向线路至少添加一条以上！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;		
	}
	
	var accountRoData = $view.accountlist.getRowData();
	if(accountRoData.length==0){
		Messenger.post({message:&quot;账户信息至少添加一条以上！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;		
	}
	var accountList = new Array();
	for(var i in accountRoData ){
		var id = accountRoData[i].lineid;
		var pathid = id+'path';
		accountRoData[i].accountFilepath = $(&quot;#&quot;+pathid).text();
		accountList[i] = accountRoData[i];
	}
	
	var linkmanRowData = $view.linkmanlist.getRowData();
	/*if(linkmanRowData.length==0){
		Messenger.post({message:&quot;联系人信息至少添加一条以上！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;		
	}*/
	var linkmanList = new Array();
	for(var i in linkmanRowData){
		linkmanList[i] = linkmanRowData[i];
	}
	
	//数据整理
	var startCitys=new Array();
	var endCitys=new Array();
	var prices=new Array();
	var qtys=new Array();
	for(var i=0;i&lt;idArr.length;i++){
		startCitys[i]=idArr[i].startCity;
		endCitys[i]=idArr[i].destProvince;
		prices[i]=idArr[i].price;
		qtys[i]=idArr[i].qty;
	}	
	//道路经营许可证和营业执照数据处理
	var idNumberFileURL = $params.idNumberFile.url;
	var idNumberFileName = $params.idNumberFile.name;
	var roadLicenceFileURL = $params.roadLicenceFile.url;
	var roadLicenceFileName = $params.roadLicenceFile.name;
	Modal.openConfirm({message:&quot;是否确定提交该分供方的认证申请？&quot;},function(){
		Messenger.post({message:&quot;正在提交&quot;,type:&quot;info&quot;,hideAfter:2,showCloseButton:true});	
		$http.post(&quot;ws/s_createCompany&quot;,{&quot;data&quot;:$model.entity,
			&quot;startCity&quot;:startCitys,&quot;endCity&quot;:endCitys,
			&quot;price&quot;:prices,&quot;qty&quot;:qtys,
			linkmanList:linkmanList,accountList:accountList,
			&quot;idNumberFileURL&quot;:idNumberFileURL,&quot;idNumberFileName&quot;:idNumberFileName,
			&quot;roadLicenceFileURL&quot;:roadLicenceFileURL,&quot;roadLicenceFileName&quot;:roadLicenceFileName})
			.success(function(data){
				Messenger.post({type:'success',message:'新增成功'});	
				/*Modal.open(&quot;f/companyAttched_dlg&quot;,{&quot;info&quot;:data.result},function(){
			    }); */	
				$view.mainForm.clean();
				var ids=$view.list.getDataIDs();
				for(var i=0;i&lt;ids.length;i++){
					$view.list.delRowData(ids[i]);
				}
				$view.accountlist.clearGridData(true);
				$view.linkmanlist.clearGridData(true);
			});		
	});
},

btn_clean_click:function($event){
	$view.mainForm.clean();
	var ids=$view.list.getDataIDs();
	for(var i=0;i&lt;ids.length;i++){
		$view.list.delRowData(ids[i]);
	}
	$view.accountlist.clearGridData(true);
    $view.linkmanlist.clearGridData(true);
},


cityId_beforeClick:function(treeId,treeNode,clickFlag){
	var check = (treeNode &amp;&amp; !treeNode.isParent);  
    if (!check)
    {
    	Messenger.error('不能选择省份');
    }
    return check;  
},


page_onload:function(){
	$view.list.addRow(rowid,parameters)
	$params.idi = 1;
},

button_click:function($event){
	Modal.open(&quot;f/interLineDlg&quot;,{},function($params){
				var cityName=functions.getCityName($params.startcity);
				var provinceName=functions.getProvinceName($params.desprovince);
				var arr=$view.list.getDataIDs();
				var rowid=0;
				if(arr.length!=0)
				rowid=arr[arr.length-1]*1+1;
				$view.list.addRowData(rowid,{startCity:cityName,destProvince:provinceName,
				price:$params.price.trim().length==0?&quot;&quot;:$params.price,qty:$params.qty.trim().length==0?&quot;&quot;:$params.qty});	
			});		
},
getCityName:function(id){
	var i=0;
	for(;i&lt;$model.cityIds.result.length;i++){
		if($model.cityIds.result[i]==id)
			break;
	}
	var cityName=$model.cityNames.result[i];
	return cityName;
},
getProvinceName:function(id){
	var i=0;
	for(;i&lt;$model.provinceIds.result.length;i++){
		if($model.provinceIds.result[i]==id)
			break;
	}
	var provinceName=$model.provinceNames.result[i];
	return provinceName;
},

button2_click:function($event){
	var selArr=$view.list.getSelectDatas();
	if(selArr.length==0){
		Messenger.post({message:&quot;请选择要删除行!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
/*	if(selArr.length>1){
		Messenger.post({message:&quot;请勾选单条记录!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}*/
		var del =new Array();
		var del = $view.list.getSelectRowIds();
		var length=del.length;
		for(var i=0;i&lt;length;i++){
			$view.list.delRowData(del[0]);
		}
	
},



roadLicenceFile_onSuccess:function(data){
	Messenger.post({message:&quot;文件上传成功!&quot;,type:&quot;success&quot;,hideAfter:2,showCloseButton:true});
	$params.roadLicenceFile=data;
},

roadLicenceFile_onError:function(data){
	Messenger.post({message:&quot;文件上传失败!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});
},

idNumberFile_onSuccess:function(data){
	Messenger.post({message:&quot;文件上传成功!&quot;,type:&quot;success&quot;,hideAfter:2,showCloseButton:true});
	$params.idNumberFile=data;
},

idNumberFile_onError:function(data){
	Messenger.post({message:&quot;文件上传失败!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});
},

tableButton_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	return &quot;&lt;button ng-click='functions.print(rowObject)'>上传银行卡照片&lt;/button>&quot;;
},
print:function(rowObject){
	var i = rowObject.lineid
	var pathid = i+&quot;path&quot;; 
	//$(&quot;#&quot;+pathid).val(&quot;aaa&quot;);
	Modal.open(&quot;f/accountFileUp&quot;,{},function(id){
	//alert(id);
		$(&quot;#&quot;+pathid).text(id);
	});
	
	//alert($(&quot;#&quot;+pathid).val());
},
accountFilepath_customFormatter:function(cellvalue,options,rowObject,dictValueExpression){
	var i = rowObject.lineid
	return '&lt;span id=&quot;'+i+'path&quot;>'+&quot;上传后显示&quot;+'&lt;/span>'
},

buttonAdd_click:function(rowId,rowData,tableData){
	$view.accountlist.addRowData($params.idi,{lineid:$params.idi},&quot;last&quot;,null);
	$params.idi++;
}</form:scripts>
