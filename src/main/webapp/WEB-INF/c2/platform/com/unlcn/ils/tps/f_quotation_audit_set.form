  <form:scripts>

add_click:function($event){
	Modal.open(&quot;f/quotationSet_dlg&quot;,{},function($params){
			$view.list.refresh();

		});
},

edit_click:function($event){
		var ob=$view.list.getSelectDatas();
	if(ob.length!=1)
	{
		Messenger.post({type:'error',message:'请选择一条数据'});
		return false;
	}
	var DataList=ob[0];
	if(ob[0].type==&quot;临时报价单&quot;)
		DataList.type=1;
	if(ob[0].type==&quot;合同报价单&quot;)
		DataList.type=2;
	if(ob[0].flowid==&quot;一号审核&quot;)
		DataList.flowid=1;
	if(ob[0].flowid==&quot;二号审核&quot;)
		DataList.flowid=2;
	if(ob[0].flowid==&quot;三号审核&quot;)
		DataList.flowid=3;
	if(ob[0].flowid==&quot;四号审核&quot;)
		DataList.flowid=4;
	if(ob[0].flowid==&quot;五号审核&quot;)
		DataList.flowid=5;
	if(ob[0].flowid==&quot;六号审核&quot;)
		DataList.flowid=6;
	if(ob[0].flowid==&quot;七号审核&quot;)
		DataList.flowid=7;
	if(ob[0].flowid==&quot;八号审核&quot;)
		DataList.flowid=8;
	
	Modal.open(&quot;f/quotationEdit_dlg&quot;,{&quot;DataList&quot;:DataList},function($params){
	$view.list.refresh();
		});
},

delete_click:function($event){
	var ob=$view.list.getSelectDatas();
	if(ob.length!=0){
	
	var idList=new Array();
	var levelList=new Array();
	var pos=0;
	for(var i=0;i&lt;ob.length;i++){
		idList[pos]=ob[i].lineid;
		pos++;
		}
		$http.post(&quot;ws/s_quotationDelete&quot;,{&quot;idList&quot;:idList})
		.success(function(){
			Messenger.post({type:'success',message:'删除成功'});
			$view.list.refresh();
		})
	}
	else
	{
		Messenger.post({type:'error',message:'请选择一条数据'});
		return;
	}
},

list_beforeProcessing:function(data,status,xhr){
	var l=data.contents.length;
	for(var i=0;i&lt;l;i++){  
		var obj=data.contents[i].type;
		var ob=data.contents[i].flowid;
		switch(obj)
		{case 1:obj=&quot;临时报价单&quot;;
		break;
		case 2:obj=&quot;合同报价单&quot;	;
		break;
		}
		switch(ob)
		{case 1:ob=&quot;一号审核&quot;;
		break;
		case 2:ob=&quot;二号审核&quot;	;
		break;
		case 3:ob=&quot;三号审核&quot;	;
		break;
		case 4:ob=&quot;四号审核&quot;	;
		break;
		case 5:ob=&quot;五号审核&quot;	;
		break;
		case 6:ob=&quot;六号审核&quot;	;
		break;
		case 7:ob=&quot;七号审核&quot;	;
		break;
		case 8:ob=&quot;八号审核&quot;	;
		break;
		}
		data.contents[i].type=obj;
		data.contents[i].flowid=ob;
	}
}</form:scripts>
