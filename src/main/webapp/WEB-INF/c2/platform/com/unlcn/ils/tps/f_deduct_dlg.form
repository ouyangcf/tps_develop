  <form:scripts>

page_onload:function(){
	var conditions = { groupOp:&quot;AND&quot;, rules:[{field:&quot;lineid&quot;,op:&quot;eq&quot;,data:$params.lineid}]};
	$view.custom_revise.refresh({filters:conditions});	
},

deductMoney_checkAfter:function(checkResult){
	var $money=$($view.deductMoney.getDom());
	var money_val=$money.val();
	var re=/^[+-]?([0-9]*\.?[0-9]+|[0-9]+\.?[0-9]*)([eE][+-]?[0-9]+)?$/;
	var result=  re.test($model.e_deduct.deductMoney);
	if(money_val.length==0)
	return;
	if($.trim(money_val)==''){
	Messenger.post({message:&quot;扣款金额仅可填写整数或小数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});		
	return;
	}
	if(!result){
		Messenger.post({message:&quot;扣款金额仅可填写整数或小数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});			
	}	
},

button_click:function($event){
	if($model.e_deduct.deductMoney==undefined){
		Messenger.post({message:&quot;请填写扣款金额！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	if($model.e_deduct.deductReson==undefined||$model.e_deduct.deductReson.length==0){
		Messenger.post({message:&quot;请填写扣款理由!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	Messenger.post({message:&quot;正在提交申诉，请稍后&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});	
	$model.e_deduct.reviseLineid=$params.lineid;
	$model.e_deduct.$save(function(){
        Modal.instance.close();
        Messenger.post({type:'success',message:'新增成功'});
    });
}</form:scripts>
