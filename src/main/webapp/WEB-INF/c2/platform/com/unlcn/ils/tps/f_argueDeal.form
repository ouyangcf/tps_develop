	var pos=functions.getiIndex();&#xD;
	$view.checktype.setOptions({});&#xD;
	for(var i=0;i&lt;$model.types.result[pos].contents.length;i++){&#xD;
		$view.checktype.addOptions($model.types.result[pos].contents[i]);&#xD;
	}&#xD;
	$view.checktype.setDefaultValue();	
},

