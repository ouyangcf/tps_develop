button2_click:function($event){
	var rules=new Array();&#xD;
	var pos=0;	&#xD;
	if($params.checkYM!=undefined){&#xD;
		//将范围设置为月初第一天以及月末最后一天&#xD;
		var myFirstDate=$($view.checkYM.getDom()).val();&#xD;
		var mySecondDate=$($view.checkYM.getDom()).val();&#xD;
		var firstDate=new Date();&#xD;
		var secondDate=new Date();&#xD;
		firstDate.setFullYear(myFirstDate.substring(0,4)*1);&#xD;
		firstDate.setMonth(myFirstDate.substring(5,myFirstDate.length)*1-1);&#xD;
		firstDate.setDate(1);&#xD;
		firstDate.setHours(0,0,0);&#xD;
		secondDate.setFullYear(myFirstDate.substring(0,4)*1);&#xD;
		secondDate.setMonth(myFirstDate.substring(5,myFirstDate.length)*1);&#xD;
		secondDate.setDate(firstDate.getDate()-1);&#xD;
		secondDate.setHours(23,59,59);&#xD;
		rules[pos]={field:&quot;createTime&quot;,op:&quot;ge&quot;,data:functions.time2Str(firstDate)};//大于月初第一天以及小于月末最后一天&#xD;
		pos++;&#xD;
		rules[pos]={field:&quot;createTime&quot;,op:&quot;le&quot;,data:functions.time2Str(secondDate)};//大于月初第一天以及小于月末最后一天&#xD;
		pos++;&#xD;
	}&#xD;
	if($params.checkTag!=undefined){&#xD;
		rules[pos]={field:&quot;argueStatus&quot;,op:&quot;eq&quot;,data:$params.checkTag};&#xD;
		pos++;&#xD;
	}		&#xD;
	//要么项目选了，要么类型选了，要么项目没选&#xD;
	//情况一，项目没选&#xD;
	if($model.entity.shipperId==null){&#xD;
		if($params.checkitem==undefined){&#xD;
			var conditions = { groupOp:&quot;AND&quot;, rules:rules};	&#xD;
			if(pos==0){&#xD;
				conditions=null;&#xD;
			}&#xD;
			$view.custom_argue.refresh({filters:conditions});	&#xD;
			return;	&#xD;
		}&#xD;
		//情况二，项目选了，类型没选&#xD;
		if($params.checkitem!=undefined&amp;&amp;$params.checktype==undefined){&#xD;
			$http.post(&quot;ws/s_getIdByItemid&quot;,{&quot;ItemLineid&quot;:$params.checkitem})&#xD;
				.success(function(data){&#xD;
					if(data.result.length!=0){&#xD;
						rules[pos]={field:&quot;checkdetailLineid&quot;,op:&quot;in&quot;,data:data.result};&#xD;
						pos++;&#xD;
					}&#xD;
					//搜不到的时候应该设置一个永远用不到的数字&#xD;
					else{&#xD;
						rules[pos]={field:&quot;checkdetailLineid&quot;,op:&quot;eq&quot;,data:-1};&#xD;
						pos++;					&#xD;
					}&#xD;
					var conditions = { groupOp:&quot;AND&quot;, rules:rules};	&#xD;
					if(pos==0){&#xD;
						conditions=null;&#xD;
					}&#xD;
					$view.custom_argue.refresh({filters:conditions});	&#xD;
					return;					&#xD;
				});&#xD;
		}&#xD;
		//情况三，类型选了&#xD;
		if($params.checkitem!=undefined&amp;&amp;$params.checktype!=undefined){&#xD;
			$http.post(&quot;ws/s_getIdByOperateid&quot;,{&quot;OperateLineid&quot;:$params.checktype})&#xD;
				.success(function(data){&#xD;
					if(data.result.length!=0){&#xD;
						rules[pos]={field:&quot;checkdetailLineid&quot;,op:&quot;in&quot;,data:data.result};&#xD;
						pos++;&#xD;
					}&#xD;
					else{&#xD;
						rules[pos]={field:&quot;checkdetailLineid&quot;,op:&quot;eq&quot;,data:-1};&#xD;
						pos++;					&#xD;
					}				&#xD;
					var conditions = { groupOp:&quot;AND&quot;, rules:rules};	&#xD;
					if(pos==0){&#xD;
						conditions=null;&#xD;
					}&#xD;
					$view.custom_argue.refresh({filters:conditions});	&#xD;
					return;					&#xD;
				});&#xD;
		}		&#xD;
	}&#xD;
	//分供方不为空的时候&#xD;
	else{&#xD;
		if($params.checkitem==undefined){&#xD;
			$http.post(&quot;ws/s_getByShipperId&quot;,{&quot;ShipperId&quot;:$model.entity.shipperId})&#xD;
				.success(function(data){&#xD;
					if(data.result.length!=0){&#xD;
						rules[pos]={field:&quot;checkdetailLineid&quot;,op:&quot;in&quot;,data:data.result};&#xD;
						pos++;&#xD;
					}&#xD;
					//搜不到的时候应该设置一个永远用不到的数字&#xD;
					else{&#xD;
						rules[pos]={field:&quot;checkdetailLineid&quot;,op:&quot;eq&quot;,data:-1};&#xD;
						pos++;					&#xD;
					}&#xD;
					var conditions = { groupOp:&quot;AND&quot;, rules:rules};	&#xD;
					if(pos==0){&#xD;
						conditions=null;&#xD;
					}&#xD;
					$view.custom_argue.refresh({filters:conditions});	&#xD;
					return;											&#xD;
				});&#xD;
		}&#xD;
		//情况二，项目选了，类型没选&#xD;
		if($params.checkitem!=undefined&amp;&amp;$params.checktype==undefined){&#xD;
			$http.post(&quot;ws/s_getIdByItemidWithShipperid&quot;,{&quot;ItemLineid&quot;:$params.checkitem,&quot;ShipperId&quot;:$model.entity.shipperId})&#xD;
				.success(function(data){&#xD;
					if(data.result.length!=0){&#xD;
						rules[pos]={field:&quot;checkdetailLineid&quot;,op:&quot;in&quot;,data:data.result};&#xD;
						pos++;&#xD;
					}&#xD;
					//搜不到的时候应该设置一个永远用不到的数字&#xD;
					else{&#xD;
						rules[pos]={field:&quot;checkdetailLineid&quot;,op:&quot;eq&quot;,data:-1};&#xD;
						pos++;					&#xD;
					}&#xD;
					var conditions = { groupOp:&quot;AND&quot;, rules:rules};	&#xD;
					if(pos==0){&#xD;
						conditions=null;&#xD;
					}&#xD;
					$view.custom_argue.refresh({filters:conditions});	&#xD;
					return;					&#xD;
				});&#xD;
		}&#xD;
		//情况三，类型选了&#xD;
		if($params.checkitem!=undefined&amp;&amp;$params.checktype!=undefined){&#xD;
			$http.post(&quot;ws/s_getIdByOperateidWithShipperid&quot;,{&quot;OperateLineid&quot;:$params.checktype,&quot;ShipperId&quot;:$model.entity.shipperId})&#xD;
				.success(function(data){&#xD;
					if(data.result.length!=0){&#xD;
						rules[pos]={field:&quot;checkdetailLineid&quot;,op:&quot;in&quot;,data:data.result};&#xD;
						pos++;&#xD;
					}&#xD;
					else{&#xD;
						rules[pos]={field:&quot;checkdetailLineid&quot;,op:&quot;eq&quot;,data:-1};&#xD;
						pos++;					&#xD;
					}				&#xD;
					var conditions = { groupOp:&quot;AND&quot;, rules:rules};	&#xD;
					if(pos==0){&#xD;
						conditions=null;&#xD;
					}&#xD;
					$view.custom_argue.refresh({filters:conditions});	&#xD;
					return;					&#xD;
				});&#xD;
		}			&#xD;
	}
},&#xD;
getArgueFile:function(){&#xD;
	var selArr=$view.custom_argue.getSelectDatas();&#xD;
	if(selArr.length==0){&#xD;
		Messenger.post({message:&quot;请勾选记录!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	&#xD;
		return;&#xD;
	}&#xD;
	if(selArr.length>1){&#xD;
		Messenger.post({message:&quot;请勾选单条记录!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	&#xD;
		return;&#xD;
	}&#xD;
	Modal.open(&quot;f/argueFileDownload&quot;,{&quot;info&quot;:selArr[0]},function($params){&#xD;
	});		&#xD;
},

