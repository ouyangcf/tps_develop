page_onload:function(){
/*	var conditions = { groupOp:&quot;AND&quot;, rules:[{field:&quot;argueStatus&quot;,op:&quot;ge&quot;,data:1}]};&#xD;
	$view.custom_argue.refresh({filters:conditions});*/	&#xD;
	for(var i=0;i&lt;$model.items.result.length;i++){&#xD;
		$view.checkitem.addOptions($model.items.result[i]);&#xD;
	}&#xD;
	$view.mainForm.clean();&#xD;
	var myDate=new Date();&#xD;
	myDate.setMonth(myDate.getMonth()-1);&#xD;
	$view.checkYM.clean();&#xD;
	$params.checkYM=undefined;&#xD;
	$params.checkYM=myDate.getFullYear()+&quot;-&quot;+(myDate.getMonth()+1);	&#xD;
	//&#xD;
	var sub0=$params.checkYM.substring(0,4);&#xD;
	var sub1=$params.checkYM.substring(5,$params.checkYM.length);&#xD;
	var myDate1=new Date();&#xD;
	var myDate2=new Date();&#xD;
	myDate1.setFullYear(sub0*1);&#xD;
	myDate1.setMonth(sub1*1-1);&#xD;
	myDate1.setDate(1);&#xD;
	myDate1.setHours(0,0,0);&#xD;
	myDate2.setFullYear(sub0*1);&#xD;
	myDate2.setMonth(sub1*1);&#xD;
	myDate2.setDate(1);&#xD;
	myDate2.setDate(myDate2.getDate()-1);&#xD;
	myDate2.setHours(23,59,59);&#xD;
	var rules=new Array();&#xD;
	var pos=0;	&#xD;
	rules[pos]={field:&quot;createTime&quot;,op:&quot;ge&quot;,data:functions.time2Str(myDate1)};//大于月初第一天以及小于月末最后一天&#xD;
	pos++;&#xD;
	rules[pos]={field:&quot;createTime&quot;,op:&quot;le&quot;,data:functions.time2Str(myDate2)};//大于月初第一天以及小于月末最后一天&#xD;
	pos++;	&#xD;
	var conditions = { groupOp:&quot;AND&quot;, rules:rules};	&#xD;
	if(pos==0){&#xD;
		conditions=null;&#xD;
	}&#xD;
	$view.custom_argue.refresh({filters:conditions});&#xD;
	//	&#xD;
	$view.div6.toggle();
},&#xD;
getiIndex:function(){&#xD;
	var ipos=0;&#xD;
	var l=$model.itemsid.result.length;&#xD;
	for(;ipos&lt;l;ipos++){&#xD;
		if($params.checkitem==$model.itemsid.result[ipos]){&#xD;
			break;&#xD;
		}	&#xD;
	}&#xD;
	return ipos;&#xD;
},&#xD;
time2Str:function(mytime){&#xD;
	Date.prototype.format =function(format)&#xD;
    {&#xD;
        var o = {&#xD;
        &quot;M+&quot; : this.getMonth()+1, //month&#xD;
&quot;d+&quot; : this.getDate(),    //day&#xD;
&quot;h+&quot; : this.getHours(),   //hour&#xD;
&quot;m+&quot; : this.getMinutes(), //minute&#xD;
&quot;s+&quot; : this.getSeconds(), //second&#xD;
&quot;q+&quot; : Math.floor((this.getMonth()+3)/3),  //quarter&#xD;
&quot;S&quot; : this.getMilliseconds() //millisecond&#xD;
        }&#xD;
        if(/(y+)/.test(format)) format=format.replace(RegExp.$1,&#xD;
        (this.getFullYear()+&quot;&quot;).substr(4- RegExp.$1.length));&#xD;
        for(var k in o)if(new RegExp(&quot;(&quot;+ k +&quot;)&quot;).test(format))&#xD;
        format = format.replace(RegExp.$1,&#xD;
        RegExp.$1.length==1? o[k] :&#xD;
        (&quot;00&quot;+ o[k]).substr((&quot;&quot;+ o[k]).length));&#xD;
        return format;&#xD;
    }&#xD;
	var mystrtime=mytime.format('yyyy-MM-dd hh:mm:ss');&#xD;
	return mystrtime;&#xD;
},

checkitem_change:function(){&#xD;
	if($params.checkitem==undefined){&#xD;
		$params.checktype=undefined;&#xD;
