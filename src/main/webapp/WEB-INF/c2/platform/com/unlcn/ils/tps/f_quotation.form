button_click:function($event){
	//计算单价报价
	var adata = $($view.ldetail.getDom()).jqGrid('getRowData');
	if ( adata.length == 0 ) 
	{
		Messenger.error('请输入报价信息');
		return false;
	}
	if($params.formula==undefined){
		Messenger.error('请输入计算公式');
		return false;		
	}
	if($params.formula.length==0){
		Messenger.error('请输入计算公式');
		return false;		
	}	
	for(var i=0; i&lt;adata.length; i++)  
	{  
		var data = adata[i];
		var selectid = data.id;
		var quotationPriceid = selectid + &quot;_quotationPrice&quot;;
		var overflowlvid=selectid + &quot;_qoverflow&quot;;
		var price = data.price;
		var kilometer = data.kilometer;
		var value = $params.formula;
		console.info('standardPrice='+data.price);
		//debugger;
		value = value.toLowerCase().replace(&quot;p&quot;,price);
		value = value.toLowerCase().replace(&quot;k&quot;,kilometer);
		var price = 0;
		try
		{
			var price = eval(value);
		}catch(e)
		{
			Messenger.error('错误的计算公式');			
			$($view.txtformula.getDom()).focus();
			break;
		}
		if ( price == null ) 
		{
			Messenger.error('错误的计算公式');			
			$($view.txtformula.getDom()).focus();
			break;
		}
		console.info('price='+price);
		
		var m_overflowlv=parseFloat(parseFloat(price-data.price)/parseFloat(data.price)).toFixed(3)*100;
		$(&quot;#&quot;+quotationPriceid).val(parseFloat(price).toFixed(3));
		var overflowid = selectid + &quot;_overflow&quot;;
		$(&quot;#&quot;+overflowid).text(m_overflowlv+&quot;%&quot;);
	}
},

button1_click:function($event){
	//计算总价
	//计算单价报价
	var adata = $($view.ldetail.getDom()).jqGrid('getRowData');
	if ( adata.length == 0 ) 
	{
		Messenger.error('请输入报价信息');
		return false;
	}
	var totalqty = 0;
	for(var i=0; i&lt;adata.length; i++)  
	{  //得到所有的数量
		var data = adata[i];
		var qty = data.qty;
		totalqty += parseFloat(qty);
	}
	var value = $params.qutotiontotal;
	if (angular.isUndefined(value))
	{
		Messenger.error('请输入正确的总价格');
		return false;
	}
	var oneprice = parseFloat(value)/parseFloat(totalqty);
	for(var i=0; i&lt;adata.length; i++)  
	{  
		var data = adata[i];
		var selectid = data.id;
		var qty = data.qty;
		var price = parseFloat(qty) * parseFloat(oneprice);
		
		var quotationPriceid = selectid + &quot;_quotationPrice&quot;;
		$(&quot;#&quot;+quotationPriceid).val(parseFloat(price).toFixed(3));
		
	}
},


