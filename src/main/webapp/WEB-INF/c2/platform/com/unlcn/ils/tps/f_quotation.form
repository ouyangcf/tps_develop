btnsave_click:function($event){
	//检查数据
	var shipperid  = $params.shipperid;
	if (angular.isUndefined(shipperid) || &quot;&quot;==shipperid)
	{
		Messenger.error('请选择的分供方');
		$($view.sltshipper.getDom()).focus();
