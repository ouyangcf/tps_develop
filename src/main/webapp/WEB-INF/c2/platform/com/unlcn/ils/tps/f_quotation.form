checkmaindata:function()
{
	$params.maindata = [];
	var adata = $($view.ldetail.getDom()).jqGrid('getRowData');
	if ( adata.length == 0 ) 
	{
		Messenger.error('请输入报价信息');
		return false;
	}
	for(var i=0; i&lt;adata.length; i++)  
	{  
		var data = adata[i];
		var selectid = data.id;
		var quotationPriceid = selectid + &quot;_quotationPrice&quot;;
		var quotationprice = $(&quot;#&quot;+quotationPriceid).val();
		var tr = $view.ldetail.getRowData(selectid);
  　		data.quotationPrice = quotationprice;
		if ( isNaN(parseFloat(data.quotationPrice)) || data.quotationPrice==&quot;&quot;)
		{
			Messenger.error('报价信息中，错误的报价单价');
			$(&quot;#&quot;+quotationPriceid).focus();
			return false;
		}
		$params.maindata[i] = data;
	}
	return true;
}
