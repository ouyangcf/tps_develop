//检查支付方式数据
checkpayment:function()
{	
	var adata = $($view.lpayment.getDom()).jqGrid('getRowData');
	if ( adata.length == 0 ) 
	{
		Messenger.error('请输入支付方式');
		return false;
	}
	var scale = 0;
	for(var i=0; i&lt;adata.length; i++)  
	{
		var data = adata[i];
		if ( isNaN(parseFloat(data.scale)))
		{
			Messenger.error('支付方式比例错误');
			return false;
		}	
		scale += parseFloat(data.scale);
	}
	
	//检查支付比例是否为100%
	if ( parseFloat(scale)!=100)
	{
		Messenger.error('所有支付方式的比例需要是100%');
		return false;
	}
	return true; 
},
