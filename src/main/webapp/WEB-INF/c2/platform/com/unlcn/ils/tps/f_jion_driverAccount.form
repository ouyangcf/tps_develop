  <form:datasources>
    <form:datasource xsi:type="form:Inputs"/>
  </form:datasources>
  <form:scripts>

button_click:function($event){
	if($params.accountType==null){
		Messenger.post({message:&quot;请选择账户类型！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	if($params.accounUnits==undefined||$params.accounUnits.length==0){
		Messenger.post({message:&quot;开票单位未填写！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	if($params.accounSubject==undefined||$params.accounSubject.length==0){
		Messenger.post({message:&quot;主体话未填写！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}	
	if($params.accountNumber==undefined||$params.accountNumber.length==0){
		Messenger.post({message:&quot;账户未填写！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}		
	Modal.instance.close($params);	
},

button1_click:function($event){
	Modal.instance.close();
},

page_onload:function(){
	$view.mainForm.clean();	
}</form:scripts>
