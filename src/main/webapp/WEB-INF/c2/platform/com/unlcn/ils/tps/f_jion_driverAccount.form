          <dync:attribute default="" group="高级自定义" id="style" name="自定义style" value="margin-top: 8px;color:red;">
            <dync:render colSpan="2" height="0" multi="false" name="text"/>
            <dync:children/>
          </dync:attribute>
          <dync:attribute default="" group="高级自定义" id="html" name="自定义HTML" value="div2_html">
            <dync:render colSpan="2" height="0" multi="false" name="html"/>
            <dync:children/>
