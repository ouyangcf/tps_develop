  <form:scripts>
openWindows:function(link){
	window.open(link);
},
page_onload:function(){
	var rules=new Array();
	var pos=0;	
	rules[pos]={field:&quot;flag&quot;,op:&quot;eq&quot;,data:$params.info.flag};//大于月初第一天以及小于月末最后一天
	pos++;
	rules[pos]={field:&quot;sourceId&quot;,op:&quot;eq&quot;,data:$params.info.id};//大于月初第一天以及小于月末最后一天
	pos++;	
	var conditions = { groupOp:&quot;AND&quot;, rules:rules};	
	if(pos==0){
		conditions=null;
	}
	$view.custom_attached.refresh({filters:conditions});	
},

button2_click:function($event){
	var selArr=$view.custom_attached.getSelectDatas();
	if(selArr.length==0){
		Messenger.post({message:&quot;请勾选记录!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	if(selArr.length>1){
		Messenger.post({message:&quot;请勾选单条记录!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	var str0=selArr[0].filePath;
	var sub0=str0.substring(0,17);
	var sub1=&quot;download/&quot;
	var sub2=str0.substring(17,str0.length);
	var newstr=location.href;
	var newpos=newstr.indexOf(&quot;#/f/&quot;); 
	var newstrs=newstr.substring(0,newpos);
	var wholeStr=newstrs
		+sub0+sub1+sub2;
	functions.openWindows(wholeStr);	
},

button3_click:function($event){
	var selArr=$view.custom_attached.getSelectDatas();
	if(selArr.length==0){
		Messenger.post({message:&quot;请勾选记录!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	var idList=new Array();
	for(var i=0;i&lt;selArr.length;i++){
		idList[i]=selArr[i].lineid;
	}
	Messenger.post({message:&quot;正在提交&quot;,type:&quot;info&quot;,hideAfter:2,showCloseButton:true});	
	$http.post(&quot;ws/s_delFile&quot;,{&quot;id&quot;:idList})	
		.success(function(data){
			Messenger.post({message:&quot;删除成功!&quot;,type:&quot;success&quot;,hideAfter:2,showCloseButton:true});	
			$view.custom_attached.refresh();	
		});
},

buttonClose_click:function($event){
	Modal.instance.dismiss();
}</form:scripts>
</form:Form>
