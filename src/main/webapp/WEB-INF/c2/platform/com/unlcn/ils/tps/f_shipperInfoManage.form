  <form:scripts>refresh:function(){
	//window.location.reload();
	$view.custom_shipperinfo.refresh();
},
page_onload:function(){
    $view.div1.toggle();
	var rules=new Array();
	var pos=0;	
	rules[pos]={field:&quot;auditFlag&quot;,op:&quot;in&quot;,data:[3,5,6]};
	pos++;	
	var conditions = { groupOp:&quot;AND&quot;, rules:rules};	
	if(pos==0){
		conditions=null;
	}
	$view.custom_shipperinfo.refresh({filters:conditions});	
	for(var i=0;i&lt;$model.provinces.result.length;i++){
		$view.provinceId.addOptions($model.provinces.result[i]);
	}	
},

custom_shipperinfo_beforeProcessing:function(data,status,xhr){
		for(var i=0;i&lt;data.contents.length;i++){
		if(data.contents[i].joinFlag==0){
			data.contents[i].joinFlag=&quot;个人&quot;;
		}
		else if(data.contents[i].joinFlag==1){
			data.contents[i].joinFlag=&quot;企业&quot;;
		}
		if(data.contents[i].auditflag==0){
			data.contents[i].auditflag=&quot;待认证&quot;;
		}
		else if(data.contents[i].auditflag==3){
			data.contents[i].auditflag=&quot;认证通过&quot;;
		}
		else if(data.contents[i].auditflag==4){
			data.contents[i].auditflag=&quot;认证未通过&quot;;
		}
		else if(data.contents[i].auditflag==5){
			data.contents[i].auditflag=&quot;认证过期&quot;;
		}	
		else if(data.contents[i].auditflag==6){
			data.contents[i].auditflag=&quot;禁用&quot;;
		}		
	}
	if(data.contents.length==0){
		Messenger.post({message:&quot;当前条件没有找到相应数据!&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});	
	}
	else{
		Messenger.post({message:&quot;当前条件共找到&quot;+data.contents.length+&quot;条相应数据!&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});	
	}
},

button64_click:function($event){
	functions.refresh();
},
getiIndex:function(){
	var ipos=0;
	var l=$model.provincesid.result.length;
	for(;ipos&lt;l;ipos++){
		if($model.e_joinDriver.provinceId==$model.provincesid.result[ipos]){
			break;
		}	
	}
	return ipos;
},
province_change:function(){
	if($params.province==undefined){
		$params.city=undefined;
		return;
	}
	var pos=functions.getiIndex();
	$view.city.setOptions({});
	for(var i=0;i&lt;$model.cities.result[pos].contents.length;i++){
		$view.city.addOptions($model.cities.result[pos].contents[i]);
	}
	$view.city.setDefaultValue();	
},

button_click:function($event){
	$view.mainForm.clean();
},

button1_click:function($event){
	var shipperName=null;
	var provinceid=null;
	var cityid=null;
	var rules=new Array();
	var pos=0;	
	var flag=false;
	if($params.shipperCode!=undefined&amp;&amp;$params.shipperCode.length!=0){
		
	}
	if($model.e_joinDriver.driverName!=undefined&amp;&amp;$model.e_joinDriver.driverName.length!=0){
		flag=true;
	}
	if($model.e_joinDriver.provinceId!=undefined){
		flag=true;
	}	
	if($model.e_joinDriver.cityId!=undefined){
		flag=true;
	}	
	if(flag==true){
		$http.post(&quot;ws/s_getAuditByCondition&quot;,{&quot;data&quot;:$model.e_joinDriver})
			.success(function(data){
				if(data.result.length!=0){
					rules[pos]={field:&quot;lineid&quot;,op:&quot;in&quot;,data:data.result};
					pos++;					
				}
				else{
					rules[pos]={field:&quot;lineid&quot;,op:&quot;in&quot;,data:['-1']};
					pos++;						
				}
				if($params.shipperType!=undefined){
					rules[pos]={field:&quot;joinFlag&quot;,op:&quot;eq&quot;,data:$params.shipperType};
					pos++;					
				}
				if($params.auditStatus!=undefined){
					rules[pos]={field:&quot;auditFlag&quot;,op:&quot;eq&quot;,data:$params.auditStatus};
					pos++;					
				}
				else{
					rules[pos]={field:&quot;auditFlag&quot;,op:&quot;in&quot;,data:[3,5,6]};
					pos++;				
				}
				var conditions = { groupOp:&quot;AND&quot;, rules:rules};	
				if(pos==0){
					conditions=null;
				}
				$view.custom_shipperinfo.refresh({filters:conditions});								
			});	
		return;
	}	
	else{
			if($params.shipperType!=undefined){
				rules[pos]={field:&quot;joinFlag&quot;,op:&quot;eq&quot;,data:$params.shipperType};
				pos++;					
			}
			if($params.auditStatus!=undefined){
				rules[pos]={field:&quot;auditFlag&quot;,op:&quot;eq&quot;,data:$params.auditStatus};
				pos++;					
			}
			else{
				rules[pos]={field:&quot;auditFlag&quot;,op:&quot;in&quot;,data:[3,5,6]};
				pos++;				
			}			
			var conditions = { groupOp:&quot;AND&quot;, rules:rules};	
			if(pos==0){
				conditions=null;
			}
			$view.custom_shipperinfo.refresh({filters:conditions});	
			return;	
	}
},

provinceId_change:function(){
	if($model.e_joinDriver.provinceId==undefined){
