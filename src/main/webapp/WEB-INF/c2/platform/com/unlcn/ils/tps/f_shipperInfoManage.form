	}
	var pos=functions.getiIndex();
	$view.cityId.setOptions({});
	for(var i=0;i&lt;$model.cities.result[pos].contents.length;i++){
		$view.cityId.addOptions($model.cities.result[pos].contents[i]);
	}
	$view.cityId.setDefaultValue();	
},

