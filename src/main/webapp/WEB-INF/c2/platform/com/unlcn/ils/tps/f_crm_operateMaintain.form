button_click:function($event){
	var rules=new Array();
	var pos=0;	
	if($params.relateFlag!=undefined){
		rules[pos]={field:&quot;realateFlag&quot;,op:&quot;eq&quot;,data:$params.relateFlag};
		pos++;
	}
	var conditions = { groupOp:&quot;AND&quot;, rules:rules};	
	if(pos==0){
		conditions=null;
	}
	$view.custom_crm_complaint_type.refresh({filters:conditions});	
	return;			
},

button37_click:function($event){
	var selArr=$view.custom_crm_complaint_type.getSelectDatas();
	if(selArr.length==0){
		Messenger.post({message:&quot;请勾选记录!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	if(selArr.length>1){
		Messenger.post({message:&quot;请勾选单条记录!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	if(selArr[0].Realate_Flag!=&quot;未关联&quot;){
		Messenger.post({message:&quot;所选对象已关联!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;	
	}
	Modal.open(&quot;f/complaintType2operate_dlg&quot;,{&quot;info&quot;:selArr[0]},function($params){
		$view.custom_crm_complaint_type.refresh();
	});		
},

