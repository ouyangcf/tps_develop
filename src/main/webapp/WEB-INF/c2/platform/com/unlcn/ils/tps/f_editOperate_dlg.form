  <form:scripts>

page_onload:function(){
	for(var i=0;i&lt;$model.items.result.length;i++){
		$view.itemLineid.addOptions($model.items.result[i]);
	}	
	$model.e_checkrule.lineid=$params.info.lineid;
	$model.e_checkrule.operateName=$params.info.operateName;
	$model.e_checkrule.subvalue=$params.info.subvalue;
	$model.e_checkrule.submoney=$params.info.submoney;
	$model.e_checkrule.itemLineid=$params.info.itemLineid;
	$model.e_checkrule.itemName=$params.info.itemName;	
	if($params.info.startDate!=undefined&amp;&amp;$params.info.startDate.length!=0){
		$model.e_checkrule.startDate=$params.info.startDate+&quot; 00:00:00&quot;;
	}
	if($params.info.endDate!=undefined&amp;&amp;$params.info.endDate.length!=0){
		$model.e_checkrule.endDate=$params.info.endDate+&quot; 00:00:00&quot;;
	}		
},

button_click:function($event){
	if($model.e_checkrule.itemLineid==undefined){
		Messenger.post({message:&quot;考核项目未选择&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});			
		return;		
	}
	if($model.e_checkrule.operateName==undefined||$model.e_checkrule.operateName.length==0){
		Messenger.post({message:&quot;考核类型名称&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});			
		return;
	}
	if($model.e_checkrule.subvalue==undefined){
		Messenger.post({message:&quot;扣分值未填写&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});			
		return;
	}
	if($model.e_checkrule.submoney==undefined){
		Messenger.post({message:&quot;考核金额未填写&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});			
		return;
	}
    Messenger.post({message:&quot;正在提交，请稍后&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});
    $http.post(&quot;ws/s_possessCheckrule&quot;,{&quot;data&quot;:$model.e_checkrule})
    	.success(function(data){
        Messenger.post({type:'success',message:'修改成功'});
        Modal.instance.close();		
    	});    	
},

button11_click:function($event){
	$view.mainForm.clean();
}</form:scripts>
