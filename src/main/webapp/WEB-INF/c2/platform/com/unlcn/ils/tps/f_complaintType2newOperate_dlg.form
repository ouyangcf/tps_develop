  <form:scripts>

page_onload:function(){
	for(var i=0;i&lt;$model.items.result.length;i++){
		$view.checkitem.addOptions($model.items.result[i]);
	}		
},

button_click:function($event){
	if($params.otherItem!=undefined&amp;&amp;$params.checkitem!=undefined){
		if($params.otherItem.length!=0){
		Messenger.post({message:&quot;考核项目只能填一个&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;		
		}
	}
	if(($params.otherItem==undefined||$params.otherItem.length==0)&amp;&amp;$params.checkitem==undefined){
		Messenger.post({message:&quot;请选择考核项目！考核项目和其它项目两者必有其一有数据&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}		
	if($model.e_checkrule.operateName==undefined||$model.e_checkrule.operateName.length==0){
		Messenger.post({message:&quot;考核类型名称未填写&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});			
		return;
	}
	if($model.e_checkrule.subvalue==undefined){
		Messenger.post({message:&quot;扣分值未填写&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});			
		return;
	}
	if($model.e_checkrule.submoney==undefined){
		Messenger.post({message:&quot;考核金额未填写&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});			
		return;
	}
	var status=-1;
	if($params.checkitem!=undefined){
		status=1;
		$model.e_crm_complaint_type.typeName=$params.checkitem;
	}
	if($params.otherItem!=undefined&amp;&amp;$params.otherItem.length!=0){
		status=0;
		$model.e_crm_complaint_type.typeName=$params.otherItem;
	}
	$model.e_crm_complaint_type.lineid=$params.info.Lineid;
	$model.e_crm_complaint_type.realateFlag=status;
	Messenger.post({message:&quot;正在提交&quot;,type:&quot;info&quot;,hideAfter:2,showCloseButton:true});	
	$http.post(&quot;ws/updateComplaintTypesWithNewoperate&quot;,{&quot;data&quot;:$model.e_crm_complaint_type,&quot;e_checkrule&quot;:$model.e_checkrule})
		.success(function(data){
			Messenger.post({message:&quot;提交成功&quot;,type:&quot;success&quot;,hideAfter:2,showCloseButton:true});	
			Modal.instance.close();
		});
},

buttonClose_click:function($event){
	$view.mainForm.clean();
},

subvalue_checkAfter:function(checkResult){
	var $subvalue=$($view.subvalue.getDom());
	var subvalue=$subvalue.val();
	var re=/^(0|[1-9]\d*)$/;
	var result=  re.test($model.e_checkrule.subvalue);
	if(subvalue.length==0)
	return;
	if($.trim(subvalue)==''){
	Messenger.post({message:&quot;扣分仅可填写整数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});		
	return;
	}
	if(!result){
		Messenger.post({message:&quot;扣分仅可填写整数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});			
	}	
},

submoney_checkAfter:function(checkResult){
	var $submoney=$($view.submoney.getDom());
	var submoney=$submoney.val();
	var re=/^[+-]?([0-9]*\.?[0-9]+|[0-9]+\.?[0-9]*)([eE][+-]?[0-9]+)?$/;
	var result=  re.test($model.e_checkrule.submoney);
	if(submoney.length==0)
	return;
	if($.trim(submoney)==''){
	Messenger.post({message:&quot;考核金额仅可填写整数或小数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});		
	return;
	}
	if(!result){
		Messenger.post({message:&quot;考核金额仅可填写整数或小数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});			
	}		
}</form:scripts>
