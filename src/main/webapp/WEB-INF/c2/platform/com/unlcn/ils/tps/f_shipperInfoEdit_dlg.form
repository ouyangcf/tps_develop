			$model.e_shipper.tel=data.result.tel;
			$model.e_shipper.hasPermit=data.result.hasPermit;
			$model.e_shipper.shortName=data.result.shortName;
			$model.e_shipper.taxNo=data.result.taxNo;
			$model.e_shipper.businessType=data.result.businessType;
			$model.e_shipper.carNumber=data.result.carNumber;
			$model.e_shipper.owner=data.result.owner;
			$model.e_shipper.certStatus=data.result.certStatus;
			$model.e_shipper.certValidTime=data.result.certValidTime;
			$model.e_shipper.hasInovice=data.result.hasInovice;
			$model.e_shipper.desc=data.result.desc;
			$model.e_shipper.roadLicence=data.result.roadLicence;
			$model.e_shipper.roadLicenceFilepath=data.result.roadLicenceFilepath;
			$model.e_shipper.dispatchUser=data.result.dispatchUser;
			$model.e_shipper.dispatchPhone=data.result.dispatchPhone;
			$model.e_shipper.gpsUser=data.result.gpsUser;
			$model.e_shipper.gpsPhone=data.result.gpsPhone;
			$model.e_shipper.receiptUser=data.result.receiptUser;
			$model.e_shipper.receiptPhone=data.result.receiptPhone;
			$model.e_shipper.accountUser=data.result.accountUser;
			$model.e_shipper.accountPhone=data.result.accountPhone;
			$model.e_shipper.createUser=data.result.createUser;
			$model.e_shipper.createTime=data.result.createTime;
			
			for(var i=0;i&lt;$model.provinces.result.length;i++){
				$view.province.addOptions($model.provinces.result[i]);
			}
			$model.e_shipper.province=data.result.province;
			if($model.e_shipper.province==undefined){
				$model.e_shipper.city=undefined;
				return;
			}
			var pos=functions.getiIndex();
			$view.city.setOptions({});
			for(var i=0;i&lt;$model.cities.result[pos].contents.length;i++){
				$view.city.addOptions($model.cities.result[pos].contents[i]);
			}		
			$model.e_shipper.city=data.result.city;		
			
			console.info('over');	
						
		});
		/*var conditions = { groupOp:&quot;AND&quot;, rules:[{field:&quot;joinId&quot;,op:&quot;eq&quot;,data:$params.info.joinId},{field:&quot;joinFlag&quot;,op:&quot;eq&quot;,data:$params.info.joinFlag}]};
		$view.list.refresh({filters:conditions});*/	
			
		var link_rules=new Array();
		link_rules[0]={field:&quot;lineid&quot;,op:&quot;eq&quot;,data:$params.info.shipperId};
		var filters={ groupOp:&quot;AND&quot;, rules:link_rules};
		$view.linkmanlist.refresh({filters:filters});
		
		var account_rules=new Array();
		 account_rules[0]={field:&quot;lineid&quot;,op:&quot;eq&quot;,data:$params.info.shipperId};
		var filters={ groupOp:&quot;AND&quot;, rules:account_rules};
		$view.accountlist.refresh({filters:filters});
},

button_click:function($event){
	var joinFlag=$params.info.joinFlag;
	var joinId=$params.info.joinId;
	Modal.open(&quot;f/interLineDlg&quot;,{},function($params){
				/*setTimeout(&quot;window.location.reload()&quot;,2000);*/
				var cityName=functions.getCityName($params.startcity);
				var provinceName=functions.getProvinceName($params.desprovince);
				var arr=$view.list.getDataIDs();
				var rowid=0;
				if(arr.length!=0)
				rowid=arr[arr.length-1]*1+1;
				//$view.list1.addRowData(rowid,{start_city:cityName,dest_province:provinceName,
				//price:$params.price,qty:$params.qty});
				//下面新增一条数据
				$model.join_interline.startCity=cityName;
				$model.join_interline.destProvince=provinceName;
				
				$model.join_interline.price=$params.price;
				$model.join_interline.qty=$params.qty;
				
				$model.join_interline.joinFlag=joinFlag;
				$model.join_interline.joinId=joinId;

				Messenger.post({type:'info',message:'正在提交'});
				$http.post(&quot;ws/s_possessInterLine&quot;,{&quot;data&quot;:$model.join_interline})
					.success(function(data){
					Messenger.post({type:'success',message:'新增成功'});
					var conditions = { groupOp:&quot;AND&quot;, rules:[{field:&quot;joinId&quot;,op:&quot;eq&quot;,data:joinId},{field:&quot;joinFlag&quot;,op:&quot;eq&quot;,data:joinFlag}]};
					$view.list.refresh({filters:conditions});
					});
			});		
},

button1_click:function($event){
	var selArr=$view.list.getSelectDatas();
	var idList=new Array();
	if(selArr.length==0){
		Messenger.post({message:&quot;请选择要删除行!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	for(var i=0;i&lt;selArr.length;i++){
	idList[i]=selArr[i].lineid;
	}
	Messenger.post({message:&quot;正在删除!&quot;,type:&quot;info&quot;,hideAfter:2,showCloseButton:true});
		$http.post(&quot;ws/s_delInterLine&quot;,{&quot;idList&quot;:idList})
			.success(function(data){
				Messenger.post({type:'success',message:'删除成功'});
				var conditions = { groupOp:&quot;AND&quot;, rules:[{field:&quot;joinId&quot;,op:&quot;eq&quot;,data:$params.info.joinId},{field:&quot;joinFlag&quot;,op:&quot;eq&quot;,data:$params.info.joinFlag}]};
				$view.list.refresh({filters:conditions});			
			});	
},
getCityName:function(id){
	var i=0;
	for(;i&lt;$model.cityIds.result.length;i++){
		if($model.cityIds.result[i]==id)
			break;
	}
	var cityName=$model.cityNames.result[i];
	return cityName;
},
getProvinceName:function(id){
	var i=0;
	for(;i&lt;$model.provinceIds.result.length;i++){
		if($model.provinceIds.result[i]==id)
			break;
	}
	var provinceName=$model.provinceNames.result[i];
	return provinceName;
},

