  <form:scripts>
getiIndex:function(){
	var ipos=0;
	var l=$model.provincesid.result.length;
	for(;ipos&lt;l;ipos++){
		if($model.e_shipper.province==$model.provincesid.result[ipos]){
			break;
		}	
	}
	return ipos;
},
