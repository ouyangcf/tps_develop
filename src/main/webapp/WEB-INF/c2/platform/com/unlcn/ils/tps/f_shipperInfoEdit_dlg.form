	/*var shippperid=window.parent.angular.element(&quot;#indexConScope&quot;).scope().shippperid;
	alert(shippperid.shipperId);
	$params.info.shipperId=shippperid.shipperId;*/
	alert($params.info.shipperId);
	$http.post(&quot;ws/s_getShipperFromCrmById&quot;,{&quot;shipperId&quot;:$params.info.shipperId})
		.success(function(data){
			console.info(data.result);
			$model.e_shipper.shipperprop=data.result.shipperprop;	
			$model.e_shipper.id=data.result.custominfoId;
			$model.e_shipper.code=data.result.code;
			$model.e_shipper.regAddress=data.result.regAddress;
			$model.e_shipper.name=data.result.name;
			$model.e_shipper.capital=data.result.capital;
			$model.e_shipper.linkman=data.result.linkman;
