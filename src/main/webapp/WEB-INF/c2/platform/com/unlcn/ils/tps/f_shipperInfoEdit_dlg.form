capital_checkAfter:function(checkResult){
	$params.perCheckMoney=undefined;	
	var $nums=$($view.capital.getDom());
	var num_val=$nums.val();
	var re=/^(0|[1-9]\d*)$/;
	var result=  re.test($model.e_shipper.capital);
	if(num_val.length==0)
	return;
	if($.trim(num_val)==''){
	Messenger.post({message:&quot;注册资金仅可填写整数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});		
	return;
	}
	if(!result){
		Messenger.post({message:&quot;注册资金仅可填写整数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});			
	}
	if($model.e_shipper.capital!=undefined){
		$params.perCheckMoney=$model.e_shipper.capital;
	}		
},

