tel_checkAfter:function(checkResult){
	var $nums=$($view.tel.getDom());
	var num_val=$nums.val();
	var re=/(\(\d{3,4}\)|\d{3,4}-|\s)?\d{7,14}/;
	var result=  re.test($model.e_shipper.tel);
	if(num_val.length==0)
	return;
	if($.trim(num_val)==''){
	Messenger.post({message:&quot;请填写电话&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});		
	return;
	}
	if(!result){
		Messenger.post({message:&quot;请填写电话&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});			
	}
},

