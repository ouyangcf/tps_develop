          <form:control type="widget/col_jqgrid">
            <form:attributes source="widget/col_jqgrid"/>
          </form:control>
          <form:validation/>
        </form:child>
        <form:child xsi:type="form:Field" binding="lineid" id="lineid" name="lineid" customize="true">
