  <form:scripts>
button_click:function($event){
	if($model.entity.shipperLineid==null||$model.entity.shipperLineid==undefined)
	{
		Messenger.post({type:'error',message:'请选择分供方'});
		return false;
	}
	if($model.entity.startCityid==null||$model.entity.startCityid==undefined)
	{
		Messenger.post({type:'error',message:'请选择线路序号'});
		return false;
	}
	if($model.entity.levelLineid==null||$model.entity.levelLineid==undefined)
	{
		Messenger.post({type:'error',message:'请选择当前级别'});
		return false;
	}
/*	if($model.entity.active==null||$model.entity.active==undefined)
	{
		Messenger.post({type:'error',message:'请选择禁用状态'});
		return false;
	}
	if($model.entity.activeMemo==null||$model.entity.activeMemo==undefined||$model.entity.activeMemo.length==0)
	{if($model.entity.active==0)
		{
			Messenger.post({type:'error',message:'请填写禁用说明'});
		return false;
		}
	}*/
	Messenger.post({type:'info',message:'正在提交'});
	$http.post(&quot;ws/s_setCurrentLevel&quot;,{&quot;data&quot;:$model.entity})
	.success(function(data){
		if(data.result==false){
			Messenger.post({type:'error',message:'该分供方和线路已存在'});
			return;
		}
		else{
			Messenger.post({type:'success',message:'提交成功'});
			$view.mainForm.clean();
		}
		
	})
	.error(function(data){
		Messenger.post({type:'error',message:'提交失败'});
	})
	
	
},

button1_click:function($event){
	$view.mainForm.clean();
}</form:scripts>
