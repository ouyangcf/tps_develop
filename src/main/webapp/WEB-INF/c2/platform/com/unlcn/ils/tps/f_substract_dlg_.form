  <form:scripts>

page_onload:function(){
	var conditions = { groupOp:&quot;AND&quot;, rules:[{field:&quot;lineid&quot;,op:&quot;eq&quot;,data:$params.lineid}]};
	$view.custom_revise.refresh({filters:conditions});		
},

subMoney_checkAfter:function(checkResult){
	var $money=$($view.subMoney.getDom());
	var money_val=$money.val();
	var re=/^[+-]?([0-9]*\.?[0-9]+|[0-9]+\.?[0-9]*)([eE][+-]?[0-9]+)?$/;
	var result=  re.test($model.e_substract.subMoney);
	if(money_val.length==0)
	return;
	if($.trim(money_val)==''){
	Messenger.post({message:&quot;考核金额仅可填写整数或小数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});		
	return;
	}
	if(!result){
		Messenger.post({message:&quot;考核金额仅可填写整数或小数!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});			
	}	
},

button4_click:function($event){
	if($model.e_substract.subMoney==undefined){
		Messenger.post({message:&quot;减免单减免金额未填写！&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}
	if($model.e_substract.subReson==undefined||$model.e_substract.subReson.length==0){
		Messenger.post({message:&quot;减免单减免原因未填写!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
	}	
	var checkMoney=$params.checkMoney*1;
	if($model.e_substract.subMoney>checkMoney){
		Messenger.post({message:&quot;减免金额大于预定考核金额,请重新选择!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;			
	}	
	Messenger.post({message:&quot;正在生成，请稍后&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});
	$http.post(&quot;ws/s_addSubstract1&quot;,{&quot;lineid&quot;:$params.lineid,&quot;e_substract&quot;:$model.e_substract})
		.success(function(data){
			if(data.result==true){
				Messenger.post({message:&quot;生成成功&quot;,type:&quot;success&quot;,hideAfter:1,showCloseButton:true});	
				Modal.instance.close();
			}
			else{
				Messenger.post({message:&quot;生成失败&quot;,type:&quot;error&quot;,hideAfter:1,showCloseButton:true});	
				Modal.instance.close();
			}
		})
		.error(function(data){
			Messenger.post({message:&quot;post异常&quot;,type:&quot;error&quot;,hideAfter:1,showCloseButton:true});		
			Modal.instance.close();	
		});			
},

custom_revise_beforeProcessing:function(data,status,xhr){
	for(var i=0;i&lt;data.contents.length;i++){
		if(data.contents[i].argueStatus==0){
			data.contents[i].argueStatus=&quot;未申诉&quot;;
		}
		else if(data.contents[i].argueStatus==1){
			data.contents[i].argueStatus=&quot;申诉未处理&quot;;
		}
		else if(data.contents[i].argueStatus==2){
			data.contents[i].argueStatus=&quot;申诉成功&quot;;
		}
		else if(data.contents[i].argueStatus==3){
			data.contents[i].argueStatus=&quot;申诉不成功&quot;;
		}
		else if(data.contents[i].argueStatus==4){
			data.contents[i].argueStatus=&quot;过期未申诉&quot;;
		}
		if(data.contents[i].reviseStatus==0){
			data.contents[i].reviseStatus=&quot;未整改&quot;;
		}
		else if(data.contents[i].reviseStatus==1){
			data.contents[i].reviseStatus=&quot;整改中&quot;;
		}
		else if(data.contents[i].reviseStatus==2){
			data.contents[i].reviseStatus=&quot;已整改&quot;;
		}
		else if(data.contents[i].reviseStatus==3){
			data.contents[i].reviseStatus=&quot;整改成功&quot;;
		}
		else if(data.contents[i].reviseStatus==4){
			data.contents[i].reviseStatus=&quot;整改不成功&quot;;
		}	
		else{
			data.contents[i].reviseStatus=&quot;过期未整改&quot;;
		}	
	}
},

button_click:function($event){
	$view.mainForm.clean();	
}</form:scripts>
