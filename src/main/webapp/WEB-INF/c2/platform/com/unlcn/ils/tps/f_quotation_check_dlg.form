  <form:scripts>btn_add_click:function($event){
	
	var checkstatus  = $params.checkstatus;
	if (angular.isUndefined(sltstatus) || &quot;&quot;==checkstatus)
	{
		Messenger.error('请选择审核状态');
		$($view.sltstatus.getDom()).focus();
		return false;
	}
	if ( checkstatus == &quot;1&quot;)
	{
		
	}
	var message={};	
	message.checkmemo = $params.checkmemo;
	message.checkstatus = checkstatus;
	Modal.instance.close(message);
},

btn_edit_click:function($event){
	Modal.instance.dismiss();
}</form:scripts>
