page_onload:function(){&#xD;
	debugger;&#xD;
	var shipperId=window.parent.angular.element(&quot;#indexConScope&quot;).scope().shipperid;&#xD;
	var strshipper=shipperId+&quot;&quot;;&#xD;
	$params.shipperid=strshipper;&#xD;
	$http.post(&quot;ws/s_getShipperById&quot;,{&quot;shippperid&quot;:strshipper})&#xD;
	.success(function(data){&#xD;
		$params.shipper1=data.result;&#xD;
		if($params.shipper1.certStatus==&quot;0&quot;)&#xD;
			$params.shipper1.certStatus=&quot;认证未通过&quot;;&#xD;
		if($params.shipper1.certStatus==&quot;1&quot;)&#xD;
			$params.shipper1.certStatus=&quot;认证通过&quot;;&#xD;
		if($params.shipper1.certStatus==&quot;2&quot;){&#xD;
			var endDate=$params.shipper1.certValidTime;&#xD;
			var cur=new Date();&#xD;
			var end_arr=endDate.split(&quot;-&quot;);&#xD;
			var end_arr1=end_arr[2].split(&quot; &quot;);&#xD;
			var starttime = new Date(end_arr[0], end_arr[1]-1, end_arr1[0]);&#xD;
			var str_cur=functions.time2Str(cur);&#xD;
			var str_end=functions.time2Str(starttime);&#xD;
			console.info('当前'+str_cur);&#xD;
			console.info('认证有效戒指'+str_end);&#xD;
			var compare_reusult=functions.duibi(str_cur,str_end);&#xD;
			if(compare_reusult==false){&#xD;
				$params.shipper1.certStatus=&quot;过期未认证&quot;;&#xD;
			}&#xD;
			else{&#xD;
				$params.shipper1.certStatus=&quot;未认证&quot;;&#xD;
			}	&#xD;
			console.info(&quot;已执行&quot;);					&#xD;
		}&#xD;
&#xD;
		if($params.shipper1.hasInovice==&quot;1&quot;){&#xD;
			$params.shipper1.hasInovice=&quot;能&quot;;&#xD;
		}&#xD;
		else{&#xD;
			$params.shipper1.hasInovice=&quot;不能&quot;;&#xD;
		}&#xD;
		if($params.shipper1.businessType==&quot;0&quot;){&#xD;
			$params.shipper1.businessType=&quot;固有&quot;;&#xD;
		}&#xD;
		if($params.shipper1.businessType==&quot;1&quot;){&#xD;
			$params.shipper1.businessType=&quot;补充&quot;;&#xD;
		}		&#xD;
		else{&#xD;
			$params.shipper1.businessType=&quot;临时&quot;;&#xD;
		}		&#xD;
		if($params.shipper1.hasPermit==&quot;1&quot;){&#xD;
			$params.shipper1.hasPermit=&quot;有&quot;;&#xD;
		}		&#xD;
		else{&#xD;
			$params.shipper1.hasPermit=&quot;无&quot;;&#xD;
		}			&#xD;
		functions.checkradio(&quot;yes&quot;);&#xD;
		$http.post(&quot;ws/s_getMapByCity&quot;,{&quot;id&quot;:$params.shipper1.city})&#xD;
			.success(function(data){&#xD;
				$params.shipper1.province=data.result.province;&#xD;
				$params.shipper1.city=data.result.city;&#xD;
			});&#xD;
	});
},

