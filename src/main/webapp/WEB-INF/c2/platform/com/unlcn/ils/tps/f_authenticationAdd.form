  <form:scripts>duibi:function(a,b){&#xD;
    var arr = a.split(&quot;-&quot;);&#xD;
    var starttime = new Date(arr[0], arr[1], arr[2]);&#xD;
    var starttimes = starttime.getTime();&#xD;
    var arrs = b.split(&quot;-&quot;);&#xD;
    var lktime = new Date(arrs[0], arrs[1], arrs[2]);&#xD;
    var lktimes = lktime.getTime();&#xD;
    if (starttimes >= lktimes) {&#xD;
        return false;&#xD;
    }&#xD;
    else&#xD;
        return true;&#xD;
&#xD;
},&#xD;
time2Str:function(mytime){&#xD;
	Date.prototype.format =function(format)&#xD;
    {&#xD;
        var o = {&#xD;
        &quot;M+&quot; : this.getMonth()+1, //month&#xD;
&quot;d+&quot; : this.getDate(),    //day&#xD;
&quot;q+&quot; : Math.floor((this.getMonth()+3)/3),  //quarter&#xD;
&quot;S&quot; : this.getMilliseconds() //millisecond&#xD;
        }&#xD;
        if(/(y+)/.test(format)) format=format.replace(RegExp.$1,&#xD;
        (this.getFullYear()+&quot;&quot;).substr(4- RegExp.$1.length));&#xD;
        for(var k in o)if(new RegExp(&quot;(&quot;+ k +&quot;)&quot;).test(format))&#xD;
        format = format.replace(RegExp.$1,&#xD;
        RegExp.$1.length==1? o[k] :&#xD;
        (&quot;00&quot;+ o[k]).substr((&quot;&quot;+ o[k]).length));&#xD;
        return format;&#xD;
    }&#xD;
	var mystrtime=mytime.format('yyyy-MM-dd');&#xD;
	return mystrtime;&#xD;
},&#xD;
btnsave_click:function($event){&#xD;
	//保存&#xD;
	&#xD;
	var fNum=$(document).find(&quot;.file-input&quot;).length;&#xD;
	if (fNum>1)//有上传,先上传，上传成功后再保存&#xD;
		$view.mulifile.submit();&#xD;
	else//没有附件，直接保存&#xD;
		functions.savedata(null);&#xD;
	&#xD;
}&#xD;
,&#xD;
//保存数据&#xD;
savedata:function(filedata)&#xD;
{	Messenger.post({message:&quot;正在提交&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});&#xD;
	var fNum=$(document).find(&quot;.file-input&quot;).length;&#xD;
	var file = null;&#xD;
	if ( filedata != null )&#xD;
		file = functions.getfile(filedata);&#xD;
	var shipperid = $params.shipperid+&quot;&quot;;&#xD;
	var authresult = $params.result;&#xD;
	var begindate = $view.begindate.getDom();&#xD;
	var dbegindate = $(begindate).val();&#xD;
	&#xD;
	var enddate = $view.enddate.getDom();&#xD;
	var denddate = $(enddate).val();&#xD;
	var filecount = parseInt(fNum)-1;&#xD;
	var postdata = null;&#xD;
	if ( authresult==&quot;yes&quot;)//认证通过&#xD;
		postdata={'begindate':dbegindate,'enddate':denddate,'okmemo':$params.okmemo};&#xD;
	else //认证不通过&#xD;
		postdata={'reason':$params.reason,'cancelmemo':$params.cancelmemo};&#xD;
	$http({&#xD;
        method: 'post',&#xD;
        url: 'ws/s_authenticationadd',&#xD;
        headers: {'Content-Type': 'application/json'},&#xD;
        data:{'postdata':postdata,'filedata':file,'shipper_id':shipperid,'result':authresult,'filecount':filecount}&#xD;
    }).success(function(data,status,headers,config) {    &#xD;
    	Messenger.success('保存成功');&#xD;
    	&#xD;
    	var ravalue = $view.raresult.getDefaultValue(&quot;yes&quot;);&#xD;
    	$view.mainForm.clean();&#xD;
    	$view.raresult.setDefaultValue(ravalue);//默认选择&#xD;
    	$model.shipper.$load();//加载分供方&#xD;
    	$view.list.refresh();//刷新认证记录&#xD;
    	$view.mulifile.reset();//清空上传附件&#xD;
    	&#xD;
    	&#xD;
    }).error(function(data,status,headers,config) {&#xD;
    	//alert(data)&#xD;
       // 当响应以错误状态返回时调用 &#xD;
       //Messenger.error('保存失败');&#xD;
     });&#xD;
     &#xD;
}&#xD;
,&#xD;
//分解文件上传返回结果&#xD;
getfile:function(data){&#xD;
	&#xD;
	if (data.fileList==null)&#xD;
	{&#xD;
		return data;&#xD;
	}&#xD;
	else&#xD;
	{&#xD;
		return data.fileList;&#xD;
	}&#xD;
}&#xD;
