custom_audit_beforeProcessing:function(data,status,xhr){
	for(var i=0;i&lt;data.contents.length;i++){
		if(data.contents[i].checkflg==0){
			data.contents[i].checkflg=&quot;待认证&quot;;
		}
		else if(data.contents[i].checkflg==3){
			data.contents[i].checkflg=&quot;认证通过&quot;;
		}
		else{
			data.contents[i].checkflg=&quot;认证未通过&quot;;
		}
	}
	if(data.contents.length==0){
		Messenger.post({message:&quot;当前条件没有找到相应数据!&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});	
	}
	else{
		Messenger.post({message:&quot;当前条件共找到&quot;+data.contents.length+&quot;条相应数据!&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});	
	};
	
	//下面进行人工排序,按照申请时间倒叙
	var l=data.contents.length;
	for(var i=0;i&lt;l-1;i++)      
	{
	   for(var j=0;j&lt;l-1-i;j++) {
			var da1=data.contents[j].createtime;
			var da2=data.contents[j+1].createtime;
			/*var year1=date1.substring(0,4);
			var month1=date1.substring(5,7);
			var day1=date1.substring(8,10);
			var da1=new Date(year1,month1-1,day1);
			
			var year2=date2.substring(0,4);
			var month2=date2.substring(5,7);
			var day2=date2.substring(8,10);
			var da2=new Date(year2,month2-1,day2);	*/
		   if(da1&lt;da2)
		   {
		      var temp=data.contents[j];
		      data.contents[j]=data.contents[j+1]; 
		      data.contents[j+1]=temp;
		   }
	   }
	}
	
},


