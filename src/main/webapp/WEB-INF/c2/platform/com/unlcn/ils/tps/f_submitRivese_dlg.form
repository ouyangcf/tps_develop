  <form:scripts>

button_click:function($event){
		if($model.e_revise.reson==undefined||$model.e_revise.reson.length==0){
		Messenger.post({message:&quot;根本原因未填写!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
		}
		if($model.e_revise.tempcontent==undefined||$model.e_revise.tempcontent.length==0){
		Messenger.post({message:&quot;临时整改措施未填写!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
		}	
		if($model.e_revise.longcontent==undefined||$model.e_revise.longcontent.length==0){
		Messenger.post({message:&quot;长期整改措施未填写!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
		}	
		if($model.e_revise.precontent==undefined||$model.e_revise.precontent.length==0){
		Messenger.post({message:&quot;预防措施未填写!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
		}	
		if($model.e_revise.verifycontent==undefined||$model.e_revise.verifycontent.length==0){
		Messenger.post({message:&quot;验证措施未填写!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
		}	
		if($model.e_revise.executeUsername==undefined||$model.e_revise.executeUsername.length==0){
		Messenger.post({message:&quot;整改人未填写!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
		}
		if($model.e_revise.contact==undefined){
		Messenger.post({message:&quot;联系方式未填写!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});	
		return;
		}	
		$model.e_revise.lineid=$params.info.lineid;
		$model.e_revise.reviseStatus=1;
		Messenger.post({message:&quot;正在提交，请稍后&quot;,type:&quot;info&quot;,hideAfter:1,showCloseButton:true});	
		$model.e_revise.$save(function(){
        Messenger.post({type:'success',message:'提交成功'});
        $http.post(&quot;ws/s_getMaxReviseId&quot;,{})
        	.success(function(data){
        		Modal.openConfirm({message:&quot;添加附件?&quot;},function($params){
        			Modal.open(&quot;f/revise_attached_dlg&quot;,{&quot;info&quot;:data.result},function($params){
        				Modal.instance.close();
        			});
        		},function($params){
        			Modal.instance.close();		        	
        		});        		
        	});	  
    });				
},

contact_checkAfter:function(checkResult){
	var $nums=$($view.contact.getDom());
	var num_val=$nums.val();
	var re=/(\(\d{3,4}\)|\d{3,4}-|\s)?\d{7,14}/;
	var result=  re.test($model.e_revise.contact);
	if(num_val.length==0)
	return;
	if($.trim(num_val)==''){
	Messenger.post({message:&quot;联系方式请填写移动或者固定电话!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});		
	return;
	}
	if(!result){
		Messenger.post({message:&quot;联系方式请填写移动或者固定电话!&quot;,type:&quot;error&quot;,hideAfter:2,showCloseButton:true});			
	}
},

button11_click:function($event){
	$view.mainForm.clean();
}</form:scripts>
