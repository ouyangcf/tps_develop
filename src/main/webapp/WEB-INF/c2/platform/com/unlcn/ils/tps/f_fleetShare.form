btnedit_click:function($event){
	
	var selectData = $view.listdata.getSelectDatas();
	if(selectData.length>1)
	{
		Messenger.post({type:'error',message:'修改时，不能选中多行'});
		return false;
	}
	var selectData = [];
	selectData = $view.listdata.getSelectRowIds();
	if(selectData.length==0||selectData[0]==null)
	{
		Messenger.post({type:'error',message:'没有选中行'});
		return false;
	}
	var param = {&quot;lineid&quot;:selectData};
    Modal.open(&quot;f/f_fleetshareedit&quot;,param,function(){
    	$view.listdata.refresh();
    });    
},

btndelete_click:function($event){
	var selectData = [];
	selectData = $view.listdata.getSelectRowIds();
	if(selectData.length==0||selectData[0]==null)
	{
		Messenger.post({type:'error',message:'没有选中行'});
		return false;
	}
	Modal.openDelConfirm({message:&quot;确定删除?&quot;},function()
	{
		var postdata = selectData.join(&quot;,&quot;);
		
		$http({
        method: 'post',
        url: 'ws/deletefleetshare',
        headers: {'Content-Type': 'application/json'},
        data:{'postdata':postdata}
		}).success(function(data,status,headers,config) 
		{    
	    	Messenger.error('删除成功');
	    	$view.listdata.refresh();
		}).error(function(data,status,headers,config) 
		{
			//alert(data)
			// 当响应以错误状态返回时调用 
			//Messenger.error('保存失败');
		});
	});
},

